{
  if (_onError != null) {
    Throwable th=_onError;
    _onError=null;
    _writeListener.onError(new IOException(th));
    close();
  }
switch (_state.get()) {
case READY:
    try {
      _writeListener.onWritePossible();
    }
 catch (    Throwable e) {
      _writeListener.onError(e);
      close();
    }
  break;
case CLOSED:
try {
  new Throwable().printStackTrace();
  _writeListener.onWritePossible();
}
 catch (Throwable e) {
  _writeListener.onError(e);
}
break;
default :
}
}

{
  State state=_state.get();
  while (state != State.CLOSED) {
    if (_state.compareAndSet(state,State.CLOSED)) {
      try {
        _channel.getResponse().closeOutput();
      }
 catch (      IOException e) {
        _channel.failed();
        LOG.ignore(e);
      }
      releaseBuffer();
      return;
    }
    state=_state.get();
  }
}

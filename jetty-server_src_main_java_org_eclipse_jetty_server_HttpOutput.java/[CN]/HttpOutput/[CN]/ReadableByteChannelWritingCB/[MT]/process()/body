{
  _buffer.clear();
  boolean eof=false;
  int len=_in.read(_buffer);
  if (len < 0) {
    eof=true;
    len=0;
    _in.close();
  }
 else   if (len < _buffer.capacity()) {
    int len2=_in.read(_buffer);
    if (len2 < 0)     eof=true;
 else     len+=len2;
  }
  _buffer.flip();
  _channel.write(_buffer,eof,this);
  if (eof) {
    closed();
    _channel.getByteBufferPool().release(_buffer);
    return true;
  }
  return false;
}

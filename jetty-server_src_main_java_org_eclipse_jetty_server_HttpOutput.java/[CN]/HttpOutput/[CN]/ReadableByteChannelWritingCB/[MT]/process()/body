{
  _buffer.clear();
  int len=_in.read(_buffer);
  if (len == -1) {
    closed();
    _channel.getByteBufferPool().release(_buffer);
    return true;
  }
  boolean eof=false;
  if (len < _buffer.capacity()) {
    int len2=_in.read(_buffer);
    if (len2 < 0)     eof=true;
 else     len+=len2;
  }
  _buffer.flip();
  _channel.write(_buffer,eof,this);
  return false;
}

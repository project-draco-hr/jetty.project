{
  this.client=client;
  this.conversation=conversation;
  scheme=uri.getScheme();
  host=client.normalizeHost(uri.getHost());
  port=client.normalizePort(scheme,uri.getPort());
  path=uri.getRawPath();
  query=uri.getRawQuery();
  extractParams(query);
  this.uri=buildURI();
  followRedirects(client.isFollowRedirects());
}

{
  try {
    if (listener != null) {
      logger.debug("Invoking callback with {} on listener {}",frame,listener);
      PingInfo pingInfo=new PingInfo(frame.getPingId());
      listener.onPing(this,pingInfo);
    }
  }
 catch (  Exception x) {
    logger.info("Exception while notifying listener " + listener,x);
  }
}

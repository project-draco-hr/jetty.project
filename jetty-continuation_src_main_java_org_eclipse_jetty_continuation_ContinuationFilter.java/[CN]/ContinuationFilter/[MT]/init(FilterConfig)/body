{
  boolean jetty_7_or_greater="org.eclipse.jetty.servlet".equals(filterConfig.getClass().getPackage().getName());
  _context=filterConfig.getServletContext();
  String param=filterConfig.getInitParameter("debug");
  _debug=param != null && Boolean.parseBoolean(param);
  if (_debug)   __debug=true;
  param=filterConfig.getInitParameter("partial");
  if (param != null)   _partial=Boolean.parseBoolean(param);
 else   _partial=ContinuationSupport.__jetty6 && !jetty_7_or_greater;
  param=filterConfig.getInitParameter("faux");
  if (param != null)   _faux=Boolean.parseBoolean(param);
 else   _faux=!(jetty_7_or_greater || _partial || _context.getMajorVersion() >= 3);
  if (_debug)   _context.log("ContinuationFilter " + " jetty=" + jetty_7_or_greater + " partial="+ _partial+ " jetty6="+ ContinuationSupport.__jetty6+ " faux="+ _faux+ " servlet3="+ ContinuationSupport.__servlet3);
}

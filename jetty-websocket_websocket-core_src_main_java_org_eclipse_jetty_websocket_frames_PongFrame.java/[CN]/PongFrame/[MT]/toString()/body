{
  StringBuilder b=new StringBuilder();
  b.append("PongFrame[");
  b.append("len=").append(getPayloadLength());
  if (hasPayload()) {
    b.append(",payload=");
    b.append(BufferUtil.toSummaryString(getPayload()));
  }
 else {
    b.append(",no-payload");
  }
  b.append("]");
  return b.toString();
}

{
  dataInfos.offer(dataInfo);
  complete=endRequest;
  post(new Runnable(){
    @Override public void run(){
      logger.debug("HTTP > {} bytes of content",dataInfo.length());
      if (state == State.HEADERS) {
        state=State.HEADERS_COMPLETE;
        handle();
      }
      state=State.CONTENT;
      handle();
    }
  }
);
}

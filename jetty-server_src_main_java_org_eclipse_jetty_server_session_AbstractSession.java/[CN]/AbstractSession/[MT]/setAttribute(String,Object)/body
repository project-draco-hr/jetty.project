{
  Object old=null;
synchronized (this) {
    checkValid();
    old=doPutOrRemove(name,value);
  }
  if (value == null || !value.equals(old)) {
    if (old != null)     unbindValue(name,old);
    if (value != null)     bindValue(name,value);
    _manager.doSessionAttributeListeners(this,name,old,value);
  }
}

{
  if (th == null)   _buffer.append("null");
 else {
    _buffer.append('\n');
    format(th.toString());
    StackTraceElement[] elements=th.getStackTrace();
    for (int i=0; elements != null && i < elements.length; i++) {
      _buffer.append("\n\tat ");
      format(elements[i].toString());
    }
  }
}

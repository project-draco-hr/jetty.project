{
  if (enabled) {
synchronized (__loggers) {
      this._level=LEVEL_DEBUG;
      for (      StdErrLog log : __loggers.values()) {
        log._level=LEVEL_DEBUG;
      }
    }
  }
 else {
synchronized (__loggers) {
      this._level=this._configuredLevel;
      for (      StdErrLog log : __loggers.values()) {
        log._level=log._configuredLevel;
      }
    }
  }
}

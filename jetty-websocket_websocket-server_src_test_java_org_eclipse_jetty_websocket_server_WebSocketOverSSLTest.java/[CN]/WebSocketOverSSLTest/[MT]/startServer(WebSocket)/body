{
  _server=new Server();
  SslSelectChannelConnector connector=new SslSelectChannelConnector();
  _server.addConnector(connector);
  SslContextFactory cf=connector.getSslContextFactory();
  cf.setKeyStorePath(MavenTestingUtils.getTestResourceFile("keystore").getAbsolutePath());
  cf.setKeyStorePassword("storepwd");
  cf.setKeyManagerPassword("keypwd");
  _server.setHandler(new WebSocketHandler(){
    @Override public WebSocket doWebSocketConnect(    HttpServletRequest request,    String protocol){
      return webSocket;
    }
  }
);
  _server.start();
  _port=connector.getLocalPort();
}

{
  final NIOBuffer i=_inNIOBuffer;
  final NIOBuffer o=_outNIOBuffer;
  return "SSL" + super.toString() + ","+ (_engine == null ? "-" : _engine.getHandshakeStatus())+ ((i != null && i.length() > 0) ? (",in=" + i.length()) : "")+ ((o != null && o.length() > 0) ? (",out=" + o.length()) : "")+ (_result == null ? "" : ("," + _result.getStatus() + ",bp="+ _result.bytesProduced()+ ",bc="+ _result.bytesConsumed()));
}

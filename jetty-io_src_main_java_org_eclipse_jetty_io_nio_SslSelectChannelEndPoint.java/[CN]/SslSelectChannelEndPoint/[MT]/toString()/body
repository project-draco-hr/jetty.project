{
  final NIOBuffer i=_inNIOBuffer;
  final NIOBuffer o=_outNIOBuffer;
  return "SSL" + super.toString() + ","+ (_engine == null ? "-" : _engine.getHandshakeStatus())+ ", in/out="+ (i == null ? 0 : i.length())+ "/"+ (o == null ? 0 : o.length())+ " bi/o="+ isBufferingInput()+ "/"+ isBufferingOutput()+ " "+ _result;
}

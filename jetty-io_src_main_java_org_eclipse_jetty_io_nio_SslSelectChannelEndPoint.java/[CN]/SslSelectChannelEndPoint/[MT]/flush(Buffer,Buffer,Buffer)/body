{
  LOG.debug("{} flush3",_session);
  int len=0;
  int flushed=0;
  if (header != null && header.hasContent()) {
    len=header.length();
    flushed=flush(header);
  }
  if (flushed == len && buffer != null && buffer.hasContent()) {
    int f=flush(buffer);
    if (f >= 0)     flushed+=f;
 else     if (flushed == 0)     flushed=-1;
  }
  return flushed;
}

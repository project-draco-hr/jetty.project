{
  try {
    _closing=true;
    shutdownOutput();
  }
 catch (  IOException e) {
    Log.ignore(e);
  }
 finally {
    super.close();
synchronized (this) {
      if (_inNIOBuffer != null)       _buffers.returnBuffer(_inNIOBuffer);
      _inNIOBuffer=null;
      if (_outNIOBuffer != null)       _buffers.returnBuffer(_outNIOBuffer);
      _outNIOBuffer=null;
    }
  }
}

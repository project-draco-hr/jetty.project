{
  final Task t0=new Task(true);
  Thread thread=new Thread(){
    @Override public void run(){
      _produce.add(t0);
      _produce.add(NULLTASK);
      _ewyk.execute();
    }
  }
;
  thread.start();
  t0.awaitRun();
  assertEquals(thread,t0.getThread());
  assertEquals(_ewyk,_executions.poll());
  _ewyk.run();
  assertThat(_ewyk.isIdle(),equalTo(true));
  t0.unblock();
  thread.join();
}

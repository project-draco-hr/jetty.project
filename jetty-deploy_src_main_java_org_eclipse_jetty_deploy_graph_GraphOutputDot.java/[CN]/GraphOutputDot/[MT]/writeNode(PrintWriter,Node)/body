{
  out.println();
  out.println("  // Node");
  out.printf("  \"%s\" [%n",toId(node));
  out.printf("    label=\"%s\",%n",node.getName());
  if (node.getName().endsWith("ed")) {
    out.printf("    color=\"#ddddff\",%n");
    out.printf("    style=filled,%n");
  }
  out.printf("    shape=box%n");
  out.println("  ];");
}

{
  System.err.printf("[slf4j.cl] loadClass(%s)%n",name);
  ClassNotFoundException ex=null;
  Class<?> c=null;
  try {
    c=this.findClass(name);
    if (resolve) {
      resolveClass(c);
    }
    return c;
  }
 catch (  ClassNotFoundException e) {
    ex=e;
  }
  if (c == null) {
    try {
      c=super.loadClass(name,resolve);
    }
 catch (    ClassNotFoundException e) {
      ex=e;
    }
  }
  throw ex;
}

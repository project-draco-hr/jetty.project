{
  Generator.Result result=queue.poll();
  if (result == null) {
    return Action.IDLE;
  }
  Generator.Result other=queue.poll();
  if (other != null)   result=result.join(other);
  active=result;
  ByteBuffer[] buffers=result.getByteBuffers();
  endPoint.write(this,buffers);
  return Action.SCHEDULED;
}

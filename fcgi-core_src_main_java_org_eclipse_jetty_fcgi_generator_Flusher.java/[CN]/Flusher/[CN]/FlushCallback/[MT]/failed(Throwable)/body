{
  if (active != null)   active.failed(x);
  active=null;
  List<Generator.Result> pending=new ArrayList<>();
synchronized (queue) {
    while (true) {
      Generator.Result result=queue.poll();
      if (result != null)       pending.add(result);
 else       break;
    }
  }
  for (  Generator.Result result : pending)   result.failed(x);
  super.failed(x);
}

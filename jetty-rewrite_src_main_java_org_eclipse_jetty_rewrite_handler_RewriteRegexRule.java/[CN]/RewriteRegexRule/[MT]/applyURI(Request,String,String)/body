{
  request.setRequestURI(newTarget);
  if (_query != null) {
    String query=(String)request.getAttribute("org.eclipse.jetty.rewrite.handler.RewriteRegexRule.Q");
    if (request.getQueryString() == null)     request.setQueryString(query);
 else     request.setQueryString(request.getQueryString() + "&" + query);
  }
}

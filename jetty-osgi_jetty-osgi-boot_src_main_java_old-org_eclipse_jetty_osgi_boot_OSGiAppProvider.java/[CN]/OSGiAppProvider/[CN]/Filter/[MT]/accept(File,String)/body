{
  File file=new File(dir,name);
  if (fileMightBeAnOSGiBundle(file)) {
    return true;
  }
  if (!file.isDirectory()) {
    String contextName=getDeployedAppName(name);
    if (contextName != null) {
      App app=_enclosedInstance.getDeployedApps().get(contextName);
      return app != null;
    }
  }
  return false;
}

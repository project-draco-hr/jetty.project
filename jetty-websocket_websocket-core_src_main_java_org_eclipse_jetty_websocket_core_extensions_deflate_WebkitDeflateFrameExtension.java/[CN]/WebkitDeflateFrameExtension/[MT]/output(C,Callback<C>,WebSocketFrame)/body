{
  if (frame.isControlFrame()) {
    nextOutput(context,callback,frame);
    return;
  }
  ByteBuffer data=frame.getPayload();
  try {
    ByteBuffer buf=deflate(data);
    frame.setPayload(buf);
    frame.setRsv1(true);
    nextOutput(context,callback,frame);
  }
  finally {
    getBufferPool().release(data);
  }
}

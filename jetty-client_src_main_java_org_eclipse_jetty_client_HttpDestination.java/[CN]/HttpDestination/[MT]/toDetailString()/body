{
  StringBuilder b=new StringBuilder();
  b.append(toString());
  b.append('\n');
synchronized (this) {
    for (    HttpConnection connection : _connections) {
      if (connection._exchange != null) {
        b.append(connection.toDetailString());
        if (_idle.contains(connection))         b.append(" IDLE");
        b.append('\n');
      }
    }
  }
  b.append("--");
  b.append('\n');
  return b.toString();
}

{
synchronized (this) {
    System.err.println(this);
    System.err.println("connections=" + _connections.size());
    System.err.println("idle=" + _idle.size());
    System.err.println("pending=" + _pendingConnections);
    for (    HttpConnection c : _connections) {
      if (!c.isIdle())       c.dump();
    }
  }
}

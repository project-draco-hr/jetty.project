{
  LOG.debug("{} released",connection);
  if (client.isRunning()) {
    boolean removed=activeConnections.remove(connection);
    if (removed)     process(connection,false);
 else     LOG.debug("{} explicit",connection);
  }
 else {
    LOG.debug("{} is stopped",client);
    remove(connection);
    connection.close();
  }
}

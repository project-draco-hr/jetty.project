{
synchronized (this) {
    out.append(String.valueOf(this));
    out.append("idle=");
    out.append(String.valueOf(_idleConnections.size()));
    out.append(" pending=");
    out.append(String.valueOf(_pendingConnections));
    out.append("\n");
    AggregateLifeCycle.dump(out,indent,_connections);
  }
}

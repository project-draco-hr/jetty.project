{
  String response=process("suspend=200&timeout=complete",null);
  assertEquals("HTTP/1.1 200 OK",response.substring(0,15));
  assertContains("history: REQUEST /ctx/path/info\r\n" + "history: initial\r\n" + "history: suspend\r\n"+ "history: onTimeout\r\n"+ "history: complete\r\n"+ "history: onComplete\r\n",response);
  assertContains("COMPLETED",response);
}

{
  String response=process("suspend=10&suspend2=10",null);
  assertContains("history: REQUEST\r\n" + "history: initial\r\n" + "history: suspend\r\n"+ "history: onTimeout\r\n"+ "history: ERROR\r\n"+ "history: !initial\r\n"+ "history: suspend\r\n"+ "history: onTimeout\r\n"+ "history: ERROR\r\n"+ "history: !initial\r\n"+ "history: onComplete\r\n",response);
  assertContains("ERROR: /ctx/path/info",response);
}

{
  _expectedCode="500 ";
  String response=process("suspend=10&suspend2=10",null);
  assertContains("history: REQUEST /ctx/path/info\r\n" + "history: initial\r\n" + "history: suspend\r\n"+ "history: onTimeout\r\n"+ "history: ERROR /ctx/path/info\r\n"+ "history: !initial\r\n"+ "history: suspend\r\n"+ "history: onTimeout\r\n"+ "history: ERROR /ctx/path/info\r\n"+ "history: !initial\r\n"+ "history: onComplete\r\n",response);
  assertContains("ERROR: /ctx/path/info",response);
}

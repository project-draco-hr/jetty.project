{
  String response=process("suspend=10&suspend2=1000&resume2=10",null);
  assertEquals("HTTP/1.1 200 OK",response.substring(0,15));
  assertContains("history: REQUEST /ctx/path/info\r\n" + "history: initial\r\n" + "history: suspend\r\n"+ "history: onTimeout\r\n"+ "history: ERROR /ctx/path/info\r\n"+ "history: !initial\r\n"+ "history: suspend\r\n"+ "history: resume\r\n"+ "history: ASYNC /ctx/path/info\r\n"+ "history: !initial\r\n"+ "history: onComplete\r\n",response);
  assertContains("DISPATCHED",response);
}

{
  String response=process("suspend=200",null);
  assertEquals("HTTP/1.1 500 Async Timeout",response.substring(0,26));
  assertContains("history: REQUEST\r\n" + "history: initial\r\n" + "history: suspend\r\n"+ "history: onTimeout\r\n"+ "history: ERROR\r\n"+ "history: !initial\r\n"+ "history: onComplete\r\n",response);
  assertContains("ERROR: /ctx/path/info",response);
}

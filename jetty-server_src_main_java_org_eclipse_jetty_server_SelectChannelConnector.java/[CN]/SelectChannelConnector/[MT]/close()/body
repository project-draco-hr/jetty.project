{
  ServerSocketChannel serverChannel=_acceptChannel;
  _acceptChannel=null;
  super.close();
  if (serverChannel != null) {
    removeBean(serverChannel);
    if (serverChannel.isOpen()) {
      try {
        serverChannel.close();
      }
 catch (      IOException e) {
        LOG.warn(e);
      }
    }
  }
  _localPort=-2;
}

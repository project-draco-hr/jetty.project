{
  Response response=client.newRequest("localhost",connector.getLocalPort()).scheme(scheme).method(HttpMethod.HEAD).path("/301/localhost/done").send().get(5,TimeUnit.SECONDS);
  Assert.assertNotNull(response);
  Assert.assertEquals(200,response.getStatus());
  Assert.assertFalse(response.getHeaders().containsKey(HttpHeader.LOCATION.asString()));
}

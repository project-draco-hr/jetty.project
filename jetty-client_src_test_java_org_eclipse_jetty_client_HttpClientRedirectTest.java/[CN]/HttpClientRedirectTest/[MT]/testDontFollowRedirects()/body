{
  Response response=client.newRequest("localhost",connector.getLocalPort()).scheme(scheme).followRedirects(false).path("/303/localhost/done?close=true").send().get(5,TimeUnit.SECONDS);
  Assert.assertNotNull(response);
  Assert.assertEquals(303,response.status());
  Assert.assertTrue(response.headers().containsKey(HttpHeader.LOCATION.asString()));
}

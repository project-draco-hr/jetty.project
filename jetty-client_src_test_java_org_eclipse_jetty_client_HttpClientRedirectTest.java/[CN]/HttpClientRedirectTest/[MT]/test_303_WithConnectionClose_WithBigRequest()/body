{
  Response response=client.newRequest("localhost",connector.getLocalPort()).scheme(scheme).path("/303/localhost/done?close=true").send().get(5,TimeUnit.SECONDS);
  Assert.assertNotNull(response);
  Assert.assertEquals(200,response.getStatus());
  Assert.assertFalse(response.getHeaders().containsKey(HttpHeader.LOCATION.asString()));
}

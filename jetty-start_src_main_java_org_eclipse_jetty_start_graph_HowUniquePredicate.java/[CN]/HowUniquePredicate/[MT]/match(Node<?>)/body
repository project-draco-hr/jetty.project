{
  if (node.getSelections().isEmpty()) {
    return false;
  }
  boolean ret=false;
  for (  Selection selection : node.getSelections()) {
    if (how.equalsIgnoreCase(selection.getHow())) {
      ret=true;
      continue;
    }
 else     if (selection.isExplicit()) {
      return false;
    }
  }
  return ret;
}

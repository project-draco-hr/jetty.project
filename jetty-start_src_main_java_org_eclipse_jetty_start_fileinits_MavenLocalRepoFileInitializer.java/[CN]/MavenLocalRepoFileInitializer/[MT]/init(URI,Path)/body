{
  Coordinates coords=getCoordinates(uri);
  if (coords == null) {
    return false;
  }
  if (isFilePresent(file)) {
    return true;
  }
  if (this.localRepositoryDir != null) {
    Path localRepoFile=getLocalRepoFile(coords);
    Files.copy(localRepoFile,file);
  }
 else {
    download(coords.toCentralURI(),file);
  }
  return true;
}

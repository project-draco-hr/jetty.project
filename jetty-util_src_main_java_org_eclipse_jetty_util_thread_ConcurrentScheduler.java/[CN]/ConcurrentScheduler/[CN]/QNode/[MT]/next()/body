{
  QNode next=get();
  while (true) {
    if (next == null)     return null;
    if (next.isDelayed() || next.isTail()) {
      if (next._prev != this && isDelayed())       next._prev=this;
      return next;
    }
    QNode next_next=next.get();
    compareAndSet(next,next_next);
    next=next_next;
  }
}

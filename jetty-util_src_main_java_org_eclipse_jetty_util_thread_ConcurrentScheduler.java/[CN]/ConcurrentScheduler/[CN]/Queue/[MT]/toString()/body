{
  StringBuilder b=new StringBuilder();
  b.append(String.format("Q@%x{%d,",hashCode(),_delay));
  b.append(_head);
  QNode next=_head.next();
  if (next != null && !next.isTail()) {
    b.append("->");
    b.append(next);
    next=next.next();
    if (next != null && !next.isTail())     b.append("...");
  }
  b.append("->");
  b.append(_tail);
  b.append("}");
  return b.toString();
}

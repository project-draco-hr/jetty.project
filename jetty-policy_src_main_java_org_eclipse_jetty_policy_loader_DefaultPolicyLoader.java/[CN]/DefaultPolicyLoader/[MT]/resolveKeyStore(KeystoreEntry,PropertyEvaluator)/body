{
  KeyStore keyStore=null;
  try {
    keyStore=KeyStore.getInstance(entry.type);
    URL keyStoreLocation=new URL(evaluator.evaluate(entry.url));
    InputStream istream=keyStoreLocation.openStream();
    keyStore.load(istream,null);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return keyStore;
}

{
  InputStream in=null;
  InputStreamReader isr=null;
  BufferedReader buf=null;
  try {
    in=connection.getInputStream();
    isr=new InputStreamReader(in);
    buf=new BufferedReader(isr);
    String line;
    while ((line=buf.readLine()) != null) {
      System.err.println(line);
    }
  }
  finally {
    IO.close(buf);
    IO.close(isr);
    IO.close(in);
  }
}

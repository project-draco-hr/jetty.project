{
  if (_serviceRegForBundles != null) {
    try {
      _serviceRegForBundles.unregister();
    }
 catch (    Exception e) {
      LOG.warn(e);
    }
  }
  if (_serviceRegForServices != null) {
    try {
      _serviceRegForServices.unregister();
    }
 catch (    Exception e) {
      LOG.warn(e);
    }
  }
  super.doStop();
}

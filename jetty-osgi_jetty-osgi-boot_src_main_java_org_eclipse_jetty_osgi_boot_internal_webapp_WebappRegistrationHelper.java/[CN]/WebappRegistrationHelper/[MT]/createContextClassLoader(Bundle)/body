{
  ClassLoader osgiCl=BUNDLE_CLASS_LOADER_HELPER.getBundleClassLoader(contributor);
  if (osgiCl != null) {
    TldLocatableURLClassloader composite=new TldLocatableURLClassloaderWithInsertedJettyClassloader(_libEtcClassLoader,osgiCl,getJarsWithTlds());
    return composite;
  }
 else {
    TldLocatableURLClassloader composite=new TldLocatableURLClassloader(_libEtcClassLoader,getJarsWithTlds());
    return composite;
  }
}

{
  boolean idle=created ? idleConnections.offerLast(connection) : idleConnections.offerFirst(connection);
  if (idle) {
    if (LOG.isDebugEnabled())     LOG.debug("Connection idle {}",connection);
    return true;
  }
 else {
    if (LOG.isDebugEnabled())     LOG.debug("Connection idle overflow {}",connection);
    connection.close();
    return false;
  }
}

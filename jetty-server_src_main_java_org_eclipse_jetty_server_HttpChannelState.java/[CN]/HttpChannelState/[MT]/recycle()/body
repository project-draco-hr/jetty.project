{
  cancelTimeout();
  try (SpinLock.Lock lock=_lock.lock()){
switch (_state) {
case DISPATCHED:
case ASYNC_IO:
      throw new IllegalStateException(getStatusString());
case UPGRADED:
    return;
default :
  break;
}
_asyncListeners=null;
_state=State.IDLE;
_async=null;
_initial=true;
_asyncReadPossible=_asyncReadUnready=false;
_asyncWrite=false;
_timeoutMs=DEFAULT_TIMEOUT;
_event=null;
}

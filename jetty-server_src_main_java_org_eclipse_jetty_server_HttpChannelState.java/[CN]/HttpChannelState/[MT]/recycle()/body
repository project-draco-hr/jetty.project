{
synchronized (this) {
switch (_state) {
case DISPATCHED:
case REDISPATCHED:
      throw new IllegalStateException(getStatusString());
default :
    _state=State.IDLE;
}
_initial=true;
_expired=false;
_responseWrapped=false;
cancelTimeout();
_timeoutMs=DEFAULT_TIMEOUT;
_event=null;
_asyncIO=false;
}
}

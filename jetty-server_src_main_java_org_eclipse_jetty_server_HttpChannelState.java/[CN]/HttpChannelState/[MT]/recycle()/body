{
synchronized (this) {
switch (_state) {
case DISPATCHED:
case REDISPATCHED:
      throw new IllegalStateException(getStatusString());
default :
    _state=State.IDLE;
}
_initial=true;
_resumed=false;
_expired=false;
_responseWrapped=false;
cancelTimeout();
_timeoutMs=DEFAULT_TIMEOUT;
_continuationListeners=null;
if (_event != null) _event._cause=null;
}
}

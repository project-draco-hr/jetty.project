{
synchronized (this) {
switch (_state) {
case DISPATCHED:
case ASYNCIO:
      throw new IllegalStateException(getStatusString());
default :
    break;
}
_asyncListeners=null;
_state=State.IDLE;
_async=null;
_initial=true;
_asyncRead=false;
_asyncWrite=false;
_timeoutMs=DEFAULT_TIMEOUT;
cancelTimeout();
_event=null;
}
}

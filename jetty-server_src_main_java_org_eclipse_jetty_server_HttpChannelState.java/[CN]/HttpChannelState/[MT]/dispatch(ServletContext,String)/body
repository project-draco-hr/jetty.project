{
  boolean dispatch;
synchronized (this) {
switch (_state) {
case ASYNCSTARTED:
      _state=State.REDISPATCHING;
    _dispatched=true;
  dispatch=false;
break;
case ASYNCWAIT:
dispatch=!_expired;
_state=State.REDISPATCH;
_dispatched=true;
break;
default :
throw new IllegalStateException(this.getStatusString());
}
if (context != null) _event.setDispatchContext(context);
if (path != null) _event.setDispatchPath(path);
}
if (dispatch) {
cancelTimeout();
scheduleDispatch();
}
}

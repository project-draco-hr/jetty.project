{
synchronized (this) {
switch (_state) {
case DISPATCHED:
case ASYNCIO:
      break;
default :
    throw new IllegalStateException(this.getStatusString());
}
if (_asyncIO) {
  _asyncIO=false;
  _state=State.ASYNCIO;
  return Action.IO_CALLBACK;
}
if (_async != null) {
  _initial=false;
switch (_async) {
case COMPLETE:
    _state=State.COMPLETING;
  _async=null;
return Action.COMPLETE;
case DISPATCH:
_state=State.DISPATCHED;
_async=null;
return Action.ASYNC_DISPATCH;
case EXPIRED:
_state=State.DISPATCHED;
_async=null;
return Action.ASYNC_EXPIRED;
case EXPIRING:
case STARTED:
scheduleTimeout();
_state=State.ASYNCWAIT;
return Action.WAIT;
}
}
_state=State.COMPLETING;
return Action.COMPLETE;
}
}

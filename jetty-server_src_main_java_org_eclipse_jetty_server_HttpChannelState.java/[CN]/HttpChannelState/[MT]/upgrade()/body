{
  cancelTimeout();
  try (Locker.Lock lock=_locker.lock()){
switch (_state) {
case IDLE:
case COMPLETED:
      break;
default :
    throw new IllegalStateException(getStatusString());
}
_asyncListeners=null;
_state=State.UPGRADED;
_async=null;
_initial=true;
_asyncReadPossible=_asyncReadUnready=false;
_asyncWrite=false;
_timeoutMs=DEFAULT_TIMEOUT;
_event=null;
}

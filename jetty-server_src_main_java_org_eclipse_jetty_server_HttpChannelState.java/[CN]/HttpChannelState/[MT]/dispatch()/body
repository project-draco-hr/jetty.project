{
  boolean dispatch=false;
synchronized (this) {
switch (_state) {
case ASYNCSTARTED:
      _state=State.REDISPATCHING;
    _dispatched=true;
  return;
case ASYNCWAIT:
dispatch=!_expired;
_state=State.REDISPATCH;
_dispatched=true;
break;
case REDISPATCH:
return;
default :
throw new IllegalStateException(this.getStatusString());
}
}
if (dispatch) {
cancelTimeout();
scheduleDispatch();
}
}

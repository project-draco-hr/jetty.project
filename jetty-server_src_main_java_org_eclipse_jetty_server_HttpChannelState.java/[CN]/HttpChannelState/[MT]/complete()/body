{
  boolean dispatch=false;
synchronized (this) {
switch (_state) {
case DISPATCHED:
case REDISPATCHED:
      throw new IllegalStateException(this.getStatusString());
case IDLE:
case ASYNCSTARTED:
    _state=State.COMPLETING;
  return;
case ASYNCWAIT:
_state=State.COMPLETING;
dispatch=!_expired;
break;
default :
throw new IllegalStateException(this.getStatusString());
}
}
if (dispatch) {
cancelTimeout();
scheduleDispatch();
}
}

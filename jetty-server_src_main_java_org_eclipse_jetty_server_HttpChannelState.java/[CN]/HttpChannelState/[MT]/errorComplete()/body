{
synchronized (this) {
switch (_state) {
case ASYNCSTARTED:
case REDISPATCHING:
      _state=State.COMPLETECALLED;
    break;
case COMPLETECALLED:
  break;
default :
throw new IllegalStateException(this.getStatusString());
}
_dispatched=false;
_event.setDispatchContext(null);
_event.setDispatchPath(null);
}
}

{
  LOG.debug("ishut {}",this);
  if (_channel.isOpen()) {
    if (_socket != null) {
      try {
        if (!_socket.isInputShutdown()) {
          _socket.shutdownInput();
        }
      }
 catch (      SocketException e) {
        LOG.debug(e.toString());
        LOG.ignore(e);
      }
 finally {
        _ishut=true;
        if (_socket.isOutputShutdown() && !_socket.isClosed())         close();
      }
    }
 else     _ishut=true;
  }
}

{
  LOG.debug("oshut {}",this);
  _oshut=true;
  if (_channel.isOpen()) {
    if (_socket != null) {
      try {
        if (!_socket.isOutputShutdown()) {
          _socket.shutdownOutput();
        }
      }
 catch (      SocketException e) {
        LOG.debug(e.toString());
        LOG.ignore(e);
      }
 finally {
        if (_ishut) {
          close();
        }
      }
    }
  }
}

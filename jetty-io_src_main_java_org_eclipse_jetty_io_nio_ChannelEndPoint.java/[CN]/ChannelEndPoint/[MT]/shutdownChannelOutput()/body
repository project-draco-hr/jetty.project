{
  if (_channel.isOpen()) {
    if (_channel instanceof SocketChannel) {
      try {
        Socket socket=((SocketChannel)_channel).socket();
        if (!socket.isOutputShutdown())         socket.shutdownOutput();
        if (socket.isInputShutdown())         close();
      }
 catch (      SocketException e) {
        LOG.ignore(e);
      }
    }
 else     close();
  }
}

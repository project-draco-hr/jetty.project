{
  LOG.debug("oshut {}",this);
  if (_channel.isOpen()) {
    if (_socket != null) {
      try {
        if (!_socket.isOutputShutdown()) {
          _socket.shutdownOutput();
        }
      }
 catch (      SocketException e) {
        LOG.debug(e.toString());
        LOG.ignore(e);
        if (!_socket.isClosed())         close();
      }
 finally {
        if (_socket.isInputShutdown() && !_socket.isClosed())         close();
      }
    }
 else     close();
  }
}

{
  int filled=fill();
  if (filled == 0) {
    if (protocol == null) {
      if (engine.getHandshakeStatus() == SSLEngineResult.HandshakeStatus.NOT_HANDSHAKING) {
        if (LOG.isDebugEnabled())         LOG.debug("{} could not negotiate protocol, SSLEngine: {}",this,engine);
        close();
      }
 else {
        fillInterested();
      }
    }
 else {
      ConnectionFactory connectionFactory=connector.getConnectionFactory(protocol);
      if (connectionFactory == null) {
        if (LOG.isDebugEnabled())         LOG.debug("{} application selected protocol '{}', but no correspondent {} has been configured",this,protocol,ConnectionFactory.class.getName());
        close();
      }
 else {
        EndPoint endPoint=getEndPoint();
        Connection oldConnection=endPoint.getConnection();
        Connection newConnection=connectionFactory.newConnection(connector,endPoint);
        if (LOG.isDebugEnabled())         LOG.debug("{} switching from {} to {}",this,oldConnection,newConnection);
        oldConnection.onClose();
        endPoint.setConnection(newConnection);
        getEndPoint().getConnection().onOpen();
      }
    }
  }
 else   if (filled < 0) {
    if (LOG.isDebugEnabled())     LOG.debug("{} closing on client close",this);
    close();
  }
 else {
    throw new IllegalStateException();
  }
}

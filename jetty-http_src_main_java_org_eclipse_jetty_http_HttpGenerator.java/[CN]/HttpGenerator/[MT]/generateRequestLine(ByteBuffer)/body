{
  header.put(StringUtil.getBytes(_request.getMethod()));
  header.put((byte)' ');
  header.put(StringUtil.getBytes(_request.getURI()));
switch (_info.getHttpVersion()) {
case HTTP_1_0:
case HTTP_1_1:
    header.put((byte)' ');
  header.put(_info.getHttpVersion().toBytes());
}
header.put(HttpTokens.CRLF);
}

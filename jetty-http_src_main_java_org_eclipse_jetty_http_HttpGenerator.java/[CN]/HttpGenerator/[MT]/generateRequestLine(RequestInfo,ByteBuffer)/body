{
  header.put(StringUtil.getBytes(request.getMethod()));
  header.put((byte)' ');
  header.put(StringUtil.getBytes(request.getUri()));
switch (request.getHttpVersion()) {
case HTTP_1_0:
case HTTP_1_1:
    header.put((byte)' ');
  header.put(request.getHttpVersion().toBytes());
}
header.put(HttpTokens.CRLF);
}

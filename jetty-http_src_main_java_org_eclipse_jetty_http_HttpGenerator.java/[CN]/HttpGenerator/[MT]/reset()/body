{
  if (_persistent != null && !_persistent && _endp != null && !_endp.isOutputShutdown()) {
    try {
      _endp.shutdownOutput();
    }
 catch (    IOException e) {
      LOG.ignore(e);
    }
  }
  super.reset();
  if (_buffer != null)   _buffer.clear();
  if (_header != null)   _header.clear();
  if (_content != null)   _content=null;
  _bypass=false;
  _needCRLF=false;
  _needEOC=false;
  _bufferChunked=false;
  _method=null;
  _uri=null;
  _noContent=false;
}

{
  if (_stylesheet != null) {
    return _stylesheet;
  }
 else {
    if (_defaultStylesheet == null) {
      try {
        _defaultStylesheet=Resource.newResource(this.getClass().getResource("/jetty-default.css"));
      }
 catch (      IOException e) {
        LOG.warn(e.toString());
        LOG.debug(e);
      }
    }
    return _defaultStylesheet;
  }
}

{
  if (remote != null) {
    return;
  }
  connection.getIOState().onConnected();
  remote=new WebSocketRemoteEndpoint(connection,outgoingHandler,getBatchMode());
  websocket.openSession(this);
  connection.getIOState().onOpened();
  if (LOG.isDebugEnabled()) {
    LOG.debug("open -> {}",dump());
  }
}

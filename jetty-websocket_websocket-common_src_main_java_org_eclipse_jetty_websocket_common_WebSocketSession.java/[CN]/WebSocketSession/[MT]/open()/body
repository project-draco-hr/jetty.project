{
  if (isOpen()) {
    throw new WebSocketException("Cannot Open WebSocketSession, Already open");
  }
  remote=new WebSocketRemoteEndpoint(connection,outgoingHandler);
  this.active=true;
  websocket.setSession(this);
  websocket.onConnect();
  if (LOG.isDebugEnabled()) {
    LOG.debug("{}",dump());
  }
}

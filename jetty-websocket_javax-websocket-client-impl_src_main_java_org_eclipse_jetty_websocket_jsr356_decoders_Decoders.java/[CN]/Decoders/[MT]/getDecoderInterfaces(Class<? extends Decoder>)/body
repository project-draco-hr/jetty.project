{
  List<ParameterizedType> ret=new ArrayList<>();
  for (  Type type : decoder.getGenericInterfaces()) {
    if (!(type instanceof ParameterizedType)) {
      continue;
    }
    ParameterizedType ptype=(ParameterizedType)type;
    if (DeploymentTypeUtils.isAssignable(type,Decoder.Text.class) || DeploymentTypeUtils.isAssignable(type,Decoder.TextStream.class) || DeploymentTypeUtils.isAssignable(type,Decoder.Binary.class)|| DeploymentTypeUtils.isAssignable(type,Decoder.BinaryStream.class)) {
      ret.add(ptype);
    }
  }
  return ret;
}

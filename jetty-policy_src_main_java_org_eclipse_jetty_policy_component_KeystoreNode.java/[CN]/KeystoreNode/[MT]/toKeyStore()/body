{
  if (keystore != null && !isDirty()) {
    return keystore;
  }
  try {
    keystore=KeyStore.getInstance(type);
    URL keyStoreLocation=new URL(url);
    InputStream istream=keyStoreLocation.openStream();
    keystore.load(istream,null);
  }
 catch (  Exception e) {
    throw new PolicyException(e);
  }
  return keystore;
}

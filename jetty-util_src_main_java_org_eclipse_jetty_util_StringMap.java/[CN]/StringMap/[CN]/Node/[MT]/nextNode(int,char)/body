{
  if (index < _chars.length) {
    if (_chars[index] == c)     return this;
    if (_next != null)     return _next.nextNode(index,c);
  }
  if (index == _chars.length && _splits != null) {
    Node<O> split=_splits[c % _splits.length];
    if (split != null)     return split.nextNode(index,c);
  }
  if (_next != null)   return _next.nextNode(index,c);
  return null;
}

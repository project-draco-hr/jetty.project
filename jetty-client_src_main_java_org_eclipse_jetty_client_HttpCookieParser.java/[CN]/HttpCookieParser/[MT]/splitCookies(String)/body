{
  List<String> result=new ArrayList<>();
  int start=0;
  int quotes=0;
  for (int i=0; i < headerValue.length(); ++i) {
    char c=headerValue.charAt(i);
    if (c == ',' && quotes % 2 == 0) {
      result.add(headerValue.substring(start,i));
      start=i + 1;
    }
 else     if (c == '"') {
      ++quotes;
    }
  }
  result.add(headerValue.substring(start));
  return result;
}

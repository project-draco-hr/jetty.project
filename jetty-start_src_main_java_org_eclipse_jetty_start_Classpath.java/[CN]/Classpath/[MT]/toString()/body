{
  StringBuffer cp=new StringBuffer(1024);
  int cnt=_elements.size();
  if (cnt >= 1) {
    cp.append(((_elements.elementAt(0))).getPath());
  }
  for (int i=1; i < cnt; i++) {
    cp.append(File.pathSeparatorChar);
    cp.append(((_elements.elementAt(i))).getPath());
  }
  return cp.toString();
}

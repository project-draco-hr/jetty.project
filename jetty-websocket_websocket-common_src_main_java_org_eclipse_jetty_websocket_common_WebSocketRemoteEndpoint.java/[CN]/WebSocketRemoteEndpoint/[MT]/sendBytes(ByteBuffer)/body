{
  if (msgLock.tryLock()) {
    try {
      msgType.set(BINARY);
      connection.getIOState().assertOutputOpen();
      if (LOG.isDebugEnabled()) {
        LOG.debug("sendBytes with {}",BufferUtil.toDetailString(data));
      }
      blockingWrite(new BinaryFrame(data));
    }
  finally {
      msgType.set(NONE);
      msgLock.unlock();
    }
  }
 else {
    throw new IllegalStateException(PRIORMSG_ERROR);
  }
}

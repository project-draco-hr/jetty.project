{
  LOG.debug("{} sslEndp.read()",_session);
  _lock.lock();
  try {
    if (BufferUtil.hasContent(_inApp))     return DoneIOFuture.COMPLETE;
    _appReadFuture=new DispatchingIOFuture(_lock);
    if (_netReadFuture == null)     _netReadFuture=scheduleOnReadable();
    return _appReadFuture;
  }
  finally {
    _lock.unlock();
  }
}

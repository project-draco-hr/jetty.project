{
  return new HTTPSPDYServerConnector(){
    private final AsyncConnectionFactory defaultAsyncConnectionFactory=new ServerHTTPSPDYAsyncConnectionFactory(SPDY.V2,getByteBufferPool(),getExecutor(),getScheduler(),this,new ReferrerPushStrategy());
    @Override protected AsyncConnectionFactory getDefaultAsyncConnectionFactory(){
      return defaultAsyncConnectionFactory;
    }
  }
;
}

{
  int len=((_buffer.peek() & 0xFF) << 8) | (_buffer.peek(_buffer.getIndex() + 1) & 0xFF);
  if ((0xFF00 & len) == 0xA000) {
    _buffer.skip(1);
    return Ajp13RequestHeaders.CACHE.get(_buffer.get());
  }
  int start=_buffer.getIndex();
  tok.update(start + 2,start + len + 2);
  _buffer.skip(len + 3);
  return tok;
}

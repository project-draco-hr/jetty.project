{
  List<ByteBuffer> result=new ArrayList<>();
  long position=0;
  long length=channel.size();
  channel.position(position);
  while (length > 0) {
    long size=Math.min(1024 * 1024 * 1024,length);
    ByteBuffer buffer=channel.map(FileChannel.MapMode.READ_ONLY,position,size);
    result.add(buffer);
    position+=size;
    length-=size;
  }
  return result;
}

{
  _dftMaxIdleSecs=seconds;
  if (__log.isDebugEnabled()) {
    if (_dftMaxIdleSecs <= 0)     __log.debug("Sessions created by this manager are immortal (default maxInactiveInterval={})",_dftMaxIdleSecs);
 else     __log.debug("SessionManager default maxInactiveInterval={}",_dftMaxIdleSecs);
  }
}

{
  ByteBuffer buffer=delegate.acquire(size,direct);
  boolean leakd=leakDetector.acquired(buffer);
  if (NOISY || !leakd)   LOG.info(String.format("ByteBuffer acquire %s@%X leakd.acquired=%s",buffer,System.identityHashCode(buffer),leakd ? "normal" : "LEAK"),new Throwable("LeakStack.Acquire"));
  return buffer;
}

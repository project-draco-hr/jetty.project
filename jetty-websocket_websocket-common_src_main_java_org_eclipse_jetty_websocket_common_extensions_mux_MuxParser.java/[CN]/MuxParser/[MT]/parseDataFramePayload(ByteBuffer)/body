{
  int capacity=buffer.remaining();
  ByteBuffer payload=ByteBuffer.allocateDirect(capacity);
  payload.put(buffer);
  BufferUtil.flipToFlush(payload,0);
  muxframe.setPayload(payload);
  try {
    LOG.debug("notifyFrame() - {}",muxframe);
    events.onMuxedFrame(muxframe);
  }
 catch (  Throwable t) {
    LOG.warn(t);
  }
}

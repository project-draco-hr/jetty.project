{
  Collection<WeakReference<HttpSession>> sessions;
synchronized (this) {
    sessions=_sessions.remove(id);
  }
  if (sessions != null) {
    for (    WeakReference<HttpSession> ref : sessions) {
      Session session=(Session)ref.get();
      if (session != null && session.isValid())       session.invalidate();
    }
    sessions.clear();
  }
}

{
  if (!isExpanded()) {
    throw new PolicyException("GrantNode needs to be expanded.");
  }
  try {
    if (codesource == null && codebase != null) {
      URL url=new URL(codebase);
      codesource=new CodeSource(url,signerArray);
    }
    return codesource;
  }
 catch (  Exception e) {
    throw new PolicyException(e);
  }
}

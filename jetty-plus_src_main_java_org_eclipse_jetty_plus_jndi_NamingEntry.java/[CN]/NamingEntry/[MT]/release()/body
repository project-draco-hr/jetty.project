{
  try {
    InitialContext ic=new InitialContext();
    ic.unbind(objectNameString);
    ic.unbind(namingEntryNameString);
    this.jndiName=null;
    this.namingEntryNameString=null;
    this.objectNameString=null;
    this.objectToBind=null;
  }
 catch (  NamingException e) {
    Log.warn(e);
  }
}

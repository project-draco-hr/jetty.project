{
  StringBuilder builder=new StringBuilder();
  builder.append("Generator[");
  builder.append(policy.getBehavior());
  if (validating) {
    builder.append(",validating");
  }
  if (rsv1InUse) {
    builder.append(",+rsv1");
  }
  if (rsv2InUse) {
    builder.append(",+rsv2");
  }
  if (rsv3InUse) {
    builder.append(",+rsv3");
  }
  builder.append("]");
  return builder.toString();
}

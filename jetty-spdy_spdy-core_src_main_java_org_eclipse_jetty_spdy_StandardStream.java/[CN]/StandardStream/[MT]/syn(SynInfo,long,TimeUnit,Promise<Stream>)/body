{
  if (isClosed() || isReset()) {
    promise.failed(new StreamException(getId(),StreamStatus.STREAM_ALREADY_CLOSED,"Stream: " + this + " already closed or reset!"));
    return;
  }
  PushSynInfo pushSynInfo=new PushSynInfo(getId(),synInfo);
  session.syn(pushSynInfo,null,timeout,unit,promise);
}

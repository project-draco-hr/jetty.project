{
  notIdle();
  if (isRemotelyClosed()) {
    if (LOG.isDebugEnabled())     LOG.debug("Stream is remotely closed, ignoring {}",dataInfo);
    return;
  }
  if (!canReceive()) {
    if (LOG.isDebugEnabled())     LOG.debug("Protocol error receiving {}, resetting",dataInfo);
    session.rst(new RstInfo(getId(),StreamStatus.PROTOCOL_ERROR),Callback.Adapter.INSTANCE);
    return;
  }
  updateCloseState(dataInfo.isClose(),false);
  notifyOnData(dataInfo);
}

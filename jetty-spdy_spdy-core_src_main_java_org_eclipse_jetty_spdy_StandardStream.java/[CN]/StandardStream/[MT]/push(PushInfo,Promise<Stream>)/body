{
  notIdle();
  if (isClosed() || isReset()) {
    close();
    promise.failed(new StreamException(getId(),StreamStatus.STREAM_ALREADY_CLOSED,"Stream: " + this + " already closed or reset!"));
    return;
  }
  PushSynInfo pushSynInfo=new PushSynInfo(getId(),pushInfo);
  session.syn(pushSynInfo,null,new StreamPromise(promise));
}

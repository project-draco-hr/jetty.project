{
  for (  Method method : handlerClass.getMethods()) {
    if ("onMessage".equals(method.getName())) {
      Class<?> paramTypes[]=method.getParameterTypes();
      if (paramTypes == null) {
        continue;
      }
      if (paramTypes.length == paramCount) {
        return paramTypes[0];
      }
    }
  }
  return null;
}

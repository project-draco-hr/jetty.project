{
  StringBuilder result=new StringBuilder();
  int frameStart=input.read();
  assertEquals(0x00,frameStart);
  while (true) {
    int read=input.read();
    if (read == -1)     throw new EOFException();
    char c=(char)read;
    if (c == 0xFF)     break;
    result.append(c);
  }
  return result.toString();
}

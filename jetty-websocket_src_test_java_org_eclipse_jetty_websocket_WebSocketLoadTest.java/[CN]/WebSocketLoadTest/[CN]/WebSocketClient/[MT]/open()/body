{
  output.write("GET /test HTTP/1.1\r\n" + "Host: localhost\r\n" + "Upgrade: WebSocket\r\n"+ "Connection: Upgrade\r\n"+ "\r\n");
  output.flush();
  String responseLine=input.readLine();
  assertTrue(responseLine.startsWith("HTTP/1.1 101 Web Socket Protocol Handshake"));
  String line;
  while ((line=input.readLine()) != null)   if (line.length() == 0)   break;
}

{
  try {
    String message="0123456789ABCDEF0123456789ABCDEF0123456789ABCDEF0123456789ABCDEF";
    for (int i=0; i < iterations; ++i) {
      writeString(message);
      String result=readString();
      assertEquals(message,result);
      latch.countDown();
    }
  }
 catch (  IOException x) {
    throw new RuntimeException(x);
  }
}

{
switch (transport) {
case HTTP:
case HTTPS:
{
      return new HttpClientTransportOverHTTP(1);
    }
case H2C:
case H2:
{
    HTTP2Client http2Client=new HTTP2Client();
    http2Client.setSelectors(1);
    return new HttpClientTransportOverHTTP2(http2Client);
  }
default :
{
  throw new IllegalArgumentException();
}
}
}

{
  String sameNames="--AaB03x\r\n" + "content-disposition: form-data; name=\"stuff\"; filename=\"stuff1.txt\"\r\n" + "Content-Type: text/plain\r\n"+ "\r\n"+ "00000\r\n"+ "--AaB03x\r\n"+ "content-disposition: form-data; name=\"stuff\"; filename=\"stuff2.txt\"\r\n"+ "Content-Type: text/plain\r\n"+ "\r\n"+ "000000000000000000000000000000000000000000000000000\r\n"+ "--AaB03x--\r\n";
  MultipartConfigElement config=new MultipartConfigElement(_dirname,1024,3072,50);
  MultiPartInputStream mpis=new MultiPartInputStream(new ByteArrayInputStream(sameNames.getBytes()),_contentType,config,new File(_dirname));
  Collection<Part> parts=mpis.getParts();
  assertEquals(2,parts.size());
  for (  Part p : parts)   assertEquals("stuff",p.getName());
  Part p=mpis.getPart("stuff");
  assertNotNull(p);
  assertEquals(5,p.getSize());
}

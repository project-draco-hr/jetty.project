{
  if (!shouldDispatchCompletion()) {
    if (_maxRecursion <= 0) {
      onCompleted(context);
      return;
    }
 else {
      Integer calls=__calls.get();
      if (calls < _maxRecursion) {
        try {
          __calls.set(calls + 1);
          onCompleted(context);
          return;
        }
  finally {
          __calls.set(calls);
        }
      }
    }
  }
  _executor.execute(context == null ? _onNullContextCompleted : new Runnable(){
    @Override public void run(){
      onCompleted(context);
    }
  }
);
}

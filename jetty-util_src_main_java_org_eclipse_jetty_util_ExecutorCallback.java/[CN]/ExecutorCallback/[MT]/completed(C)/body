{
  if (_invoker == null) {
    if (context == null)     _executor.execute(_onComplete);
 else {
      _executor.execute(new Runnable(){
        @Override public void run(){
          onCompleted(context);
        }
      }
);
    }
  }
 else   if (alwaysDispatchCompletion()) {
    _invoker.fork(context);
  }
 else {
    _invoker.invoke(context);
  }
}

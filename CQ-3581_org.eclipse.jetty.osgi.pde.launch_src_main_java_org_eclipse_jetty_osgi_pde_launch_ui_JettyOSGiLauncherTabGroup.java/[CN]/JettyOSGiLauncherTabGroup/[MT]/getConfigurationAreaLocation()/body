{
  OSGiSettingsTab settingsTab=null;
  for (  ILaunchConfigurationTab t : getTabs()) {
    if (t instanceof OSGiSettingsTab) {
      settingsTab=(OSGiSettingsTab)t;
      break;
    }
  }
  if (settingsTab == null) {
    return null;
  }
  return getfLastEnteredConfigArea(getfConfigurationBlock(settingsTab));
}

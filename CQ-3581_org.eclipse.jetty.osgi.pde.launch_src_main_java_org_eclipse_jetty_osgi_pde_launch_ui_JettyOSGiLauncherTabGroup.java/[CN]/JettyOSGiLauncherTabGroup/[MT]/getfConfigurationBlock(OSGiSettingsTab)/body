{
  try {
    if (fConfigurationBlock == null) {
      fConfigurationBlock=OSGiSettingsTab.class.getDeclaredField("fConfigurationBlock");
      fConfigurationBlock.setAccessible(true);
    }
    return fConfigurationBlock.get(settingsTab);
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
  return null;
}

{
  getLog().info("Configuring Jetty for project: " + project.getName());
  if (skip) {
    getLog().info("Skipping Jetty start: jetty.skip==true");
    return;
  }
  PluginLog.setLog(getLog());
  Runtime.getRuntime().addShutdownHook(new ShutdownThread());
  random=new Random();
  startJettyRunner();
}

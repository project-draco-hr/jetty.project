{
  ISession session=stream.getSession();
  int oldSize=session.updateRecvWindow(-length);
  if (LOG.isDebugEnabled())   LOG.debug("Updated session recv window {} -> {} for {}",oldSize,oldSize - length,session);
  oldSize=stream.updateRecvWindow(-length);
  if (LOG.isDebugEnabled())   LOG.debug("Updated stream recv window {} -> {} for {}",oldSize,oldSize - length,stream);
}

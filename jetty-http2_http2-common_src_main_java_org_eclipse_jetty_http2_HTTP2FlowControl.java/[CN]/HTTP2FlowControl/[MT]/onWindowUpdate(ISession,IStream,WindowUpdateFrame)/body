{
  int delta=frame.getWindowDelta();
  if (frame.getStreamId() > 0) {
    if (stream != null) {
      int oldSize=stream.updateWindowSize(delta);
      if (LOG.isDebugEnabled())       LOG.debug("Updated stream window {} -> {} for {}",oldSize,oldSize + delta,stream);
    }
  }
 else {
    int oldSize=session.updateWindowSize(delta);
    if (LOG.isDebugEnabled())     LOG.debug("Updated session window {} -> {} for {}",oldSize,oldSize + delta,session);
  }
}

{
  if (length == 0)   return;
  ISession session=stream.getSession();
  int oldSize=session.updateSendWindow(-length);
  if (LOG.isDebugEnabled())   LOG.debug("Updated session send window {} -> {} for {}",oldSize,oldSize - length,session);
  oldSize=stream.updateSendWindow(-length);
  if (LOG.isDebugEnabled())   LOG.debug("Updated stream send window {} -> {} for {}",oldSize,oldSize - length,stream);
}

{
  boolean relativeRange=true;
  assertInvalidRange("bytes=a-b",relativeRange);
  assertInvalidRange("byte=10-3",relativeRange);
  assertInvalidRange("onceuponatime=5-10",relativeRange);
  assertInvalidRange("bytes=300-310",relativeRange);
  relativeRange=false;
  assertInvalidRange("bytes=-5",relativeRange);
  assertInvalidRange("bytes=10-",relativeRange);
  assertInvalidRange("bytes=250-300",relativeRange);
}

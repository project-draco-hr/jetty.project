{
  int oldInterestOps;
  int newInterestOps;
  boolean pending;
  try (SpinLock.Lock lock=_lock.lock()){
    pending=_updatePending;
    oldInterestOps=_desiredInterestOps;
    newInterestOps=oldInterestOps | operation;
    if (newInterestOps != oldInterestOps)     _desiredInterestOps=newInterestOps;
  }
   if (LOG.isDebugEnabled())   LOG.debug("changeInterests p={} {}->{} for {}",pending,oldInterestOps,newInterestOps,this);
  if (!pending)   _selector.submit(_runUpdateKey);
}

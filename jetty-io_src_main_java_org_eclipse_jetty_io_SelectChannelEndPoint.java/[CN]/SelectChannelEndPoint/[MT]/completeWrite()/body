{
  if (_writeBuffers == null)   return;
  try {
    flush(_writeBuffers);
    for (    ByteBuffer b : _writeBuffers) {
      if (b.hasRemaining()) {
        _interestOps=_interestOps | SelectionKey.OP_WRITE;
        return;
      }
    }
    writeCompleted();
  }
 catch (  IOException e) {
    writeFailed(e);
  }
}

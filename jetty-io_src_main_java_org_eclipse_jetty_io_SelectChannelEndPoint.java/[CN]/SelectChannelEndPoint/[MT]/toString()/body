{
  String keyString="";
  if (_key.isValid()) {
    if (_key.isReadable())     keyString+="r";
    if (_key.isWritable())     keyString+="w";
  }
 else {
    keyString+="!";
  }
  return String.format("SCEP@%x{l(%s)<->r(%s),open=%b,ishut=%b,oshut=%b,i=%d%s,r=%s,w=%s}-{%s}",hashCode(),getRemoteAddress(),getLocalAddress(),isOpen(),isInputShutdown(),isOutputShutdown(),_interestOps,keyString,_readInterest,_writeFlusher,getConnection());
}

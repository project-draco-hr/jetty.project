{
  while (true) {
    State current=_interestState.get();
    if (LOG.isDebugEnabled())     LOG.debug("Updating key, state {} for {}",current,this);
switch (current) {
case UPDATE_PENDING:
case UPDATED:
{
        if (!_interestState.compareAndSet(current,State.LOCKED))         continue;
        try {
          setKeyInterests();
          return;
        }
  finally {
          _interestState.set(State.UPDATED);
        }
      }
case LOCKED:
{
      Thread.yield();
      break;
    }
default :
{
    throw new IllegalStateException("Invalid state: " + current);
  }
}
}
}

{
  baseRequest.setHandled(true);
  if (_bufferSize > 0)   response.setBufferSize(_bufferSize);
  if (_mimeType != null)   response.setContentType(_mimeType);
  for (int i=0; i < _writes; i++) {
    response.addHeader("Write",Integer.toString(i));
    response.getOutputStream().write(_content);
    if (_flush)     response.getOutputStream().flush();
  }
  if (_close)   response.getOutputStream().close();
  response.addHeader("Written","true");
}

{
  if (_u >= _uris.size()) {
    _u=0;
    _r++;
    if (_r >= _repeats)     return false;
  }
  String uri=_uris.get(_u++);
  reset();
  setMethod(HttpMethods.GET);
  setURL(uri);
  try {
    _client.send(this);
  }
 catch (  IOException e) {
    e.printStackTrace();
    return false;
  }
  return true;
}

{
  ArrayList<URI> uris=new ArrayList<URI>();
  while (loader != null && (loader instanceof URLClassLoader)) {
    URL[] urls=((URLClassLoader)loader).getURLs();
    if (urls != null) {
      for (      URL u : urls) {
        try {
          uris.add(u.toURI());
        }
 catch (        Exception e) {
          Log.warn(e);
        }
      }
    }
    if (visitParent)     loader=loader.getParent();
 else     loader=null;
  }
  return uris;
}

{
  Object o=context.getAttribute(MULTI_THREADED);
  if (o instanceof Boolean) {
    if (((Boolean)o).booleanValue())     return true;
  }
  o=context.getServer().getAttribute(MULTI_THREADED);
  if (o instanceof Boolean) {
    if (((Boolean)o).booleanValue())     return true;
  }
  return Boolean.getBoolean(MULTI_THREADED);
}

{
  if (contains(o))   return false;
  Bean b=new Bean(o);
  b._joined=joined;
  _beans.add(b);
  if (o instanceof LifeCycle) {
    LifeCycle l=(LifeCycle)o;
    if (joined && isRunning()) {
      try {
        l.start();
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
  }
  return true;
}

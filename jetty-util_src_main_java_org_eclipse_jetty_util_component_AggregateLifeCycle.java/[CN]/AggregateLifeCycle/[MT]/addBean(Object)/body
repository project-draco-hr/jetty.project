{
  if (contains(o))   return false;
  Bean b=new Bean(o);
  _beans.add(b);
  if (o instanceof LifeCycle) {
    LifeCycle l=(LifeCycle)o;
    if (l.isRunning())     b._joined=false;
 else     if (isRunning()) {
      try {
        l.start();
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
  }
  return true;
}

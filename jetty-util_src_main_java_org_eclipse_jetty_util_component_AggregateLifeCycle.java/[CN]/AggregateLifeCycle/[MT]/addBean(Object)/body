{
  if (o == null)   return false;
  boolean added=false;
  if (!_dependentBeans.contains(o)) {
    _dependentBeans.add(o);
    added=true;
  }
  try {
    if (isStarted() && o instanceof LifeCycle)     ((LifeCycle)o).start();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return added;
}

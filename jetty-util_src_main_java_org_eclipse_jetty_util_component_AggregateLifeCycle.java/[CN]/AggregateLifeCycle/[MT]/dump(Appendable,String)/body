{
  dumpThis(out);
  int size=_beans.size();
  if (size == 0)   return;
  int i=0;
  for (  Bean b : _beans) {
    i++;
    if (b._managed) {
      out.append(indent).append(" +- ");
      if (b._bean instanceof Dumpable)       ((Dumpable)b._bean).dump(out,indent + (i == size ? "    " : " |  "));
 else       dumpObject(out,b._bean);
    }
 else     dumpObject(out,b._bean);
  }
  if (i != size)   out.append(indent).append(" |\n");
}

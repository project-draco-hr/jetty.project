{
  Iterator<?> iter=_dependentBeans.iterator();
  T t=null;
  int count=0;
  while (iter.hasNext()) {
    Object o=iter.next();
    if (clazz.isInstance(o)) {
      count++;
      if (t == null)       t=(T)o;
    }
  }
  if (count > 1 && LOG.isDebugEnabled())   LOG.debug("getBean({}) 1 of {}",clazz.getName(),count);
  return t;
}

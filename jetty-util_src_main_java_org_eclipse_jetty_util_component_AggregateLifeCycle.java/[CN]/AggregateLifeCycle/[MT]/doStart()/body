{
  for (  Bean b : _beans) {
    if (b._bean instanceof LifeCycle) {
      LifeCycle l=(LifeCycle)b._bean;
      if (!l.isRunning()) {
        if (b._managed)         l.start();
      }
    }
    if (b._managed && b._bean instanceof LifeCycle) {
      LifeCycle l=(LifeCycle)b._bean;
      if (!l.isRunning())       l.start();
    }
  }
  _started=true;
  super.doStart();
}

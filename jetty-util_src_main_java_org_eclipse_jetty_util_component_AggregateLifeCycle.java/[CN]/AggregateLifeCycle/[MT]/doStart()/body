{
  _started=true;
  for (  Bean b : _beans) {
    if (b._bean instanceof LifeCycle) {
      LifeCycle l=(LifeCycle)b._bean;
switch (b._managed) {
case MANAGED:
        if (!l.isRunning())         l.start();
      break;
case AUTO:
    if (l.isRunning())     b._managed=Managed.UNMANAGED;
 else {
      b._managed=Managed.MANAGED;
      l.start();
    }
  break;
}
}
}
super.doStart();
}

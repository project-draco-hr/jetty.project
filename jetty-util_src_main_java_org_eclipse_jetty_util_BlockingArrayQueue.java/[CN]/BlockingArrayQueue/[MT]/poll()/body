{
  if (_size.get() == 0)   return null;
  E e=null;
  _headLock.lock();
  try {
    if (_size.get() > 0) {
      final int head=_head;
      e=(E)_elements[head];
      _head=(head + 1) % _elements.length;
      if (_size.decrementAndGet() > 0)       _notEmpty.signal();
    }
  }
  finally {
    _headLock.unlock();
  }
  return e;
}

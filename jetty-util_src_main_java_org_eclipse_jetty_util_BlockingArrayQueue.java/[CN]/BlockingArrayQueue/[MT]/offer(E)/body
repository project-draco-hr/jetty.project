{
  Objects.requireNonNull(e);
  final Lock tailLock=_tailLock;
  final Lock headLock=_headLock;
  boolean notEmpty=false;
  tailLock.lock();
  try {
    int size=_size.get();
    if (size >= _limit)     return false;
    if (size == _capacity) {
      headLock.lock();
      try {
        if (!grow())         return false;
      }
  finally {
        headLock.unlock();
      }
    }
    int tail=_tail;
    _elements[tail]=e;
    _tail=(tail + 1) % _capacity;
    notEmpty=_size.getAndIncrement() == 0;
  }
  finally {
    tailLock.unlock();
  }
  if (notEmpty) {
    headLock.lock();
    try {
      _notEmpty.signal();
    }
  finally {
      headLock.unlock();
    }
  }
  return true;
}

{
  try {
    for (int i=0; i < loops; i++) {
      _loops[thread]=i;
      doPaths(name + "-" + i);
      Thread.sleep(_random.nextInt(100));
    }
    _loops[thread]=loops;
  }
 catch (  Exception e) {
    System.err.println(e);
    _connector.dump();
    _loops[thread]=-_loops[thread];
    throw e;
  }
}

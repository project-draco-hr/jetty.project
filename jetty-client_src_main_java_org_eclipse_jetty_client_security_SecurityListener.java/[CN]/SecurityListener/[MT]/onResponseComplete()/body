{
  _responseComplete=true;
  if (_needIntercept) {
    if (_requestComplete && _responseComplete) {
      if (LOG.isDebugEnabled())       LOG.debug("onResponseComplete, Both complete: Resending from onResponseComplete" + _exchange);
      _responseComplete=false;
      _requestComplete=false;
      setDelegatingResponses(true);
      setDelegatingRequests(true);
      _destination.resend(_exchange);
    }
 else {
      if (LOG.isDebugEnabled())       LOG.debug("onResponseComplete, Request not yet complete from onResponseComplete,  calling super " + _exchange);
      super.onResponseComplete();
    }
  }
 else {
    if (LOG.isDebugEnabled())     LOG.debug("OnResponseComplete, delegating to super with Request complete=" + _requestComplete + ", response complete="+ _responseComplete+ " "+ _exchange);
    super.onResponseComplete();
  }
}

{
  if (MultiPartInputStreamParser.this._config.getMaxFileSize() > 0 && _size + length > MultiPartInputStreamParser.this._config.getMaxFileSize())   throw new IllegalStateException("Multipart Mime part " + _name + " exceeds max filesize");
  if (_file == null && MultiPartInputStreamParser.this.isFileNeeded(_size + length))   createFile();
  _out.write(bytes,offset,length);
  _size+=length;
}

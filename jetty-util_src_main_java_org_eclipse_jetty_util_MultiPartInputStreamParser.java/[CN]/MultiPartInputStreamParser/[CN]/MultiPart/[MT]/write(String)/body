{
  if (_file == null) {
    _temporary=false;
    _file=new File(_tmpDir,fileName);
    BufferedOutputStream bos=null;
    try {
      bos=new BufferedOutputStream(new FileOutputStream(_file));
      _bout.writeTo(bos);
      bos.flush();
    }
  finally {
      if (bos != null)       bos.close();
      _bout=null;
    }
  }
 else {
    _temporary=false;
    File f=new File(_tmpDir,fileName);
    if (_file.renameTo(f))     _file=f;
 else     throw new IOException("Part rename failure: from " + _file.getName() + " to "+ fileName+ " in "+ _tmpDir.getAbsolutePath());
  }
}

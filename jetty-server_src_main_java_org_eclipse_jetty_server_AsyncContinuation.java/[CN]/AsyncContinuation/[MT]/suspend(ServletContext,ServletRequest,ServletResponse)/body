{
synchronized (this) {
    HttpServletRequest r=(HttpServletRequest)request;
    System.err.printf("Suspend %s %s | %s %s%n",r.getServletPath(),r.getPathInfo(),r.getAttribute(Dispatcher.FORWARD_SERVLET_PATH),r.getAttribute(Dispatcher.FORWARD_PATH_INFO));
    doSuspend(context,request,response);
    if (request instanceof HttpServletRequest)     _event._pathInContext=URIUtil.addPaths(((HttpServletRequest)request).getServletPath(),((HttpServletRequest)request).getPathInfo());
 else     _event._pathInContext=null;
  }
}

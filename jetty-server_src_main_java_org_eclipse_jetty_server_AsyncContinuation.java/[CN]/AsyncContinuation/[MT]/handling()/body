{
synchronized (this) {
    _responseWrapped=false;
switch (_state) {
case __DISPATCHED:
case __REDISPATCHED:
case __COMPLETED:
      throw new IllegalStateException(this.getStatusString());
case __IDLE:
    _initial=true;
  _state=__DISPATCHED;
return true;
case __ASYNCSTARTED:
case __REDISPATCHING:
throw new IllegalStateException(this.getStatusString());
case __COMPLETING:
_state=__UNCOMPLETED;
return false;
case __ASYNCWAIT:
return false;
case __REDISPATCH:
_state=__REDISPATCHED;
return true;
default :
throw new IllegalStateException("" + _state);
}
}
}

{
synchronized (this) {
    return ((_state == __IDLE) ? "IDLE" : (_state == __DISPATCHED) ? "DISPATCHED" : (_state == __SUSPENDING) ? "SUSPENDING" : (_state == __SUSPENDED) ? "SUSPENDED" : (_state == __REDISPATCHING) ? "REDISPATCHING" : (_state == __UNSUSPENDING) ? "UNSUSPENDING" : (_state == __REDISPATCHED) ? "REDISPATCHED" : (_state == __COMPLETING) ? "COMPLETING" : (_state == __UNCOMPLETED) ? "UNCOMPLETED" : (_state == __COMPLETE) ? "COMPLETE" : ("UNKNOWN?" + _state)) + (_initial ? ",initial" : "") + (_resumed ? ",resumed" : "")+ (_expired ? ",expired" : "");
  }
}

{
  for (int i=0; i < 2; i++) {
    String content="This is a loooooooooooooooooooooooooooooooooo" + "ooooooooooooooooooooooooooooooooooooooooooooo" + "ooooooooooooooooooooooooooooooooooooooooooooo"+ "ooooooooooooooooooooooooooooooooooooooooooooo"+ "ooooooooooooooooooooooooooooooooooooooooooooo"+ "ooooooooooooooooooooooooooooooooooooooooooooo"+ "ooooooooooooooooooooooooooooooooooooooooooooo"+ "ooooooooooooooooooooooooooooooooooooooooooooo"+ "ooooooooooooooooooooooooooooooooooooooooooooo"+ "ooooooooooooooooooooooonnnnnnnnnnnnnnnnggggggggg content";
    byte[] bytes=content.getBytes();
    _outputStream.write("GET / HTTP/1.1\r\nHost: localhost\r\n".getBytes());
    Thread.currentThread();
    Thread.sleep(500L);
    String str="Content-Length: " + bytes.length + "\r\n"+ "\r\n";
    _outputStream.write(str.getBytes());
    Thread.currentThread();
    Thread.sleep(500L);
    _outputStream.write(bytes,0,(bytes.length / 2));
    Thread.currentThread();
    Thread.sleep(1000L);
    _outputStream.write(bytes,bytes.length / 2,(bytes.length - bytes.length / 2));
  }
  byte[] inbuf=new byte[__OK_RESPONSE.getBytes().length * 2];

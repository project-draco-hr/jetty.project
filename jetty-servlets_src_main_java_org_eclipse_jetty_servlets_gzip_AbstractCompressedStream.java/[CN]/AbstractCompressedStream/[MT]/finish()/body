{
  if (!_closed) {
    if (_out == null || _bOut != null) {
      long length=_wrapper.getContentLength();
      if (length > 0 && length < _wrapper.getMinCompressSize())       doNotCompress();
 else       doCompress();
    }
    if (_compressedOutputStream != null && !_closed) {
      _closed=true;
      _compressedOutputStream.close();
    }
  }
}

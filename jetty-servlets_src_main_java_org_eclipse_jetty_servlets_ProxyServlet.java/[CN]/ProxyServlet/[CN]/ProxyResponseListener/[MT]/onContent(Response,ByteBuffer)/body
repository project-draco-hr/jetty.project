{
  byte[] buffer;
  int offset;
  int length=content.remaining();
  if (content.hasArray()) {
    buffer=content.array();
    offset=content.arrayOffset();
  }
 else {
    buffer=new byte[length];
    content.get(buffer);
    offset=0;
  }
  try {
    onResponseContent(request,response,proxyResponse,buffer,offset,length);
  }
 catch (  IOException x) {
    proxyResponse.abort(x);
  }
}

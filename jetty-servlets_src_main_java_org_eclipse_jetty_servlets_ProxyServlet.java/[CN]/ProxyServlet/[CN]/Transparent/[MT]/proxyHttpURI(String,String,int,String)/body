{
  try {
    if (!uri.startsWith(_prefix))     return null;
    URI dstUri=new URI(_proxyTo + uri.substring(_prefix.length())).normalize();
    if (!validateDestination(dstUri.getHost(),dstUri.getPath()))     return null;
    return new HttpURI(dstUri.toString());
  }
 catch (  URISyntaxException ex) {
    throw new MalformedURLException(ex.getMessage());
  }
}

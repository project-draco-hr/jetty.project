{
  if (config.getInitParameter("ProxyTo") != null)   _proxyTo=config.getInitParameter("ProxyTo");
  if (config.getInitParameter("Prefix") != null)   _prefix=config.getInitParameter("Prefix");
  if (_proxyTo == null)   throw new UnavailableException("No ProxyTo");
  super.init(config);
  _log.info(_name + " @ " + (_prefix == null ? "-" : _prefix)+ " to "+ _proxyTo);
}

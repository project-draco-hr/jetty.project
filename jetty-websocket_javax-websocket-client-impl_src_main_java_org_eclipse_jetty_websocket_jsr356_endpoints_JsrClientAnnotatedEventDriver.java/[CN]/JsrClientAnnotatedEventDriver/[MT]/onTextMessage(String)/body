{
  if (events.onText == null) {
    return;
  }
  Decoder.Text<?> decoder=(Text<?>)events.onText.getDecoder();
  try {
    decoder.init(endpointconfig);
    events.onText.call(websocket,jsrsession,decoder.decode(message));
  }
 catch (  DecodeException e) {
    onFatalError(e);
  }
 finally {
    decoder.destroy();
  }
}

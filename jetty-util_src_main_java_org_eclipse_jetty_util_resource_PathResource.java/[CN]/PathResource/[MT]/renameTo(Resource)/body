{
  if (dest instanceof PathResource) {
    PathResource destRes=(PathResource)dest;
    try {
      Path result=Files.move(path,destRes.path,StandardCopyOption.ATOMIC_MOVE);
      return Files.exists(result,linkOptions);
    }
 catch (    IOException e) {
      LOG.ignore(e);
      return false;
    }
  }
 else {
    return false;
  }
}

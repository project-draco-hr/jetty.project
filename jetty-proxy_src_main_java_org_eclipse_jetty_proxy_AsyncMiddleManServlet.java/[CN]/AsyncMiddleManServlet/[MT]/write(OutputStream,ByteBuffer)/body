{
  int length=content.remaining();
  if (length <= 0) {
    return false;
  }
  int offset=0;
  byte[] buffer;
  if (content.hasArray()) {
    offset=content.arrayOffset();
    buffer=content.array();
  }
 else {
    buffer=new byte[length];
    content.get(buffer);
  }
  output.write(buffer,offset,length);
  return (length > 0);
}

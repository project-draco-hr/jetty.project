{
  while (isOpen()) {
    try {
      if (!_closed.await(10,TimeUnit.SECONDS)) {
        System.err.println("wait timeout:\n--");
        System.err.println(takeOutputString());
        System.err.println("==");
        break;
      }
    }
 catch (    Exception e) {
      LOG.warn(e);
    }
  }
}

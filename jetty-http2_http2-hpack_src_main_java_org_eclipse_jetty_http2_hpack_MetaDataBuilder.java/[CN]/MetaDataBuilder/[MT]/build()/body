{
  try {
    HttpFields fields=_fields;
    _fields=new HttpFields(Math.max(10,fields.size() + 5));
    if (_method != null)     return new MetaData.Request(_scheme,_method,_authority,_host,_port,_path,fields);
    if (_status != 0)     return new MetaData.Response(_status,fields);
    return new MetaData(fields);
  }
  finally {
    _status=0;
    _method=null;
    _scheme=null;
    _authority=null;
    _path=null;
    _host=null;
    _port=0;
  }
}

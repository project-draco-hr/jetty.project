{
  super(new AggregateEventTrigger(),notifier,pollInterval);
  _url=url;
  _uuid=uuid;
  _appid=appid;
  _client=new HttpClient();
  _client.setTimeout(60000);
  _client.setConnectorType(HttpClient.CONNECTOR_SELECT_CHANNEL);
  try {
    _client.start();
    _srvip=getServerIP();
  }
 catch (  Exception ex) {
    Log.debug(ex);
  }
  sendData(new Properties());
}

{
  super(new AggregateEventTrigger(),notifier,pollInterval);
  _url=url;
  _uuid=uuid;
  _appid=appid;
  _client=new HttpClient();
  _client.setConnectTimeout(2000);
  _client.setIdleTimeout(30000);
  try {
    _client.start();
    _srvip=getServerIP();
  }
 catch (  Exception ex) {
    LOG.debug(ex);
  }
  sendData(new Properties());
}

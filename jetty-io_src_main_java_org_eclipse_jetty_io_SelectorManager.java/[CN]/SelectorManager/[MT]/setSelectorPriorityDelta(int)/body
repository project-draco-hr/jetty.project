{
  int old=_priorityDelta;
  _priorityDelta=selectorPriorityDelta;
  if (old != selectorPriorityDelta && isStarted()) {
    for (    ManagedSelector selector : _selectors)     if (selector._thread != null)     selector._thread.setPriority(Math.max(Thread.MIN_PRIORITY,Math.min(Thread.MAX_PRIORITY,selector._thread.getPriority() - old + selectorPriorityDelta)));
  }
}

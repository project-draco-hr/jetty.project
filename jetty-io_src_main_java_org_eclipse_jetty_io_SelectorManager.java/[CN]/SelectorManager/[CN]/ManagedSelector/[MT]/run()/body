{
  _thread=Thread.currentThread();
  String name=_thread.getName();
  try {
    _thread.setName(name + " Selector" + _id);
    LOG.debug("Starting {} on {}",_thread,this);
    while (isRunning()) {
      try {
        select();
      }
 catch (      IOException e) {
        LOG.warn(e);
      }
    }
    processChanges();
  }
  finally {
    LOG.debug("Stopped {} on {}",_thread,this);
    _thread.setName(name);
  }
}

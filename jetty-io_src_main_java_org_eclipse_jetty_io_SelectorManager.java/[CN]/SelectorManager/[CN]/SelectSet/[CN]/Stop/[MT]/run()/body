{
  try {
    for (    SelectionKey key : _selector.keys()) {
      Object attachment=key.attachment();
      if (attachment instanceof EndPoint) {
        EndPoint endpoint=(EndPoint)attachment;
        endpoint.close();
      }
    }
    _selector.close();
  }
 catch (  IOException x) {
    LOG.ignore(x);
  }
 finally {
    latch.countDown();
  }
}

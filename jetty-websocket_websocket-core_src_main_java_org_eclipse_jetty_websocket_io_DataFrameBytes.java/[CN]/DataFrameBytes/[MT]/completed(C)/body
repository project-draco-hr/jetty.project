{
  if (LOG.isDebugEnabled()) {
    LOG.debug("completed({}) - frame.remaining() = {}",context,frame.remaining());
  }
  connection.getBufferPool().release(buffer);
  if (frame.remaining() > 0) {
    connection.getQueue().prepend(this);
  }
 else {
    super.completed(context);
  }
  connection.flush();
}

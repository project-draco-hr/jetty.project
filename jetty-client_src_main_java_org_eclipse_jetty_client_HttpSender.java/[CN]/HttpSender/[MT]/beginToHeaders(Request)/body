{
  if (!updateRequestState(RequestState.BEGIN,RequestState.HEADERS))   return false;
  if (LOG.isDebugEnabled())   LOG.debug("Request headers {}{}{}",request,System.getProperty("line.separator"),request.getHeaders().toString().trim());
  RequestNotifier notifier=getHttpChannel().getHttpDestination().getRequestNotifier();
  notifier.notifyHeaders(request);
  return true;
}

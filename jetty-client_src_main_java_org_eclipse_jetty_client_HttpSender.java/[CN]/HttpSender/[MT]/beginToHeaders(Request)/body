{
  if (!updateRequestState(RequestState.BEGIN,RequestState.TRANSIENT))   return false;
  if (LOG.isDebugEnabled())   LOG.debug("Request headers {}{}{}",request,System.lineSeparator(),request.getHeaders().toString().trim());
  RequestNotifier notifier=getHttpChannel().getHttpDestination().getRequestNotifier();
  notifier.notifyHeaders(request);
  if (!updateRequestState(RequestState.TRANSIENT,RequestState.HEADERS))   terminateRequest(getHttpExchange(),failure);
  return true;
}

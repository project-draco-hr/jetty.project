{
  RequestState current=requestState.get();
switch (current) {
case COMMIT:
case CONTENT:
{
      if (!updateRequestState(current,RequestState.TRANSIENT_CONTENT))       return false;
      if (LOG.isDebugEnabled())       LOG.debug("Request content {}{}{}",request,System.lineSeparator(),BufferUtil.toDetailString(content));
      RequestNotifier notifier=getHttpChannel().getHttpDestination().getRequestNotifier();
      notifier.notifyContent(request,content);
      if (!updateRequestState(RequestState.TRANSIENT_CONTENT,RequestState.CONTENT))       terminateRequest(getHttpExchange(),failure);
      return true;
    }
default :
{
    return false;
  }
}
}

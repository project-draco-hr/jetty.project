{
  if (!updateRequestState(RequestState.QUEUED,RequestState.TRANSIENT))   return false;
  if (LOG.isDebugEnabled())   LOG.debug("Request begin {}",request);
  RequestNotifier notifier=getHttpChannel().getHttpDestination().getRequestNotifier();
  notifier.notifyBegin(request);
  if (!updateRequestState(RequestState.TRANSIENT,RequestState.BEGIN))   terminateRequest(getHttpExchange(),failure,false);
  return true;
}

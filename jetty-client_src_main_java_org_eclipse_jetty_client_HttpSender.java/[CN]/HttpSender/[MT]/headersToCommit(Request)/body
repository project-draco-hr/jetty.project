{
  if (!updateRequestState(RequestState.HEADERS,RequestState.TRANSIENT))   return false;
  if (LOG.isDebugEnabled())   LOG.debug("Request committed {}",request);
  RequestNotifier notifier=getHttpChannel().getHttpDestination().getRequestNotifier();
  notifier.notifyCommit(request);
  if (!updateRequestState(RequestState.TRANSIENT,RequestState.COMMIT))   terminateRequest(getHttpExchange(),failure,true);
  return true;
}

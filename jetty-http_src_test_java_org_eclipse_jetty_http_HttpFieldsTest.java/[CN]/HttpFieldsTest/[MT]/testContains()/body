{
  HttpFields header=new HttpFields();
  header.add("n0","");
  header.add("n1",",");
  header.add("n2",",,");
  header.add("N3","abc");
  header.add("N4","def");
  header.add("n5","abc,def,hig");
  header.add("N6","abc");
  header.add("n6","def");
  header.add("N6","hig");
  header.add("n7","abc ,  def;q=0.9  ,  hig");
  for (int i=0; i < 8; i++) {
    assertFalse("" + i,header.contains("n" + i,"xyz"));
    assertEquals("" + i,i >= 4,header.contains("n" + i,"def"));
  }
}

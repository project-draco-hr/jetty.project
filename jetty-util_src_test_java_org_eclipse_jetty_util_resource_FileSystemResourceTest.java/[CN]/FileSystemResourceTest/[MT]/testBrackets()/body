{
  File dir=testdir.getDir();
  try {
    Path foo=new File(dir,"foo[1]").toPath();
    Files.createFile(foo);
  }
 catch (  Exception e) {
    assumeNoException(e);
  }
  try (Resource base=newResource(testdir.getDir())){
    Resource res=base.addPath("foo[1]");
    assertThat("Alias: " + res,res.getAlias(),nullValue());
  }
 }

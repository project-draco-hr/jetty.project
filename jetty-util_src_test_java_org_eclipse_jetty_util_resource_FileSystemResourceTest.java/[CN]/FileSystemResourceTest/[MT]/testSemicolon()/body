{
  Path dir=testdir.getDir().toPath().normalize().toRealPath();
  try {
    Path foo=dir.resolve("foo;");
    Files.createFile(foo);
  }
 catch (  Exception e) {
    assumeNoException(e);
  }
  try (Resource base=newResource(dir.toFile())){
    Resource res=base.addPath("foo;");
    assertThat("Alias: " + res,res,hasNoAlias());
  }
 }

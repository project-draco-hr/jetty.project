{
  System.out.printf("Got connect: %s%n",conn);
  this.conn=conn;
  try {
    Future<WriteResult> fut;
    fut=conn.write("Hello");
    fut.get(2,TimeUnit.SECONDS);
    fut=conn.write("Thanks for the conversation.");
    fut.get(2,TimeUnit.SECONDS);
    conn.close(StatusCode.NORMAL,"I'm done");
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}

{
  _started=true;
  for (  Bean b : _beans) {
    if (b._bean instanceof LifeCycle) {
      LifeCycle l=(LifeCycle)b._bean;
switch (b._managed) {
case MANAGED:
        if (!l.isRunning())         start(l);
      break;
case AUTO:
    if (l.isRunning())     unmanage(b);
 else {
      manage(b);
      start(l);
    }
  break;
}
}
}
super.doStart();
}

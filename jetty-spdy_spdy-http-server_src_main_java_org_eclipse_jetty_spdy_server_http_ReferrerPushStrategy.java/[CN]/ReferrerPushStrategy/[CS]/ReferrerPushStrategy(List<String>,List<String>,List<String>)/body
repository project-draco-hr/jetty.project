{
  for (  String pushRegexp : pushRegexps)   this.pushRegexps.add(Pattern.compile(pushRegexp));
  this.pushContentTypes.addAll(pushContentTypes);
  for (  String allowedPushOrigin : allowedPushOrigins)   this.allowedPushOrigins.add(Pattern.compile(allowedPushOrigin.replace(".","\\.").replace("*",".*")));
}

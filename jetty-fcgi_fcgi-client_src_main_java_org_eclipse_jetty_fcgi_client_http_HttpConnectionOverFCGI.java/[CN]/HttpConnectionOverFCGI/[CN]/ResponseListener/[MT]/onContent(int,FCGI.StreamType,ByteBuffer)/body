{
switch (stream) {
case STD_OUT:
{
      HttpChannelOverFCGI channel=channels.get(request);
      if (channel != null)       channel.content(buffer);
 else       noChannel(request);
      break;
    }
case STD_ERR:
{
    LOG.info(BufferUtil.toUTF8String(buffer));
    break;
  }
default :
{
  throw new IllegalArgumentException();
}
}
}

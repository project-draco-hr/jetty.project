{
  EndPoint endPoint=getEndPoint();
  ByteBuffer buffer=this.buffer;
  while (true) {
    try {
      if (parse(buffer))       return false;
      int read=endPoint.fill(buffer);
      if (LOG.isDebugEnabled())       LOG.debug("Read {} bytes from {}",read,endPoint);
      if (read > 0) {
        if (parse(buffer))         return false;
      }
 else       if (read == 0) {
        fillInterested();
        return true;
      }
 else {
        shutdown();
        return true;
      }
    }
 catch (    Exception x) {
      LOG.debug(x);
      close(x);
      return false;
    }
  }
}

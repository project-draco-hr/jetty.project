{
  int o=s.indexOf(c,offset);
  if (o < offset) {
    System.err.println("FAILED: " + test);
    System.err.println("'" + c + "' not in:");
    System.err.println(s.substring(offset));
    System.err.flush();
    System.out.println("--\n" + s);
    System.out.flush();
    assertTrue(test,false);
  }
  return o;
}

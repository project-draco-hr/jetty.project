{
  int uid=SetUID.getuid();
  if (uid != 0) {
    Log.warn("Expected to be running UID = 0, but got UID = " + uid);
    return;
  }
  if (_gid != 0) {
    Log.info("Setting GID=" + _gid);
    SetUID.setgid(_gid);
  }
  if (_uid != 0) {
    Log.info("Setting UID=" + _uid);
    SetUID.setuid(_uid);
  }
}

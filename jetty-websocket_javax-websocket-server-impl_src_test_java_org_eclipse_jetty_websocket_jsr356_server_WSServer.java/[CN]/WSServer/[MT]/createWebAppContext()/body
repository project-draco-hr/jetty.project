{
  WebAppContext context=new WebAppContext();
  context.setContextPath(this.contextPath);
  context.setWar(this.contextDir.getAbsolutePath());
  context.setConfigurations(new Configuration[]{new WebSocketConfiguration(),new AnnotationConfiguration(),new WebXmlConfiguration(),new WebInfConfiguration(),new PlusConfiguration(),new MetaInfConfiguration(),new FragmentConfiguration(),new EnvConfiguration()});
  return context;
}

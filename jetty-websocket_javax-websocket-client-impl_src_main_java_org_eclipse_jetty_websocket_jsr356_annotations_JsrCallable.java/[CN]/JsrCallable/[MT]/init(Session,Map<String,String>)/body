{
  idxSession=findIndexForRole(Param.Role.SESSION);
  if (idxSession >= 0) {
    args[idxSession]=session;
  }
  if (pathParams != null) {
    for (    Param param : params) {
      if (param.role == Role.PATH_PARAM) {
        int idx=param.index;
        String value=pathParams.get(param.getPathParamName());
        args[idx]=value;
      }
    }
  }
}

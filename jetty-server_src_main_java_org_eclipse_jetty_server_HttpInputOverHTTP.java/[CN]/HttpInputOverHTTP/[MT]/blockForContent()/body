{
  while (true) {
    try (Blocker blocker=_readBlocker.acquire()){
      _httpConnection.fillInterested(blocker);
      if (LOG.isDebugEnabled())       LOG.debug("{} block readable on {}",this,blocker);
      blocker.block();
    }
     Object content=getNextContent();
    if (content != null || isFinished())     break;
  }
}

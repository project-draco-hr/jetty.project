{
  if (HttpHeader.CONTENT_TYPE == name)   setContentType(value);
 else {
    if (_connection.isIncluding())     return;
    _connection.getResponseFields().put(name,value);
    if (HttpHeader.CONTENT_LENGTH == name) {
      if (value == null)       _connection._generator.setContentLength(-1);
 else       _connection._generator.setContentLength(Long.parseLong(value));
    }
  }
}

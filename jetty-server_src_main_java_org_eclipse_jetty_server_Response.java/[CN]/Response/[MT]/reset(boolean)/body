{
  if (!preserveCookies)   reset();
 else {
    ArrayList<String> cookieValues=new ArrayList<String>(5);
    Enumeration<String> vals=_fields.getValues(HttpHeader.SET_COOKIE.asString());
    while (vals.hasMoreElements())     cookieValues.add(vals.nextElement());
    reset();
    for (    String v : cookieValues)     _fields.add(HttpHeader.SET_COOKIE,v);
  }
}

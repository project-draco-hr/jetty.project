{
  String comment=cookie.getComment();
  boolean http_only=false;
  if (comment != null) {
    int i=comment.indexOf(HTTP_ONLY_COMMENT);
    if (i >= 0) {
      http_only=true;
      comment=comment.substring(i,i + HTTP_ONLY_COMMENT.length()).trim();
      if (comment.length() == 0)       comment=null;
    }
  }
  _connection.getResponseFields().addSetCookie(cookie.getName(),cookie.getValue(),cookie.getDomain(),cookie.getPath(),cookie.getMaxAge(),comment,cookie.getSecure(),http_only,cookie.getVersion());
}

{
  if (_compressedOutputStream == null) {
    if (_response.isCommitted())     throw new IllegalStateException();
    if (setContentEncoding()) {
      _out=_compressedOutputStream=createStream();
      if (_bOut != null) {
        _out.write(_bOut.getBuf(),0,_bOut.getCount());
        _bOut=null;
      }
    }
 else     doNotCompress();
  }
}

{
  if (_unpackedOverlayResources != null && !_unpackedOverlayResources.isEmpty()) {
    try {
      for (      Resource r : _unpackedOverlayResources) {
        IO.delete(r.getFile());
      }
    }
 catch (    IOException e) {
      LOG.ignore(e);
    }
  }
  super.deconfigure(context);
  context.setBaseResource(_originalResourceBase);
}

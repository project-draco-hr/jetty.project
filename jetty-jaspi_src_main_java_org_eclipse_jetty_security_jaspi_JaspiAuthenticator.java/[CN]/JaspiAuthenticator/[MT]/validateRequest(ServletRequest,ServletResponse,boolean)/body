{
  System.err.println("JaspiAuthenticator.validateRequest, uri=" + ((javax.servlet.http.HttpServletRequest)request).getRequestURI() + "lazy="+ _allowLazyAuthentication+ " mandatory="+ mandatory);
  new Throwable().printStackTrace();
  JaspiMessageInfo info=new JaspiMessageInfo(request,response,mandatory);
  request.setAttribute("org.eclipse.jetty.security.jaspi.info",info);
  return validateRequest(info);
}

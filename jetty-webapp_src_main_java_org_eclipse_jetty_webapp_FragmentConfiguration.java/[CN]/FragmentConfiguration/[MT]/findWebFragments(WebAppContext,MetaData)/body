{
  @SuppressWarnings("unchecked") Map<Resource,Resource> frags=(Map<Resource,Resource>)context.getAttribute(FRAGMENT_RESOURCES);
  if (frags != null) {
    for (    Resource key : frags.keySet()) {
      if (key.isDirectory()) {
        metaData.addFragment(key,frags.get(key));
      }
 else {
        metaData.addFragment(key,frags.get(key));
      }
    }
  }
}

{
  Log.debug("Scanning WEB-INF/lib jars");
  parseAnnotationsFromJars(context,finder,context.getClassLoader(),context.getInitParameter(__web_inf_pattern),false,true,new ClassNameResolver(){
    public boolean isExcluded(    String name){
      if (context.isSystemClass(name))       return true;
      if (context.isServerClass(name))       return false;
      return false;
    }
    public boolean shouldOverride(    String name){
      if (context.isParentLoaderPriority())       return false;
      return true;
    }
  }
);
}

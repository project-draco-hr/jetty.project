{
  if (value == null)   throw new IllegalArgumentException("null value");
  Field field=_names.get(header.toString());
  Field last=null;
  while (field != null) {
    last=field;
    field=field._next;
  }
  field=new Field(header,value);
  _fields.add(field);
  if (last != null)   last._next=field;
 else   _names.put(header.toString(),field);
}

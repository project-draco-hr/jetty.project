{
  ServletMapping theMapping=null;
  if (_servletMappings != null) {
    for (    ServletMapping m : _servletMappings) {
      String[] paths=m.getPathSpecs();
      if (paths != null) {
        for (        String path : paths) {
          if (pattern.equals(path))           theMapping=m;
        }
      }
    }
  }
  return theMapping;
}

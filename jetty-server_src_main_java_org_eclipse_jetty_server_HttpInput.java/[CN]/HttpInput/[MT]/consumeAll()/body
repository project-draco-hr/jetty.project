{
synchronized (lock()) {
    if (_onError != null)     return false;
    try {
      while (!isFinished()) {
        Content item=getNextContent();
        if (item == null)         _contentState.waitForContent(this);
 else         consume(item,remaining(item));
      }
      return true;
    }
 catch (    IOException e) {
      LOG.debug(e);
      return false;
    }
  }
}

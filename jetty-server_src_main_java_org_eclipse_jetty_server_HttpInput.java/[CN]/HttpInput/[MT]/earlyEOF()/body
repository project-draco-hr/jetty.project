{
synchronized (_inputQ) {
    if (!isFinished()) {
      if (_inputQ.isEmpty()) {
        if (LOG.isDebugEnabled())         LOG.debug("{} Early EOF",this);
        _state=EARLY_EOF;
      }
 else {
        if (LOG.isDebugEnabled())         LOG.debug("{} Early EOF pending",this);
        _inputQ.add(EARLY_EOF_CONTENT);
      }
      if (_listener == null) {
        _inputQ.notify();
        return;
      }
    }
  }
  onReadPossible();
}

{
  final Throwable error;
  final ReadListener listener;
  boolean aeof=false;
synchronized (_inputQ) {
    if (_state == EOF)     return;
    if (_state == AEOF) {
      _state=EOF;
      aeof=true;
    }
 else     if (!_unready)     return;
    listener=_listener;
    error=_state instanceof ErrorState ? ((ErrorState)_state).getError() : null;
    _unready=false;
  }
  try {
    if (aeof)     listener.onAllDataRead();
 else     if (error == null)     listener.onDataAvailable();
 else     listener.onError(error);
  }
 catch (  Throwable e) {
    LOG.warn(e.toString());
    LOG.debug(e);
    try {
      if (aeof || error == null)       listener.onError(e);
    }
 catch (    Throwable e2) {
      LOG.warn(e2.toString());
      LOG.debug(e2);
      throw new RuntimeIOException(e2);
    }
  }
}

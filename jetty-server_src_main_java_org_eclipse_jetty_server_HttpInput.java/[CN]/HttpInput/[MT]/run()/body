{
  final Throwable error;
  final ReadListener listener;
  boolean aeof=false;
synchronized (_inputQ) {
    if (_state == EOF)     return;
    if (_state == AEOF) {
      _state=EOF;
      aeof=true;
    }
 else     if (!_unready)     return;
    listener=_listener;
    error=_state instanceof ErrorState ? ((ErrorState)_state).getError() : null;
    _unready=false;
  }
  try {
    if (error != null)     listener.onError(error);
 else     if (aeof)     listener.onAllDataRead();
 else {
      listener.onDataAvailable();
synchronized (_inputQ) {
        if (_state == AEOF) {
          _state=EOF;
          aeof=true;
        }
      }
      if (aeof)       listener.onAllDataRead();
    }
  }
 catch (  Throwable e) {
    LOG.warn(e.toString());
    LOG.debug(e);
    listener.onError(e);
  }
}

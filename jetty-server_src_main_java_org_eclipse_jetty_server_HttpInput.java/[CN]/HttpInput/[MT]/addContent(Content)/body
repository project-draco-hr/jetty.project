{
  boolean woken=false;
synchronized (_inputQ) {
    boolean wasEmpty=_inputQ.isEmpty();
    _inputQ.add(item);
    if (LOG.isDebugEnabled())     LOG.debug("{} addContent {}",this,item);
    if (wasEmpty) {
      if (_listener == null)       _inputQ.notify();
 else       woken=_channelState.onReadPossible();
    }
  }
  return woken;
}

{
synchronized (lock()) {
    ByteBuffer content=_inputQ.peekUnsafe();
    ;
    while (content != null) {
      content.clear();
      _inputQ.pollUnsafe();
      onContentConsumed(content);
      content=_inputQ.peekUnsafe();
      if (content == null)       onAllContentConsumed();
    }
    _inputEOF=false;
  }
}

{
  try {
synchronized (_inputQ) {
      if (_listener == null)       return true;
      if (_unready)       return false;
      if (_state instanceof EOFState)       return true;
      if (nextReadable() != null)       return true;
      _unready=true;
    }
    unready();
    return false;
  }
 catch (  IOException e) {
    LOG.ignore(e);
    return true;
  }
}

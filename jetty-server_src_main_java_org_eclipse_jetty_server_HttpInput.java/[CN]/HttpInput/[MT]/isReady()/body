{
synchronized (lock()) {
    if (_listener == null)     return true;
    int available=available();
    if (available > 0)     return true;
    if (!_notReady) {
      _notReady=true;
      if (_state.isEOF())       _channelState.onReadPossible();
 else       unready();
    }
    return false;
  }
}

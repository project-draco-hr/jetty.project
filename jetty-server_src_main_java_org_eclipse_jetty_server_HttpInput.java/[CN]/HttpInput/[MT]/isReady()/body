{
synchronized (_inputQ) {
    if (_listener == null)     return true;
    if (_unready)     return false;
    if (_state instanceof EOFState)     return true;
    if (_inputQ.isEmpty()) {
      try {
        produceContent();
      }
 catch (      IOException e) {
        failed(e);
      }
    }
    if (!_inputQ.isEmpty())     return true;
    _unready=true;
  }
  unready();
  return false;
}

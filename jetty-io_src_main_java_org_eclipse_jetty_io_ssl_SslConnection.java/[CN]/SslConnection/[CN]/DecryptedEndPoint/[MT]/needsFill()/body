{
synchronized (DecryptedEndPoint.this) {
    if (BufferUtil.hasContent(_decryptedInput))     return true;
    if (BufferUtil.isEmpty(_encryptedInput) || _needToFillMoreDataToProgress) {
      if (_fillRequiresFlushToProgress) {
        if (BufferUtil.hasContent(_encryptedOutput)) {
          _cannotAcceptMoreAppDataToFlush=true;
          getEndPoint().write(null,_writeCallback,_encryptedOutput);
        }
 else {
          _fillRequiresFlushToProgress=false;
          return true;
        }
      }
 else       SslConnection.this.fillInterested();
      return false;
    }
 else {
      return true;
    }
  }
}

{
  try {
    super.onOpen();
    _sslEngine.beginHandshake();
    if (_sslEngine.getUseClientMode())     _decryptedEndPoint.write(null,new Callback.Empty<>(),BufferUtil.EMPTY_BUFFER);
    getDecryptedEndPoint().getAsyncConnection().onOpen();
  }
 catch (  SSLException x) {
    getEndPoint().close();
    throw new RuntimeIOException(x);
  }
}

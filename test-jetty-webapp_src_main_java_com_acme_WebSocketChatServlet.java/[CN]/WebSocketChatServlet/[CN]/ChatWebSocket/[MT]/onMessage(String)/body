{
  if (data.indexOf("disconnect") >= 0) {
    try {
      _connection.close();
    }
 catch (    IOException e) {
      LOG.warn(e);
    }
  }
 else {
    for (    ChatWebSocket member : _members) {
      try {
        member._connection.write(null,new FutureCallback<>(),data);
      }
 catch (      IOException e) {
        LOG.warn(e);
      }
    }
  }
}

{
  int strategies[]=new int[]{Deflater.DEFAULT_STRATEGY,Deflater.FILTERED,Deflater.HUFFMAN_ONLY};
  int flushes[]=new int[]{Deflater.FULL_FLUSH,Deflater.NO_FLUSH,Deflater.SYNC_FLUSH};
  byte uncompressed[]=StringUtil.getUtf8Bytes("info:");
  for (int level=0; level <= 9; level++) {
    for (    int strategy : strategies) {
      for (      int flush : flushes) {
        deflate(uncompressed,level,true,strategy,flush);
        deflate(uncompressed,level,false,strategy,flush);
      }
    }
  }
}

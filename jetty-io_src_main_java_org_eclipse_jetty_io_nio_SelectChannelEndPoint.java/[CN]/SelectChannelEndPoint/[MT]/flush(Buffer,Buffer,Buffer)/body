{
  int l=super.flush(header,buffer,trailer);
  if (l == 0 && (header != null && header.hasContent() || buffer != null && buffer.hasContent() || trailer != null && trailer.hasContent())) {
synchronized (this) {
      if (_dispatched)       _writable=false;
    }
  }
 else   if (l > 0) {
    _writable=true;
    notIdle();
  }
  return l;
}

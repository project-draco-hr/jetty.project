{
synchronized (this) {
    return "SCEP@" + hashCode() + "{"+ _socket.getRemoteSocketAddress()+ "->"+ _socket.getLocalSocketAddress()+ (_dispatched ? ",D" : "")+ (isOpen() ? ",open" : "")+ (isInputShutdown() ? ",ishut" : "")+ (isOutputShutdown() ? ",oshut" : "")+ (_readBlocked ? "" : ",RB")+ (_writeBlocked ? "" : ",WB")+ (_writable ? "" : ",!W")+ ",io="+ _interestOps+ ((_key == null || !_key.isValid()) ? "!" : ((_key.isReadable() ? "R" : "") + (_key.isWritable() ? "W" : "")))+ "}";
  }
}

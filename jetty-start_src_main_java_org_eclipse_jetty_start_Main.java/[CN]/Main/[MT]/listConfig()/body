{
  InputStream cfgstream=null;
  try {
    cfgstream=getConfigStream();
    byte[] buf=new byte[4096];
    int len=0;
    while (len >= 0) {
      len=cfgstream.read(buf);
      if (len > 0)       System.out.write(buf,0,len);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.exit(1);
  }
 finally {
    close(cfgstream);
  }
}

{
  if (!ini.exists()) {
    System.err.println("Warning - can't find ini file: " + ini);
    return Collections.emptyList();
  }
  List<String> args=new ArrayList<String>();
  FileReader reader=null;
  BufferedReader buf=null;
  try {
    reader=new FileReader(ini);
    buf=new BufferedReader(reader);
    String arg;
    while ((arg=buf.readLine()) != null) {
      arg=arg.trim();
      if (arg.length() == 0 || arg.startsWith("#")) {
        continue;
      }
      if (arg.endsWith("/")) {
        try {
          File start_d=new File(arg);
          if (!start_d.exists() || !start_d.isDirectory())           start_d=new File(_jettyHome,arg);
          if (start_d.isDirectory()) {
            File[] inis=start_d.listFiles(new FilenameFilter(){
              @Override public boolean accept(              File dir,              String name){
                return name.toLowerCase().endsWith(".ini");
              }
            }
);
            Arrays.sort(inis);
            for (            File i : inis)             args.addAll(loadStartIni(i));
            continue;
          }
        }
 catch (        Exception e) {
          e.printStackTrace();
        }
      }
      args.add(arg);
    }
  }
 catch (  IOException e) {
    usageExit(e,ERR_UNKNOWN);
  }
 finally {
    Main.close(buf);
    Main.close(reader);
  }
  return args;
}

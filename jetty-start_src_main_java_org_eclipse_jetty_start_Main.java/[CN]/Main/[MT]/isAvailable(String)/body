{
  try {
    Class.forName(classname);
    return true;
  }
 catch (  NoClassDefFoundError e) {
    if (DEBUG)     System.err.println(e);
  }
catch (  ClassNotFoundException e) {
    if (DEBUG)     System.err.println(e);
  }
  ClassLoader loader=_classpath.getClassLoader();
  try {
    loader.loadClass(classname);
    return true;
  }
 catch (  NoClassDefFoundError e) {
    if (DEBUG)     System.err.println(e);
  }
catch (  ClassNotFoundException e) {
    if (DEBUG)     System.err.println(e);
  }
  return false;
}

{
  if (path == null)   return path;
  if (path.startsWith(_config.getJettyHome()))   path="${jetty.home}" + path.substring(_config.getJettyHome().length());
  if (_config.getJettyBase() != null && path.startsWith(_config.getJettyBase()))   path="${jetty.base}" + path.substring(_config.getJettyBase().length());
  return path;
}

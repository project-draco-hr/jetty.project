{
  Map<String,RoleInfo> mappings=(Map<String,RoleInfo>)_constraintMap.match(pathInContext);
  if (mappings != null) {
    String httpMethod=request.getMethod();
    RoleInfo roleInfo=mappings.get(httpMethod);
    if (roleInfo == null) {
      roleInfo=mappings.get(null);
      if (roleInfo != null) {
        return roleInfo;
      }
    }
  }
  return null;
}

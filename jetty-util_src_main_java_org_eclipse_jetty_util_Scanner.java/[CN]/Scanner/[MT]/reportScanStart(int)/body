{
  for (  Listener listener : _listeners) {
    try {
      if (listener instanceof ScanCycleListener) {
        ((ScanCycleListener)listener).scanStarted(cycle);
      }
    }
 catch (    Exception e) {
      Log.warn(e);
      Log.warn(listener + " failed on scan start for cycle " + cycle);
    }
  }
}

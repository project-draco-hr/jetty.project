{
  if (_expect100Continue) {
    if (available() == 0) {
      if (_response.isCommitted())       throw new IllegalStateException("Committed before 100 Continues");
      commit(HttpGenerator.CONTINUE_100_INFO,null);
    }
    _expect100Continue=false;
  }
  if (_in == null)   _in=new HttpInput(HttpChannel.this);
  return _in;
}

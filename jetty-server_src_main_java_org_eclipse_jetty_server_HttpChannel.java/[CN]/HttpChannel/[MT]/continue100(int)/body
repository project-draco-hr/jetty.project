{
  if (isExpecting100Continue()) {
    _expect100Continue=false;
    if (available == 0) {
      if (_response.isCommitted())       throw new IOException("Committed before 100 Continues");
      boolean committed=commit(HttpGenerator.CONTINUE_100_INFO,null,false);
      if (!committed)       throw new IOException("Concurrent commit while trying to send 100-Continue");
    }
  }
}

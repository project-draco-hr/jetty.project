{
  if (status < 400 || status > 599)   status=HttpStatus.BAD_REQUEST_400;
  try {
    if (_state.handling()) {
      commitResponse(new ResponseInfo(HttpVersion.HTTP_1_1,new HttpFields(),0,status,reason,false),null,true);
      _state.unhandle();
    }
  }
 catch (  IOException e) {
    LOG.warn(e);
  }
 finally {
    _state.completed();
  }
}

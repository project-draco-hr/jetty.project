{
  long now=System.currentTimeMillis();
  Session session=_converter.sessionFromEntity(e);
  if (session == null)   return;
  Session memSession=_sessions.get(session.getId());
  if (memSession == null)   memSession=_sessions.putIfAbsent(session.getId(),memSession);
  if (memSession.isExpiredAt(now))   memSession.timeout();
}

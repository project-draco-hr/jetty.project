{
  try {
    _semaphore.acquire();
    if (_cause == COMPLETED)     return;
    if (_cause instanceof IOException)     throw (IOException)_cause;
    if (_cause instanceof CancellationException)     throw (CancellationException)_cause;
    throw new IOException(_cause);
  }
 catch (  final InterruptedException e) {
    throw new InterruptedIOException(){
{
        initCause(e);
      }
    }
;
  }
 finally {
    _done.set(false);
    _cause=null;
  }
}

{
  String key;
synchronized (_data) {
    while (true) {
      key=Long.toString(Math.abs(_random.nextLong()),30 + (int)(System.currentTimeMillis() % 7));
      if (!_data.containsKey(key))       break;
    }
    _data.put(key,datum);
  }
  Cookie cookie=new Cookie(_cookieName,key);
  cookie.setPath(_cookiePath);
  response.addCookie(cookie);
}

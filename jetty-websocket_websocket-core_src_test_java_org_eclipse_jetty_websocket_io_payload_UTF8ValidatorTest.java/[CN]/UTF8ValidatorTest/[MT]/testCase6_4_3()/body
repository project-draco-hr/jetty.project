{
  ByteBuffer part1=asByteBuffer("cebae1bdb9cf83cebcceb5");
  ByteBuffer part2=asByteBuffer("f4908080");
  ByteBuffer part3=asByteBuffer("656469746564");
  UTF8Validator validator=new UTF8Validator();
  validator.process(part1);
  try {
    validator.process(part2);
    Assert.fail("Expected a " + BadPayloadException.class);
  }
 catch (  BadPayloadException e) {
  }
  validator.process(part3);
}

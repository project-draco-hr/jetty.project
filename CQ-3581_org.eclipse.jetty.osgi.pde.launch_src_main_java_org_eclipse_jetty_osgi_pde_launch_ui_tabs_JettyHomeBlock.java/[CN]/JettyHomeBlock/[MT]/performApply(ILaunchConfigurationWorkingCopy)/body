{
  super.performApply(configuration);
  String jettyHome=getWorkingDirectoryText();
  if (_initializing || jettyHome == null || jettyHome.length() == 0) {
    _initializing=false;
    return;
  }
  try {
    JettyHomeHelper.setupJettyHomeAndJettyXML(_editedJettyXml,getWorkingDirectoryText(),false);
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}

{
  if (__parser != null)   return;
  __parser=new XmlParser();
  try {
    URL configURL=Loader.getResource(XmlConfiguration.class,"org/eclipse/jetty/xml/configure_6_0.dtd",true);
    __parser.redirectEntity("configure.dtd",configURL);

{
  String id=node.getAttribute("id");
  String name=node.getAttribute("name");
  Object defval=node.getAttribute("default");
  Object prop=null;
  if (_propertyMap != null && _propertyMap.containsKey(name)) {
    prop=_propertyMap.get(name);
  }
 else   if (defval != null)   prop=defval;
  if (id != null)   _idMap.put(id,prop);
  if (prop != null)   configure(prop,node,0);
  return prop;
}

{
  Class<?> oClass=nodeClass(_config);
  String id=_config.getAttribute("id");
  Object obj=id == null ? null : _idMap.get(id);
  if (obj == null && oClass != null)   obj=oClass.newInstance();
  if (oClass != null && !oClass.isInstance(obj))   throw new ClassCastException(oClass.toString());
  configure(obj,_config,0);
  return obj;
}

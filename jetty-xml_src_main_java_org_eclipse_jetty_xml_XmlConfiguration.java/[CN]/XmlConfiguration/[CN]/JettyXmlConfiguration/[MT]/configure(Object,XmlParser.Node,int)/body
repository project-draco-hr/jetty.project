{
  String id=cfg.getAttribute("id");
  if (id != null)   _idMap.put(id,obj);
  for (; i < cfg.size(); i++) {
    Object o=cfg.get(i);
    if (o instanceof String)     continue;
    XmlParser.Node node=(XmlParser.Node)o;
    try {
      String tag=node.getTag();
      if ("Set".equals(tag))       set(obj,node);
 else       if ("Put".equals(tag))       put(obj,node);
 else       if ("Call".equals(tag))       call(obj,node);
 else       if ("Get".equals(tag))       get(obj,node);
 else       if ("New".equals(tag))       newObj(obj,node);
 else       if ("Array".equals(tag))       newArray(obj,node);
 else       if ("Ref".equals(tag))       refObj(obj,node);
 else       if ("Property".equals(tag))       propertyObj(obj,node);
 else       throw new IllegalStateException("Unknown tag: " + tag);
    }
 catch (    Exception e) {
      LOG.warn("Config error at " + node,e.toString());
      throw e;
    }
  }
}

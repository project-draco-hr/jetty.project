{
  String id=cfg.getAttribute("id");
  if (id != null)   _configuration.getIdMap().put(id,obj);
  for (; i < cfg.size(); i++) {
    Object o=cfg.get(i);
    if (o instanceof String)     continue;
    XmlParser.Node node=(XmlParser.Node)o;
    if ("Arg".equals(node.getTag())) {
      LOG.warn("Ignored arg: " + node);
      continue;
    }
    break;
  }
  for (; i < cfg.size(); i++) {
    Object o=cfg.get(i);
    if (o instanceof String)     continue;
    XmlParser.Node node=(XmlParser.Node)o;
    try {
      String tag=node.getTag();
switch (tag) {
case "Set":
        set(obj,node);
      break;
case "Put":
    put(obj,node);
  break;
case "Call":
call(obj,node);
break;
case "Get":
get(obj,node);
break;
case "New":
newObj(obj,node);
break;
case "Array":
newArray(obj,node);
break;
case "Ref":
refObj(obj,node);
break;
case "Property":
propertyObj(node);
break;
default :
throw new IllegalStateException("Unknown tag: " + tag + " in "+ _url);
}
}
 catch (Exception e) {
LOG.warn("Config error at " + node,e.toString() + " in " + _url);
throw e;
}
}
}

{
  String id=node.getAttribute("id");
  String name=node.getAttribute("name");
  String defaultValue=node.getAttribute("default");
  Object prop;
  Map<String,String> property_map=_configuration.getProperties();
  if (property_map != null && property_map.containsKey(name))   prop=property_map.get(name);
 else   prop=defaultValue;
  if (id != null)   _configuration.getIdMap().put(id,prop);
  if (prop != null)   configure(prop,node,0);
  return prop;
}

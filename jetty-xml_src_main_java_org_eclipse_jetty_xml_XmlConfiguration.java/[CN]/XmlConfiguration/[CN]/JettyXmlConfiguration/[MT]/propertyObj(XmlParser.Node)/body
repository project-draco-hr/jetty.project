{
  String idAttr=node.getAttribute("id");
  String nameAttr=node.getAttribute("name");
  String defaultValue=node.getAttribute("default");
  Object value=null;
  Map<String,String> properties=_configuration.getProperties();
  if (properties != null && nameAttr != null)   value=resolve(properties,nameAttr);
  if (value == null && defaultValue != null)   value=interpolate(properties,defaultValue);
  if (idAttr != null)   _configuration.getIdMap().put(idAttr,value);
  if (value != null)   configure(value,node,0);
  return value;
}

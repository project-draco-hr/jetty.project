{
  String id=node.getAttribute("id");
  String name=node.getAttribute("name");
  String defaultValue=node.getAttribute("default");
  Object prop;
  if (_propertyMap != null && _propertyMap.containsKey(name))   prop=_propertyMap.get(name);
 else   prop=defaultValue;
  if (id != null)   _idMap.put(id,prop);
  if (prop != null)   configure(prop,node,0);
  return prop;
}

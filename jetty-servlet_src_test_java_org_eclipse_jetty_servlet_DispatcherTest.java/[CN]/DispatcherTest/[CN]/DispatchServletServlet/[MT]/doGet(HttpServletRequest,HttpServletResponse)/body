{
  RequestDispatcher dispatcher=null;
  if (request.getParameter("include") != null)   dispatcher=getServletContext().getRequestDispatcher(request.getParameter("include"));
 else   if (request.getParameter("forward") != null)   dispatcher=getServletContext().getRequestDispatcher(request.getParameter("forward"));
  dispatcher.forward(new ServletRequestWrapper(request),new ServletResponseWrapper(response));
}

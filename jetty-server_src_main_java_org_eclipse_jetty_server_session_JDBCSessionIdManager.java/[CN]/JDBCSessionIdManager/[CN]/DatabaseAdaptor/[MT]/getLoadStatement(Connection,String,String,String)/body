{
  if (contextPath == null || "".equals(contextPath)) {
    if (isEmptyStringNull()) {
      PreparedStatement statement=connection.prepareStatement("select * from " + _sessionTable + " where sessionId = ? and contextPath is null and virtualHost = ?");
      statement.setString(1,rowId);
      statement.setString(2,virtualHosts);
      return statement;
    }
  }
  PreparedStatement statement=connection.prepareStatement("select * from " + _sessionTable + " where sessionId = ? and contextPath = ? and virtualHost = ?");
  statement.setString(1,rowId);
  statement.setString(2,contextPath);
  statement.setString(3,virtualHosts);
  return statement;
}

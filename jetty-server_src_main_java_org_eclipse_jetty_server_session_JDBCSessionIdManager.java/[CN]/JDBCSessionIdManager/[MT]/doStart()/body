{
  initializeDatabase();
  prepareTables();
  super.doStart();
  if (LOG.isDebugEnabled())   LOG.debug("Scavenging interval = " + getScavengeInterval() + " sec");
  _scheduler=_server.getBean(Scheduler.class);
  if (_scheduler == null) {
    _scheduler=new ScheduledExecutorScheduler();
    _ownScheduler=true;
    _scheduler.start();
  }
  setScavengeInterval(getScavengeInterval());
}

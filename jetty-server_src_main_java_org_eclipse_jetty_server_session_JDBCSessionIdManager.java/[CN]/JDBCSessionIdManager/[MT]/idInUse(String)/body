{
  if (id == null)   return false;
  String clusterId=getClusterId(id);
synchronized (_sessionIds) {
    if (_sessionIds.contains(clusterId))     return true;
    try {
      return exists(clusterId);
    }
 catch (    Exception e) {
      Log.warn("Problem checking inUse for id=" + clusterId,e);
      return false;
    }
  }
}

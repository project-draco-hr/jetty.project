{
  try {
    LOG.debug("onIdleExpired {}ms {} {}",idleForMs,this,_endp);
    _expired=true;
    _endp.close();
  }
 catch (  IOException e) {
    LOG.ignore(e);
    try {
      _endp.close();
    }
 catch (    IOException e2) {
      LOG.ignore(e2);
    }
  }
synchronized (this) {
    this.notifyAll();
  }
}

{
  byte[] response=new byte[4000];
  int n=0;
  for (int i=0; i < response.length && response[n] >= 0; i++) {
    try {
      response[n++]=(byte)sock.getInputStream().read();
    }
 catch (    SocketTimeoutException e) {
      break;
    }
  }
  String sResult=new String(response);
  return sResult;
}

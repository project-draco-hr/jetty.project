{
  LOG.debug("{} Connection State Change: {}",policy.getBehavior(),state);
switch (state) {
case OPEN:
    LOG.debug("fillInterested");
  fillInterested();
break;
case CLOSED:
this.disconnect();
break;
case CLOSING:
CloseInfo close=ioState.getCloseInfo();
outgoingFrame(close.asFrame(),new OnDisconnectCallback());
default :
break;
}
}

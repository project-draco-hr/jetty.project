{
  for (  WebSocketSession session : openSessions) {
    if (session.getConnection() != null) {
      try {
        session.getConnection().close(StatusCode.SHUTDOWN,"Shutdown");
      }
 catch (      Throwable t) {
        LOG.debug("During Shutdown All Connections",t);
      }
    }
  }
  openSessions.clear();
}

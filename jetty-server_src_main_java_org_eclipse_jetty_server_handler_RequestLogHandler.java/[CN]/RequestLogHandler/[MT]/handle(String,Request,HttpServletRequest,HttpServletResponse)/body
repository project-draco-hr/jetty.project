{
  try {
    super.handle(target,baseRequest,request,response);
  }
  finally {
    if (baseRequest.getHttpChannelState().isAsync()) {
      if (baseRequest.getHttpChannelState().isInitial())       baseRequest.getAsyncContext().addListener(_listener);
    }
 else     _requestLog.log(baseRequest,(Response)response);
  }
}

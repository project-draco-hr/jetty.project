{
  boolean failure=false;
  try (SpinLock.Lock lock=_lock.lock()){
switch (_state) {
case SUCCEEDED:
case FAILED:
case IDLE:
case CLOSED:
case CALLED:
      break;
case PENDING:
case PROCESSING:
{
      _state=State.FAILED;
      failure=true;
      break;
    }
default :
  throw new IllegalStateException("state=" + _state);
}
}
 if (failure) onCompleteFailure(x);
}

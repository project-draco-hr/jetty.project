{
  loop:   while (true) {
    State current=_state.get();
switch (current) {
case PROCESSING:
{
        if (!_state.compareAndSet(State.PROCESSING,State.CALLED))         continue loop;
        break loop;
      }
case PENDING:
{
      if (!_state.compareAndSet(State.PENDING,State.PROCESSING))       continue loop;
      processing();
      break loop;
    }
case CLOSED:
{
    break loop;
  }
default :
{
  throw new IllegalStateException(toString());
}
}
}
}

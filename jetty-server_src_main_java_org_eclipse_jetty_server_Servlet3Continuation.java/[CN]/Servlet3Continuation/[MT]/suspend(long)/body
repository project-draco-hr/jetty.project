{
  _asyncContext=_request.startAsync();
  ;
  if (!_request.getAsyncRequest().isInitial() || _resumed || _timeout) {
    _resumed=false;
    _timeout=false;
    return _resumed;
  }
  _request.setAsyncTimeout(timeout);
  _request.addAsyncListener(this);
  if (_retry == null)   _retry=new RetryRequest();
  throw _retry;
}

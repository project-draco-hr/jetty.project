{
  MultiMap mm=new MultiMap();
  String key="formats";
  mm.put(key,"gzip");
  assertMapSize(mm,1);
  assertValues(mm,key,"gzip");
  List<String> extras=new ArrayList<String>();
  extras.add("jar");
  extras.add("pack200");
  extras.add("zip");
  mm.addValues(key,extras);
  assertMapSize(mm,1);
  assertValues(mm,key,"gzip","jar","pack200","zip");
}

{
synchronized (this) {
    BufferedReader reader=new BufferedReader(new StringReader(data));
    String line;
    while ((line=reader.readLine()) != null) {
      output.write(DATA_FIELD);
      output.write(line.getBytes(UTF_8.name()));
      output.write(CRLF);
    }
    output.write(CRLF);
    flush();
  }
}

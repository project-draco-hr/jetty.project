{
synchronized (_lock) {
    try {
      if (_state == STARTED || _state == STARTING)       return;
      setStarting();
      doStart();
      Log.debug("started {}",this);
      setStarted();
    }
 catch (    Exception e) {
      Log.warn("failed " + this,e);
      setFailed(e);
      throw e;
    }
catch (    Error e) {
      Log.warn("failed " + this,e);
      setFailed(e);
      throw e;
    }
  }
}

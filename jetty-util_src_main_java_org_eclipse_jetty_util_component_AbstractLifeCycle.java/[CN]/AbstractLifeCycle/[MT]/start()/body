{
synchronized (_lock) {
    try {
      if (_state == __STARTED || _state == __STARTING)       return;
      setStarting();
      doStart();
      Log.debug(STARTED + " {}",this);
      setStarted();
    }
 catch (    Exception e) {
      Log.debug(FAILED + " " + this,e);
      setFailed(e);
      throw e;
    }
catch (    Error e) {
      Log.debug(FAILED + " " + this,e);
      setFailed(e);
      throw e;
    }
  }
}

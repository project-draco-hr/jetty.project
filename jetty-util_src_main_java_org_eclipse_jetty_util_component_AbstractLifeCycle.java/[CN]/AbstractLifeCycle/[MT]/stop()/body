{
synchronized (_lock) {
    try {
      if (_state == STOPPING || _state == STOPPED)       return;
      setStopping();
      doStop();
      Log.debug("stopped {}",this);
      setStopped();
    }
 catch (    Exception e) {
      Log.warn("failed " + this,e);
      setFailed(e);
      throw e;
    }
catch (    Error e) {
      Log.warn("failed " + this,e);
      setFailed(e);
      throw e;
    }
  }
}

{
synchronized (_lock) {
    try {
      if (_state == __STOPPING || _state == __STOPPED)       return;
      setStopping();
      doStop();
      Log.debug(STOPPED + " {}",this);
      setStopped();
    }
 catch (    Exception e) {
      Log.warn(FAILED + " " + this,e);
      setFailed(e);
      throw e;
    }
catch (    Error e) {
      Log.warn(FAILED + " " + this,e);
      setFailed(e);
      throw e;
    }
  }
}

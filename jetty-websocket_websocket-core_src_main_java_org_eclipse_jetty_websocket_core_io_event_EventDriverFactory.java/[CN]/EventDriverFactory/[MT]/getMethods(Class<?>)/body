{
  if (pojo == null) {
    throw new InvalidWebSocketException("Cannot get methods for null class");
  }
  if (cache.containsKey(pojo)) {
    return cache.get(pojo);
  }
  EventMethods methods=discoverMethods(pojo);
  if (methods == null) {
    return null;
  }
  cache.put(pojo,methods);
  return methods;
}

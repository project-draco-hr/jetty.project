{
  out.append(String.valueOf(_loader)).append("\n");
  if (_loader != null) {
    Object parent=_loader.getParent();
    if (parent != null) {
      if (!(parent instanceof Dumpable))       parent=new CLDump((ClassLoader)parent);
      if (_loader instanceof URLClassLoader)       AggregateLifeCycle.dump(out,indent,TypeUtil.asList(((URLClassLoader)_loader).getURLs()),Collections.singleton(parent));
 else       AggregateLifeCycle.dump(out,indent,Collections.singleton(parent));
    }
  }
}

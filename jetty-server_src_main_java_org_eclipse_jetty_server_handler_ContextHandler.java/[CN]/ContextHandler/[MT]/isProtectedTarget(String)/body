{
  if (target == null || _protectedTargets == null)   return false;
  while (target.startsWith("//"))   target=URIUtil.compactPath(target);
  boolean isProtected=false;
  int i=0;
  while (!isProtected && i < _protectedTargets.length) {
    isProtected=StringUtil.startsWithIgnoreCase(target,_protectedTargets[i++]);
  }
  return isProtected;
}

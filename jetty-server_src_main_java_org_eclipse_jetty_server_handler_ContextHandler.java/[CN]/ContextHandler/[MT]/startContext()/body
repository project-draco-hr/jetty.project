{
  String managedAttributes=_initParams.get(MANAGED_ATTRIBUTES);
  if (managedAttributes != null)   addEventListener(new ManagedAttributeListener(this,managedAttributes.split(",")));
  super.doStart();
  if (!_contextListeners.isEmpty()) {
    ServletContextEvent event=new ServletContextEvent(_scontext);
    for (    ServletContextListener listener : _contextListeners)     callContextInitialized(listener,event);
  }
}

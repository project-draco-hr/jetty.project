{
  _eventListeners.remove(listener);
  if (listener instanceof ServletContextListener)   _contextListeners.remove(listener);
  if (listener instanceof ServletContextAttributeListener)   _contextAttributeListeners.remove(listener);
  if (listener instanceof ServletRequestListener)   _requestListeners.remove(listener);
  if (listener instanceof ServletRequestAttributeListener)   _requestAttributeListeners.remove(listener);
}

{
  if (other._forbidden)   setForbidden(true);
 else   if (other._unchecked)   setUnchecked(true);
 else   if (other._isAnyRole)   setAnyRole(true);
 else   if (!_isAnyRole) {
    for (    String r : other._roles)     _roles=(String[])LazyList.addToArray(_roles,r,String.class);
  }
  setUserDataConstraint(other._userDataConstraint);
}

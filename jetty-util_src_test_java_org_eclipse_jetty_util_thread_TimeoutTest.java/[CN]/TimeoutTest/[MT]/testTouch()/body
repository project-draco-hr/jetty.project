{
  timeout.setDuration(200);
  timeout.setNow(1350);
  timeout.schedule(tasks[2]);
  timeout.setNow(1500);
  timeout.tick();
  for (int i=0; i < tasks.length; i++) {
    assertEquals("isExpired " + i,i != 2 && i < 4,tasks[i].isExpired());
  }
  timeout.setNow(1550);
  timeout.tick();
  for (int i=0; i < tasks.length; i++) {
    assertEquals("isExpired " + i,i < 4,tasks[i].isExpired());
  }
}

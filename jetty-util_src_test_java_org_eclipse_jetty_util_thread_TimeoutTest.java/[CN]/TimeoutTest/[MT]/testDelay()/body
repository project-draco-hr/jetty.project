{
  Timeout.Task task=new Timeout.Task();
  timeout.setNow(1100);
  timeout.schedule(task,300);
  timeout.setDuration(200);
  timeout.setNow(1300);
  timeout.tick();
  assertEquals("delay",false,task.isExpired());
  timeout.setNow(1500);
  timeout.tick();
  assertEquals("delay",false,task.isExpired());
  timeout.setNow(1700);
  timeout.tick();
  assertEquals("delay",true,task.isExpired());
}

{
synchronized (this) {
    NoSqlSession session=_sessions.get(idInCluster);
    try {
      __log.debug("invalidating session {}",idInCluster);
      if (session != null) {
        session.invalidate();
      }
    }
 catch (    Exception e) {
      __log.warn("Problem invalidating session {}",idInCluster,e);
    }
  }
}

{
synchronized (this) {
    try {
      NoSqlSession session=_sessions.remove(oldClusterId);
      update(session,newClusterId,newNodeId);
      session.setClusterId(newClusterId);
      session.setNodeId(newNodeId);
      _sessions.put(newClusterId,session);
    }
 catch (    Exception e) {
      __log.warn(e);
    }
  }
}

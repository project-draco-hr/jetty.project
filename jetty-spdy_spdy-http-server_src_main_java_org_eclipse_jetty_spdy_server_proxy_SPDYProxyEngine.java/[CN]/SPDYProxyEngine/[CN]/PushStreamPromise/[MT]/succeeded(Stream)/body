{
  super.succeeded(receiverStream);
  if (LOG.isDebugEnabled())   LOG.debug("P -> C PushStreamPromise.succeeded() called with pushStreamPromise: {}",pushStreamPromise);
  PushStreamPromise promise=pushStreamPromise;
  if (promise != null)   receiverStream.push(convertPushInfo((PushInfo)getInfo(),getSenderStream(),receiverStream),pushStreamPromise);
}

{
  ServletContext context=sce.getServletContext();
  FilterRegistration registration=context.addFilter("TestFilter",TestFilter.class.getName());
  registration.setAsyncSupported(true);
  registration.addMappingForUrlPatterns(EnumSet.of(DispatcherType.ERROR,DispatcherType.ASYNC,DispatcherType.FORWARD,DispatcherType.INCLUDE,DispatcherType.REQUEST),true,new String[]{"/dump/*","/dispatch/*","*.dump"});
}

{
  if (type == null) {
    return false;
  }
  if (type.isPrimitive()) {
    if (!targetClass.isPrimitive()) {
      Class<?> primitive=CLASS_PRIMITIVE_MAP.get(targetClass);
      return primitive.equals(type);
    }
    if (Byte.TYPE.equals(type) || Double.TYPE.equals(type) || Float.TYPE.equals(type)|| Integer.TYPE.equals(type)|| Long.TYPE.equals(type)|| Short.TYPE.equals(type)) {
      return Byte.TYPE.equals(targetClass) || Double.TYPE.equals(targetClass) || Float.TYPE.equals(targetClass)|| Integer.TYPE.equals(targetClass)|| Long.TYPE.equals(targetClass)|| Short.TYPE.equals(targetClass);
    }
    return false;
  }
 else {
    if (targetClass.isPrimitive()) {
      Class<?> targetPrimitive=CLASS_PRIMITIVE_MAP.get(type);
      return targetClass.equals(targetPrimitive);
    }
  }
  if (type.equals(targetClass)) {
    return true;
  }
  return targetClass.isAssignableFrom(type);
}

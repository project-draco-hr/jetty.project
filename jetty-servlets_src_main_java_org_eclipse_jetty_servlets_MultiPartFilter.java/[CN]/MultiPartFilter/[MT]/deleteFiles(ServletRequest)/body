{
  ArrayList files=(ArrayList)request.getAttribute(FILES);
  if (files != null) {
    Iterator iter=files.iterator();
    while (iter.hasNext()) {
      File file=(File)iter.next();
      try {
        file.delete();
      }
 catch (      Exception e) {
        _context.log("failed to delete " + file,e);
      }
    }
  }
}

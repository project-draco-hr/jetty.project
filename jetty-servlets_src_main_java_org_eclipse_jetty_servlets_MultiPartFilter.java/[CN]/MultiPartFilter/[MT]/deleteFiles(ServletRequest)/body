{
  if (!_deleteFiles)   return;
  MultiPartInputStream mpis=(MultiPartInputStream)request.getAttribute(MULTIPART);
  if (mpis != null) {
    try {
      mpis.deleteParts();
    }
 catch (    Exception e) {
      _context.log("Error deleting multipart tmp files",e);
    }
  }
  request.removeAttribute(MULTIPART);
}

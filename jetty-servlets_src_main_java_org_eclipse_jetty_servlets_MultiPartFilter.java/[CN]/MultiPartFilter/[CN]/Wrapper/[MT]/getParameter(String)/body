{
  Object o=_params.get(name);
  if (!(o instanceof byte[]) && LazyList.size(o) > 0)   o=LazyList.get(o,0);
  if (o instanceof byte[]) {
    try {
      String s=new String((byte[])o,_encoding);
      return s;
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
 else   if (o != null)   return String.valueOf(o);
  return null;
}

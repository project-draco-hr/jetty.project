{
  Object o=map.get(name);
  if (o instanceof byte[]) {
    try {
      String s=new String((byte[])o,encoding);
      return s;
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
 else   if (o instanceof String)   return (String)o;
 else   if (o instanceof String[]) {
    String[] s=(String[])o;
    return s.length > 0 ? s[0] : null;
  }
  return null;
}

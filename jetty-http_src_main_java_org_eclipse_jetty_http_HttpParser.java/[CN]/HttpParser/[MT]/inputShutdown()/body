{
  _persistent=false;
switch (_state) {
case END:
    break;
case EOF_CONTENT:
  _state=State.END;
_handler.messageComplete(_contentPosition);
break;
default :
_state=State.END;
if (!_headResponse) _handler.earlyEOF();
_handler.messageComplete(_contentPosition);
}
if (!isComplete() && !isStart()) throw new EofException();
return true;
}

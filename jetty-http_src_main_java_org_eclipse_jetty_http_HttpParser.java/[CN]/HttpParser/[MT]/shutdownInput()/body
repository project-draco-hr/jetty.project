{
  LOG.debug("shutdownInput {}",this);
switch (_state) {
case START:
case END:
    break;
case EOF_CONTENT:
  setState(State.END);
return _handler.messageComplete(_contentPosition);
case CLOSED:
break;
default :
setState(State.END);
if (!_headResponse) _handler.earlyEOF();
return _handler.messageComplete(_contentPosition);
}
return false;
}

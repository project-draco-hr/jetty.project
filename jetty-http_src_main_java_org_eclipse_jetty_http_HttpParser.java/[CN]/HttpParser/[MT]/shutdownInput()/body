{
switch (_state) {
case START:
case END:
    break;
case EOF_CONTENT:
  setState(State.END);
_handler.messageComplete(_contentPosition);
break;
case CLOSED:
break;
default :
setState(State.END);
if (!_headResponse) _handler.earlyEOF();
_handler.messageComplete(_contentPosition);
}
LOG.debug("shutdownInput {}",this);
}

{
  try {
    pdMapping.clear();
    for (Iterator<String> i=_policies.iterator(); i.hasNext(); ) {
      File policyFile=new File(i.next());
      pdMapping.putAll(DefaultPolicyLoader.load(new FileInputStream(policyFile),_evaluator));
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}

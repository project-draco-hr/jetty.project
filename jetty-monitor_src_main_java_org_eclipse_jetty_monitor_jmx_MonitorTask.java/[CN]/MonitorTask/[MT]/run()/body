{
  final long timestamp=System.currentTimeMillis();
  final EventTrigger trigger=_action.getTrigger();
  _callback.dispatch(new Runnable(){
    public void run(){
      try {
        if (trigger.match(timestamp))         _action.doExecute(timestamp);
      }
 catch (      Exception ex) {
        Log.debug(ex);
      }
    }
  }
);
}

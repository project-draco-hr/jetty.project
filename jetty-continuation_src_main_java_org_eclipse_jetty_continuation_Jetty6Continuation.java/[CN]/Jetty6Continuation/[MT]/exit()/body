{
  _initial=false;
  Throwable th=_retry;
  _retry=null;
  if (th instanceof ThreadDeath)   throw (ThreadDeath)th;
  if (th instanceof Error)   throw (Error)th;
  if (th instanceof RuntimeException)   throw (RuntimeException)th;
  if (_listeners != null) {
    for (    ContinuationListener l : _listeners)     l.onComplete(this);
  }
}

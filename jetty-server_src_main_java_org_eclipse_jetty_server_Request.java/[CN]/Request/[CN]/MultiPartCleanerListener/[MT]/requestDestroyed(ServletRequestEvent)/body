{
  MultiPartInputStream mpis=(MultiPartInputStream)sre.getServletRequest().getAttribute(__MULTIPART_INPUT_STREAM);
  if (mpis != null) {
    try {
      mpis.deleteParts();
    }
 catch (    MultiException e) {
      sre.getServletContext().log("Errors deleting multipart tmp files",e);
    }
  }
}

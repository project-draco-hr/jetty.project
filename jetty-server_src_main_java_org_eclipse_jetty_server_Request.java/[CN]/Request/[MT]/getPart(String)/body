{
  if (getContentType() == null || !getContentType().startsWith("multipart/form-data"))   return null;
  if (_multiPartInputStream == null) {
    _multiPartInputStream=new MultiPartInputStream(getInputStream(),getContentType(),(MultipartConfigElement)getAttribute(__MULTIPART_CONFIG_ELEMENT),(_context != null ? (File)_context.getAttribute("javax.servlet.context.tempdir") : null));
  }
  return _multiPartInputStream.getPart(name);
}

{
  if (_metadata.getURI() == null || !_metadata.getURI().hasQuery())   _queryParameters=NO_PARAMS;
 else {
    _queryParameters=new MultiMap<>();
    if (_queryEncoding == null)     _metadata.getURI().decodeQueryTo(_queryParameters);
 else {
      try {
        _metadata.getURI().decodeQueryTo(_queryParameters,_queryEncoding);
      }
 catch (      UnsupportedEncodingException e) {
        if (LOG.isDebugEnabled())         LOG.warn(e);
 else         LOG.warn(e.toString());
      }
    }
  }
}

{
  if (!_asyncSupported)   throw new IllegalStateException("!asyncSupported");
  HttpChannelState state=getHttpChannelState();
  if (_async == null)   _async=new AsyncContextState(state);
  AsyncContextEvent event=new AsyncContextEvent(_context,_async,state,this,this,getResponse());
  state.startAsync(event);
  return _async;
}

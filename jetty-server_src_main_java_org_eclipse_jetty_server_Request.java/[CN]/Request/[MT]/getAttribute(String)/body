{
  if ("org.eclipse.jetty.io.EndPoint.maxIdleTime".equalsIgnoreCase(name))   return new Long(getConnection().getEndPoint().getMaxIdleTime());
  Object attr=(_attributes == null) ? null : _attributes.getAttribute(name);
  if (attr == null && Continuation.ATTRIBUTE.equals(name))   return _async;
  return attr;
}

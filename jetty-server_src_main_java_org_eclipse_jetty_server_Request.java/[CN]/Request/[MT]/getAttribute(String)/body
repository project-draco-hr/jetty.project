{
  if (name.startsWith("org.eclipse.jetty")) {
    if ("org.eclipse.jetty.server.Server".equals(name))     return _channel.getServer();
    if ("org.eclipse.jetty.server.HttpChannel".equals(name))     return _channel;
    if ("org.eclipse.jetty.server.HttpConnection".equals(name) && _channel.getHttpTransport() instanceof HttpConnection)     return _channel.getHttpTransport();
  }
  return (_attributes == null) ? null : _attributes.getAttribute(name);
}

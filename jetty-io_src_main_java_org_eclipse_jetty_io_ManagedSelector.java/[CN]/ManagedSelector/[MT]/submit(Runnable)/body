{
  if (LOG.isDebugEnabled())   LOG.debug("Queued change {} on {}",change,this);
  Selector selector=null;
  try (SpinLock.Lock lock=_lock.lock()){
    _actions.offer(change);
    if (_selecting) {
      selector=_selector;
      _selecting=false;
    }
  }
   if (selector != null)   selector.wakeup();
}

{
  try {
    File policyFile=new File(filename);
    Set<PolicyBlock> policyBlocks=DefaultPolicyLoader.load(new FileInputStream(policyFile),JettyPolicy.getContext());
    for (    PolicyBlock policy : policyBlocks) {
      onPolicyChange(policy);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}

{
  try {
    _executor.execute(task);
    return true;
  }
 catch (  RejectedExecutionException e) {
    LOG.debug(e);
    LOG.warn("RejectedExecution {}",task);
    try {
      if (task instanceof Rejectable)       ((Rejectable)task).reject();
    }
 catch (    Exception x) {
      e.addSuppressed(x);
      LOG.warn(e);
    }
  }
  return false;
}

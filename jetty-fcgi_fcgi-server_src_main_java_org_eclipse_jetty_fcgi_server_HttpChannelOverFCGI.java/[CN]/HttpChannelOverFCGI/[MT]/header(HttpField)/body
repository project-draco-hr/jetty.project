{
  if (FCGI.Headers.REQUEST_METHOD.equalsIgnoreCase(field.getName()))   method=field.getValue();
 else   if (FCGI.Headers.DOCUMENT_URI.equalsIgnoreCase(field.getName()))   path=field.getValue();
 else   if (FCGI.Headers.QUERY_STRING.equalsIgnoreCase(field.getName()))   query=field.getValue();
 else   if (FCGI.Headers.SERVER_PROTOCOL.equalsIgnoreCase(field.getName()))   version=field.getValue();
 else   processField(field);
}

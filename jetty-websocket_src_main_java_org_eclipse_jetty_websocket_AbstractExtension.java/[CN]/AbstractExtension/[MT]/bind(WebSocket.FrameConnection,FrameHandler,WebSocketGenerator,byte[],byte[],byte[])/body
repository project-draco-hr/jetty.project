{
  _connection=connection;
  _inbound=incoming;
  _outbound=outgoing;
  if (dataOpcodes != null)   System.arraycopy(dataOpcodes,0,_dataOpcodes,0,dataOpcodes.length);
  if (controlOpcodes != null)   System.arraycopy(controlOpcodes,0,_dataOpcodes,0,controlOpcodes.length);
  if (bitMasks != null)   System.arraycopy(bitMasks,0,_bitMasks,0,bitMasks.length);
}

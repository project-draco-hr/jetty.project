{
  while (true) {
    CloseState current=closed.get();
switch (current) {
case NOT_CLOSED:
case LOCALLY_CLOSED:
case REMOTELY_CLOSED:
{
        if (closed.compareAndSet(current,CloseState.CLOSED)) {
          flusher.close();
          disconnect();
          return;
        }
        break;
      }
default :
{
      return;
    }
}
}
}

{
  if (LOG.isDebugEnabled())   LOG.debug("Shutting down {}",this);
switch (closed.get()) {
case NOT_CLOSED:
{
      if (LOG.isDebugEnabled())       LOG.debug("Abrupt close for {}",this);
      terminate();
      break;
    }
case LOCALLY_CLOSED:
{
    control(null,Callback.Adapter.INSTANCE,new DisconnectFrame());
    break;
  }
case REMOTELY_CLOSED:
{
  break;
}
default :
{
break;
}
}
}

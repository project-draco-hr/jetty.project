{
switch (closed.get()) {
case NOT_CLOSED:
{
      if (notifyIdleTimeout(this)) {
        close(ErrorCode.NO_ERROR.code,"idle_timeout",Callback.NOOP);
        return true;
      }
      return false;
    }
case LOCALLY_CLOSED:
case REMOTELY_CLOSED:
{
    abort(new TimeoutException());
    return true;
  }
default :
{
  return true;
}
}
}

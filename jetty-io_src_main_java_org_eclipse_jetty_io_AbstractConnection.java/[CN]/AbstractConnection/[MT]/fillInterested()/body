{
  LOG.debug("fillInterested {}",this);
  loop:   while (true) {
switch (_state.get()) {
case IDLE:
      if (_state.compareAndSet(State.IDLE,State.INTERESTED)) {
        getEndPoint().fillInterested(_readCallback);
        break loop;
      }
    break;
case FILLING:
  if (_state.compareAndSet(State.FILLING,State.FILLING_INTERESTED))   break loop;
break;
case FILLING_INTERESTED:
case INTERESTED:
break loop;
}
}
}

{
  LOG.debug("fillInterested {}",this);
  loop:   while (true) {
switch (_state.get()) {
case IDLE:
      if (_state.compareAndSet(State.IDLE,State.INTERESTED)) {
        getEndPoint().fillInterested(_readCallback);
        break loop;
      }
    break;
case FILLING:
  if (_state.compareAndSet(State.FILLING,State.FILLING_INTERESTED))   break loop;
break;
case FILLING_BLOCKED:
if (_state.compareAndSet(State.FILLING_BLOCKED,State.FILLING_BLOCKED_INTERESTED)) break loop;
break;
case BLOCKED:
if (_state.compareAndSet(State.BLOCKED,State.BLOCKED_INTERESTED)) break loop;
break;
case FILLING_BLOCKED_INTERESTED:
case FILLING_INTERESTED:
case BLOCKED_INTERESTED:
case INTERESTED:
break loop;
}
}
}

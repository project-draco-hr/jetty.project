{
  try {
    LOG.debug("onIdleExpired {} {}",this,_endp);
    if (_endp.isInputShutdown() || _endp.isOutputShutdown())     _endp.close();
 else     _endp.shutdownOutput();
  }
 catch (  IOException e) {
    LOG.ignore(e);
    try {
      _endp.close();
    }
 catch (    IOException e2) {
      LOG.ignore(e2);
    }
  }
}

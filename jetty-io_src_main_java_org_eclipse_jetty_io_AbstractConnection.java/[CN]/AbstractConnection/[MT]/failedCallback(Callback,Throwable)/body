{
  boolean dispatchFailure=isDispatchIO() && NonBlockingThread.isNonBlockingThread();
  if (dispatchFailure) {
    try {
      getExecutor().execute(new Runnable(){
        @Override public void run(){
          callback.failed(x);
        }
      }
);
    }
 catch (    RejectedExecutionException e) {
      LOG.debug(e);
      callback.failed(x);
    }
  }
 else {
    callback.failed(x);
  }
}

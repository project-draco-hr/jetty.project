{
  if (echo) {
switch (opcode) {
case WebSocketConnectionD06.OP_CLOSE:
case WebSocketConnectionD06.OP_PING:
case WebSocketConnectionD06.OP_PONG:
      break;
default :
    try {
      connection.sendFrame(flags,opcode,data,offset,length);
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
}
}
return false;
}

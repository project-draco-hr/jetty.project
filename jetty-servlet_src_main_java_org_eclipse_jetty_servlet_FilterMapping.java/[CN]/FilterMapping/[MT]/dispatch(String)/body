{
  if ("request".equalsIgnoreCase(type))   return REQUEST;
  if ("forward".equalsIgnoreCase(type))   return FORWARD;
  if ("include".equalsIgnoreCase(type))   return INCLUDE;
  if ("error".equalsIgnoreCase(type))   return ERROR;
  if ("async".equalsIgnoreCase(type))   return ASYNC;
  throw new IllegalArgumentException(type);
}

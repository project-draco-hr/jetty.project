{
  _dispatches=DEFAULT;
  if (dispatcherTypes.contains(DispatcherType.ERROR))   _dispatches|=ERROR;
  if (dispatcherTypes.contains(DispatcherType.FORWARD))   _dispatches|=FORWARD;
  if (dispatcherTypes.contains(DispatcherType.INCLUDE))   _dispatches|=INCLUDE;
  if (dispatcherTypes.contains(DispatcherType.REQUEST))   _dispatches|=REQUEST;
}

{
  Set<String> ids=getAppenderIds(props,key);
  List<Appender> appenders=new ArrayList<Appender>();
  for (  String id : ids) {
    if (declaredAppenders.containsKey(id)) {
      appenders.add(declaredAppenders.get(id));
    }
 else {
      System.err.println("No such Appender: " + id);
    }
  }
  return appenders;
}

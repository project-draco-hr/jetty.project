{
  _connector.stop();
  _connector=null;
  _sslContextFactory.stop();
  if (_threadPool instanceof LifeCycle) {
    ((LifeCycle)_threadPool).stop();
  }
  for (  HttpDestination destination : _destinations.values()) {
    destination.close();
  }
  _timeoutQ.cancelAll();
  _idleTimeoutQ.cancelAll();
  super.doStop();
}

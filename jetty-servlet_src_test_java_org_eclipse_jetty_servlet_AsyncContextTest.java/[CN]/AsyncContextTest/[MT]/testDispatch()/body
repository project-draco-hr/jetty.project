{
  String request="GET /forward HTTP/1.1\r\n" + "Host: localhost\r\n" + "Content-Type: application/x-www-form-urlencoded\r\n"+ "Connection: close\r\n"+ "\r\n";
  String responseString=_connector.getResponses(request);
  BufferedReader br=parseHeader(responseString);
  assertThat("!ForwardingServlet",br.readLine(),equalTo("Dispatched back to ForwardingServlet"));
}

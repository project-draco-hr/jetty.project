{
  for (  FilterHolder h : context.getServletHandler().getFilters())   _filterHolders.put(h.getName(),h);
  if (context.getServletHandler().getFilterMappings() != null)   _filterMappings.addAll(Arrays.asList(context.getServletHandler().getFilterMappings()));
  for (  ServletHolder h : context.getServletHandler().getServlets())   _servletHolders.put(h.getName(),h);
  if (context.getServletHandler().getServletMappings() != null)   _servletMappings.addAll(Arrays.asList(context.getServletHandler().getServletMappings()));
}

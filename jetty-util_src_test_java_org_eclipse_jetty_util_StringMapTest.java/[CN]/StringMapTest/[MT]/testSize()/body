{
  assertEquals(0,m0.size());
  assertEquals(1,m1.size());
  assertEquals(5,m5.size());
  assertEquals(3,m5i.size());
  m1.remove("abc");
  m5.remove("abc");
  m5.put("bbb","x");
  m5i.put("ABC","x");
  assertEquals(0,m0.size());
  assertEquals(0,m1.size());
  assertEquals(4,m5.size());
  assertEquals(3,m5i.size());
}

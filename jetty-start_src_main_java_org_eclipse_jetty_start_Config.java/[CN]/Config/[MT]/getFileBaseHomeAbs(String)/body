{
  File file;
  String base=getJettyBase();
  if (base != null) {
    file=new File(base,filename);
    if (file.exists())     return file;
  }
  file=new File(getJettyHome(),filename);
  if (file.exists())   return file;
  file=new File(filename);
  if (file.exists())   return file;
  return null;
}

{
  if (!_dirty && _fields != null && _fields.length > _added && _fields[_added].equals(f)) {
    _added++;
    return;
  }
  if (_dirty) {
    _added++;
    _fields=(String[])LazyList.addToArray(_fields,f,String.class);
  }
 else {
    _dirty=true;
    if (_added > 0) {
      String[] fields=new String[_added + 1];
      System.arraycopy(_fields,0,fields,0,_added);
      fields[_added++]=f;
      _fields=fields;
    }
 else {
      _fields=new String[]{f};
      _added=1;
    }
  }
}

{
  assumeJava7();
  URI uri=serverURI.resolve("/dump.jsp%00x/");
  HttpURLConnection conn=null;
  try {
    conn=(HttpURLConnection)uri.toURL().openConnection();
    conn.setConnectTimeout(1000);
    conn.setReadTimeout(1000);
    assertResponseOnBadRequest(conn);
  }
  finally {
    close(conn);
  }
}

{
  _in=new HttpInput(){
    @Override protected void onReadPossible(){
      _history.add("onReadPossible");
    }
    @Override protected void produceContent() throws IOException {
      _history.add("produceContent " + _content.size());
      for (String s=_content.poll(); s != null; s=_content.poll())       _in.content(new Content(BufferUtil.toBuffer(s)));
    }
    @Override protected void blockForContent() throws IOException {
      _history.add("blockForContent");
      super.blockForContent();
    }
    @Override protected void unready(){
      _history.add("unready");
    }
  }
;
}

{
  HttpClient client=new HttpClient();
  try {
    client.start();
    ContentResponse response=client.GET("http://127.0.0.1:" + TestJettyOSGiBootCore.DEFAULT_JETTY_HTTP_PORT + "/jsp/dump.jsp").get(5,TimeUnit.SECONDS);
    Assert.assertEquals(HttpStatus.OK_200,response.getStatus());
    String content=new String(response.getContent());
    Assert.assertTrue(content.contains("<tr><th>ServletPath:</th><td>/jsp/dump.jsp</td></tr>"));
  }
  finally {
    client.stop();
  }
}

{
  if (_delegate == null) {
    try {
      _delegate=_authenticator.validateRequest(_request,__nullResponse,false);
      if (_delegate.isSend())       _delegate=Authentication.NOT_CHECKED;
    }
 catch (    ServerAuthException e) {
      Log.debug(e);
      _delegate=Authentication.FAILED;
    }
  }
  return _delegate;
}

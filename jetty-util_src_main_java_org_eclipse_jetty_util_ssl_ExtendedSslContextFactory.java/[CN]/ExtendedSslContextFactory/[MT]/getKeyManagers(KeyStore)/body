{
  KeyManager[] managers=super.getKeyManagers(keyStore);
  if (managers != null) {
    for (int idx=0; idx < managers.length; idx++) {
      if (managers[idx] instanceof X509ExtendedKeyManager)       managers[idx]=new SniX509ExtendedKeyManager((X509ExtendedKeyManager)managers[idx],getCertAlias());
    }
  }
  LOG.debug("managers={} for {}",managers,this);
  return managers;
}

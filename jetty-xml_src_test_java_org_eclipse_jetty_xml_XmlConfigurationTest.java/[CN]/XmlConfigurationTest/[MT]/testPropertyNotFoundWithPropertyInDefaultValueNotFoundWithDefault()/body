{
  String value="bar";
  XmlConfiguration xmlConfiguration=new XmlConfiguration("" + "<Configure class=\"org.eclipse.jetty.xml.DefaultTestConfiguration\">" + "  <Set name=\"first\"><Property name=\"not_found\" default=\"${not_found|" + value + "}\"/></Set>  "+ "</Configure>");
  DefaultTestConfiguration config=(DefaultTestConfiguration)xmlConfiguration.configure();
  assertEquals(value,config.getFirst());
}

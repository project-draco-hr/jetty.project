{
  ByteBuffer buf=ByteBuffer.allocate(16);
  buf.put(new byte[]{(byte)0x81,(byte)0x85,0x37,(byte)0xfa,0x21,0x3d,0x7f,(byte)0x9f,0x4d,0x51,0x58});
  buf.flip();
  WebSocketPolicy policy=new WebSocketPolicy(WebSocketBehavior.SERVER);
  Parser parser=new Parser(policy);
  FrameParseCapture capture=new FrameParseCapture();
  parser.setListener(capture);
  parser.parse(buf);
  capture.assertNoErrors();
  capture.assertHasFrame(OpCode.TEXT,1);
  WebSocketFrame txt=capture.getFrames().get(0);
  Assert.assertThat("TextFrame.payload",txt.getPayloadAsUTF8(),is("Hello"));
}

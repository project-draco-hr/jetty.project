{
  if (getStopAtShutdown())   ShutdownThread.register(this);
  LOG.info("jetty-" + getVersion());
  HttpGenerator.setServerVersion(getVersion());
  MultiException mex=new MultiException();
  try {
    super.doStart();
  }
 catch (  Throwable e) {
    mex.add(e);
  }
  if (mex.size() == 0) {
    for (    Connector _connector : _connectors) {
      try {
        _connector.start();
      }
 catch (      Throwable e) {
        mex.add(e);
      }
    }
  }
  if (isDumpAfterStart())   dumpStdErr();
  mex.ifExceptionThrow();
}

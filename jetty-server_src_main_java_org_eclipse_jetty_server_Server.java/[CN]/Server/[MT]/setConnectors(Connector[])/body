{
  if (connectors != null) {
    for (    Connector connector : connectors) {
      if (connector.getServer() != this)       throw new IllegalArgumentException("Connector " + connector + " cannot be shared among server "+ connector.getServer()+ " and server "+ this);
    }
  }
  Connector[] oldConnectors=getConnectors();
  _container.update(this,oldConnectors,connectors,"connector");
  _connectors.removeAll(Arrays.asList(oldConnectors));
  if (connectors != null)   _connectors.addAll(Arrays.asList(connectors));
}

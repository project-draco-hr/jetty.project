{
  out.append(toString()).append(isStarted() ? " started" : " STOPPED").append('\n');
  if (_connectors != null)   for (  Connector c : _connectors)   out.append(" +-").append(String.valueOf(c)).append('\n');
  if (_threadPool != null)   out.append(" +-").append(String.valueOf(_threadPool)).append('\n');
  dumpHandlers(out,indent);
}

{
  int idxa=getIndexOf(list,fragNameA);
  int idxb=getIndexOf(list,fragNameB);
  if (idxb >= 0 && idxa < idxb) {
    return false;
  }
  if (idxb < 0) {
    if (list == _afterOthers) {
      return true;
    }
 else     if (list == _beforeOthers) {
      if (_afterOthers.contains(fragNameB))       throw new IllegalStateException("Incorrect relationship: " + fragNameB + " after "+ fragNameA);
 else       return false;
    }
  }
  return true;
}

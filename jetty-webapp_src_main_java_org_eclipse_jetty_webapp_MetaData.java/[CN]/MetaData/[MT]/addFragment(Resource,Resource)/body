{
  Boolean metaComplete=(Boolean)_context.getAttribute(METADATA_COMPLETE);
  if (metaComplete != null && metaComplete.booleanValue())   return;
  FragmentDescriptor descriptor=new FragmentDescriptor(xmlResource,this);
  _webFragmentResourceMap.put(jarResource,descriptor);
  _webFragmentRoots.add(descriptor);
  descriptor.parse();
  if (descriptor.getName() != null)   _webFragmentNameMap.put(descriptor.getName(),descriptor);
  if (_ordering != null && _ordering.isAbsolute())   return;
  if (_ordering == null && descriptor.isOrdered())   _ordering=new RelativeOrdering();
}

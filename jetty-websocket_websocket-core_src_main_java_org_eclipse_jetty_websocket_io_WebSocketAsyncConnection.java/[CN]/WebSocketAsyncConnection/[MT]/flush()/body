{
  FrameBytes<?> frameBytes=null;
  ByteBuffer buffer=null;
synchronized (queue) {
    if (flushing || queue.isEmpty()) {
      return;
    }
    frameBytes=queue.pop();
    buffer=frameBytes.getByteBuffer();
    if (buffer == null) {
      return;
    }
    flushing=true;
  }
  write(buffer,this,frameBytes);
}

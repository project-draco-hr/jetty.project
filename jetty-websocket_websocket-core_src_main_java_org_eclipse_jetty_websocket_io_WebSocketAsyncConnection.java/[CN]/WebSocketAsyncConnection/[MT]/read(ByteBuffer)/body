{
  while (true) {
    int filled=fill(getEndPoint(),buffer);
    if (filled == 0) {
      break;
    }
    if (filled < 0) {
      terminateConnection(StatusCode.PROTOCOL,null);
      break;
    }
    LOG.debug("Filled {} bytes - {}",filled,BufferUtil.toDetailString(buffer));
    parser.parse(buffer);
  }
}

{
  try {
    CloseInfo close=new CloseInfo(statusCode,message);
    if (ioState.onCloseHandshake(false)) {
      this.disconnect();
    }
 else {
      WebSocketFrame frame=close.asFrame();
      LOG.debug("Issuing: {}",frame);
      write(frame);
    }
  }
 catch (  IOException e) {
    LOG.debug(e);
  }
}

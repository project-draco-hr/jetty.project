{
  int startCount=incomingFrameQueue.size();
  ByteBuffer buf=bufferPool.acquire(policy.getBufferSize(),false);
  try {
    while (incomingFrameQueue.size() < (startCount + expectedCount)) {
      read(buf);
      parser.parse(buf);
    }
  }
  finally {
    bufferPool.release(buf);
  }
  return incomingFrameQueue;
}

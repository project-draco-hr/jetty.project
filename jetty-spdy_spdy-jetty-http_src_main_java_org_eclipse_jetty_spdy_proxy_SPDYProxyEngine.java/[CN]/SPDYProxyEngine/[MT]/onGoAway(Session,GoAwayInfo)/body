{
  Set<Session> target=null;
  for (  Set<Session> sessions : clientSessions.values()) {
    for (    Session session : sessions) {
      if (session == clientSession) {
        target=sessions;
        break;
      }
    }
    if (target != null)     break;
  }
  if (target != null) {
    target.remove(clientSession);
  }
}

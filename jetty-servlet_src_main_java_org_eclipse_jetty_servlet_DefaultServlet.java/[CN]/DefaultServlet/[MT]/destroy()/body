{
  try {
    if (_nioCache != null)     _nioCache.stop();
  }
 catch (  Exception e) {
    Log.warn(Log.EXCEPTION,e);
  }
 finally {
    try {
      if (_bioCache != null)       _bioCache.stop();
    }
 catch (    Exception e) {
      Log.warn(Log.EXCEPTION,e);
    }
 finally {
      super.destroy();
    }
  }
}

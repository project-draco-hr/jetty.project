{
  _list=null;
  _entry=null;
  _file=null;
  if (!getUseCaches()) {
    if (_jarFile != null) {
      try {
        if (LOG.isDebugEnabled())         LOG.debug("Closing JarFile " + _jarFile.getName());
        _jarFile.close();
      }
 catch (      IOException ioe) {
        LOG.ignore(ioe);
      }
    }
  }
  _jarFile=null;
  super.close();
}

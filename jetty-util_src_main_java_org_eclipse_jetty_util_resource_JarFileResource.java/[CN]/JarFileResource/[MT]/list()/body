{
  if (isDirectory() && _list == null) {
    List<String> list=null;
    try {
      list=listEntries();
    }
 catch (    Exception e) {
      LOG.warn("Retrying list:" + e);
      LOG.debug(e);
      release();
      list=listEntries();
    }
    if (list != null) {
      _list=new String[list.size()];
      list.toArray(_list);
    }
  }
  return _list;
}

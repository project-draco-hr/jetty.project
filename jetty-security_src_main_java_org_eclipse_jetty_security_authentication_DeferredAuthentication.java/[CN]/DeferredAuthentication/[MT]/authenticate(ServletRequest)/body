{
  try {
    Authentication authentication=_authenticator.validateRequest(request,__deferredResponse,true);
    if (authentication != null && (authentication instanceof Authentication.User) && !(authentication instanceof Authentication.ResponseSent)) {
      if (_identityService != null)       _previousAssociation=_identityService.associate(((Authentication.User)authentication).getUserIdentity());
      return authentication;
    }
  }
 catch (  ServerAuthException e) {
    LOG.debug(e);
  }
  return this;
}

{
  try {
    Authentication authentication=_authenticator.validateRequest(request,response,true);
    if (authentication instanceof Authentication.User && _identityService != null)     _previousAssociation=_identityService.associate(((Authentication.User)authentication).getUserIdentity());
    return authentication;
  }
 catch (  ServerAuthException e) {
    LOG.debug(e);
  }
  return this;
}

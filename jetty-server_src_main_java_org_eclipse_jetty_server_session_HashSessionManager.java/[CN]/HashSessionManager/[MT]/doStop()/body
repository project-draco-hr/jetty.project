{
  if (_storeDir != null)   saveSessions();
  super.doStop();
  _sessions.clear();
  _sessions=null;
synchronized (this) {
    if (_saveTask != null)     _saveTask.cancel();
    _saveTask=null;
    if (_task != null)     _task.cancel();
    _task=null;
    if (_timer != null)     _timer.cancel();
    _timer=null;
  }
}

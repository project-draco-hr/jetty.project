{
  try {
    File file=new File(_storeDir,idInCuster);
    if (file.exists()) {
      FileInputStream in=new FileInputStream(file);
      HashedSession session=restoreSession(in,null);
      in.close();
      addSession(session,false);
      session.didActivate();
      file.delete();
      return session;
    }
  }
 catch (  Exception e) {
    Log.warn("Problem restoring session " + idInCuster,e);
  }
  return null;
}

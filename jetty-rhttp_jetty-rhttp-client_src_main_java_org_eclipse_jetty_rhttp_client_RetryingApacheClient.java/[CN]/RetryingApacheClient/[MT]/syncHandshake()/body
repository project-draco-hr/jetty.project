{
  while (true) {
    try {
      super.syncHandshake();
      break;
    }
 catch (    IOException x) {
      getLogger().debug("Handshake failed, backing off and retrying");
      try {
        Thread.sleep(1000);
      }
 catch (      InterruptedException xx) {
        throw (IOException)new IOException().initCause(xx);
      }
    }
  }
}

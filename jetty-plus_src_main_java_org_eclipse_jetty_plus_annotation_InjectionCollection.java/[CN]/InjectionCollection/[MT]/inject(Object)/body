{
  if (injectable == null)   return;
  Class<?> clazz=injectable.getClass();
  while (clazz != null) {
    List<Injection> injections=getFieldInjections(clazz);
    for (    Injection i : injections) {
      i.inject(injectable);
    }
    injections=getMethodInjections(clazz);
    for (    Injection i : injections) {
      i.inject(injectable);
    }
    clazz=clazz.getSuperclass();
  }
}

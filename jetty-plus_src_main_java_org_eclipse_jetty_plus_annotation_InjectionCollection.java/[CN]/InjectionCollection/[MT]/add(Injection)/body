{
  if ((injection == null) || injection.getTargetClassName() == null)   return;
  if (Log.isDebugEnabled())   Log.debug("Adding injection for class=" + (injection.getTargetClassName() + " on a " + (injection.isField() ? injection.getFieldName() : injection.getMethodName())));
  List<Injection> injections=(List<Injection>)_injectionMap.get(injection.getTargetClassName());
  if (injections == null) {
    injections=new ArrayList<Injection>();
    _injectionMap.put(injection.getTargetClassName(),injections);
  }
  injections.add(injection);
}

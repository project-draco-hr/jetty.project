{
  List<LoginService> list=getServer().getBeans(LoginService.class);
  String realm=getRealmName();
  if (realm != null) {
    for (    LoginService service : list)     if (service.getName() != null && service.getName().equals(realm))     return service;
  }
 else   if (list.size() == 1)   return list.get(0);
  return null;
}

{
  TestCB cb=new TestCB(){
    int i=10;
    @Override protected Next process() throws Exception {
      processed++;
      if (i-- > 1) {
        succeeded();
        return Next.SCHEDULED;
      }
      return Next.SUCCEEDED;
    }
  }
;
  cb.iterate();
  Assert.assertTrue(cb.waitForComplete());
  Assert.assertEquals(10,cb.processed);
}

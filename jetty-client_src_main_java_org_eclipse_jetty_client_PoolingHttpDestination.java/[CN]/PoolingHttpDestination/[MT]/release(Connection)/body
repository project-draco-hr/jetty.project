{
  @SuppressWarnings("unchecked") C connection=(C)c;
  LOG.debug("{} released",connection);
  HttpClient client=getHttpClient();
  if (client.isRunning()) {
    if (connectionPool.isActive(connection))     process(connection,false);
 else     LOG.debug("{} explicit",connection);
  }
 else {
    LOG.debug("{} is stopped",client);
    close(connection);
    connection.close();
  }
}

{
  try (Locker.Lock lock=_lock.lock()){
    for (    ObjectName oname : _beans.values()) {
      if (oname != null) {
        try {
          _mbeanServer.unregisterMBean(oname);
        }
 catch (        MBeanRegistrationException|InstanceNotFoundException e) {
          LOG.warn(e);
        }
      }
    }
  }
 }

{
  if (LOG.isDebugEnabled())   LOG.debug("beanRemoved {}",obj);
  try (Locker.Lock lock=_lock.lock()){
    ObjectName bean=_beans.remove(obj);
    if (bean != null) {
      try {
        _mbeanServer.unregisterMBean(bean);
        if (LOG.isDebugEnabled())         LOG.debug("Unregistered {}",bean);
      }
 catch (      javax.management.InstanceNotFoundException e) {
        LOG.ignore(e);
      }
catch (      Exception e) {
        LOG.warn(e);
      }
    }
  }
 }

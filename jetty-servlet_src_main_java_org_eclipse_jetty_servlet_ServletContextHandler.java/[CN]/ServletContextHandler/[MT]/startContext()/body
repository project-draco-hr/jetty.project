{
  getSessionHandler();
  getSecurityHandler();
  getServletHandler();
  Handler handler=_servletHandler;
  if (_securityHandler != null) {
    _securityHandler.setHandler(handler);
    handler=_securityHandler;
  }
  if (_sessionHandler != null) {
    _sessionHandler.setHandler(handler);
    handler=_sessionHandler;
  }
  HandlerWrapper wrapper=this;
  while (wrapper != handler && wrapper.getHandler() instanceof HandlerWrapper)   wrapper=(HandlerWrapper)wrapper.getHandler();
  if (wrapper != handler) {
    if (wrapper.getHandler() != null)     throw new IllegalStateException("!ScopedHandler");
    wrapper.setHandler(handler);
  }
  super.startContext();
  if (_servletHandler != null && _servletHandler.isStarted())   _servletHandler.initialize();
}

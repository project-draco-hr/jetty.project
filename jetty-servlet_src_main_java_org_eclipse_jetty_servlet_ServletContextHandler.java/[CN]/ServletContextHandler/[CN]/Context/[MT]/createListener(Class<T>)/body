{
  try {
    T l=null;
    try {
      l=clazz.newInstance();
    }
 catch (    InstantiationException e) {
      throw new ServletException(e);
    }
catch (    IllegalAccessException e) {
      throw new ServletException(e);
    }
    if (_decorator != null)     l=_decorator.decorateListenerInstance(l);
    _created.add(l);
    return l;
  }
 catch (  ServletException e) {
    throw e;
  }
catch (  Exception e) {
    throw new ServletException(e);
  }
}

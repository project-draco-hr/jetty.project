{
  try {
    T l=super.createListener(clazz);
    for (int i=_decorators.size() - 1; i >= 0; i--) {
      Decorator decorator=_decorators.get(i);
      l=decorator.decorateListenerInstance(l);
    }
    return l;
  }
 catch (  ServletException e) {
    throw e;
  }
catch (  Exception e) {
    throw new ServletException(e);
  }
}

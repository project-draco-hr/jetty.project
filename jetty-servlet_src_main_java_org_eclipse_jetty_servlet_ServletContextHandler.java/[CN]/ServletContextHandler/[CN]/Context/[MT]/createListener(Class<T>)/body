{
  try {
    T l=null;
    try {
      l=clazz.newInstance();
    }
 catch (    InstantiationException e) {
      throw new ServletException(e);
    }
catch (    IllegalAccessException e) {
      throw new ServletException(e);
    }
    for (    Decorator decorator : _decorators)     l=decorator.decorateListenerInstance(l);
    return l;
  }
 catch (  ServletException e) {
    throw e;
  }
catch (  Exception e) {
    throw new ServletException(e);
  }
}

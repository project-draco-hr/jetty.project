{
  try {
    T s=c.newInstance();
    if (_decorator != null)     s=_decorator.decorateServletInstance(s);
    _created.add(s);
    return s;
  }
 catch (  InstantiationException e) {
    throw new ServletException(e);
  }
catch (  IllegalAccessException e) {
    throw new ServletException(e);
  }
}

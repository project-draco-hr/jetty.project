{
  try {
    T s=c.newInstance();
    for (int i=_decorators.size() - 1; i >= 0; i--) {
      Decorator decorator=_decorators.get(i);
      s=decorator.decorateServletInstance(s);
    }
    return s;
  }
 catch (  InstantiationException e) {
    throw new ServletException(e);
  }
catch (  IllegalAccessException e) {
    throw new ServletException(e);
  }
}

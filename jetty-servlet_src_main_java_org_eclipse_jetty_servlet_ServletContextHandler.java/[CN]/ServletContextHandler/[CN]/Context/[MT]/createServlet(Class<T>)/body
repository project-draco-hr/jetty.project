{
  try {
    T s=c.newInstance();
    for (    Decorator decorator : _decorators)     s=decorator.decorateServletInstance(s);
    return s;
  }
 catch (  InstantiationException e) {
    throw new ServletException(e);
  }
catch (  IllegalAccessException e) {
    throw new ServletException(e);
  }
}

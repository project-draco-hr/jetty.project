{
  try {
    T f=c.newInstance();
    if (_decorator != null)     f=_decorator.decorateFilterInstance(f);
    _created.add(f);
    return f;
  }
 catch (  InstantiationException e) {
    throw new ServletException(e);
  }
catch (  IllegalAccessException e) {
    throw new ServletException(e);
  }
}

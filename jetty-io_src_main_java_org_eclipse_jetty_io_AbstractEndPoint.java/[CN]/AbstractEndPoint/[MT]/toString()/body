{
  return String.format("%s@%x{%s<->%d,%s,%s,%s,%s,%s,%d/%d,%s}",getClass().getSimpleName(),hashCode(),getRemoteAddress(),getLocalAddress().getPort(),isOpen() ? "Open" : "CLOSED",isInputShutdown() ? "ISHUT" : "in",isOutputShutdown() ? "OSHUT" : "out",_fillInterest.isInterested() ? "R" : "-",_writeFlusher.isInProgress() ? "W" : "-",getIdleFor(),getIdleTimeout(),getConnection() == null ? null : getConnection().getClass().getSimpleName());
}

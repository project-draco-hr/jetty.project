{
  if (bundle == null)   return false;
  boolean deployed=false;
  Object[] deployers=_serviceTracker.getServices();
  if (deployers != null) {
    int i=0;
    while (!deployed && i < deployers.length) {
      BundleProvider p=(BundleProvider)deployers[i];
      deployed=p.bundleAdded(bundle);
      i++;
    }
  }
  return deployed;
}

{
  Server server=createWebAppServer("/clogging","dummy-webapp-logging-commons.war");
  server.start();
  SimpleRequest.get(server,"/clogging/logging");
  server.stop();
  TestAppender.LogEvent expectedLogs[]={new LogEvent(null,-1,Severity.DEBUG,"LoggingServlet","LoggingServlet(commons-logging) initialized",null),new LogEvent(null,-1,Severity.INFO,"LoggingServlet","LoggingServlet(commons-logging) GET requested",null)};
  assertContainsLogEvents(testAppender,expectedLogs);
}

{
  Server server=createWebAppServer("/log4j","dummy-webapp-logging-log4j.war");
  server.start();
  SimpleRequest.get(server,"/log4j/logging");
  server.stop();
  TestAppender.LogEvent expectedLogs[]={new LogEvent(null,-1,Severity.DEBUG,LOGGING_SERVLET_ID,"LoggingServlet(log4j) initialized",null),new LogEvent(null,-1,Severity.INFO,LOGGING_SERVLET_ID,"LoggingServlet(log4j) GET requested",null)};
  assertContainsLogEvents(testAppender,expectedLogs);
}

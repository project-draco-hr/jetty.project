{
  if (classCerts == null) {
    return false;
  }
  for (int i=0; i < permCerts.length; ++i) {
    boolean found=false;
    for (int j=0; j < classCerts.length; ++j) {
      if (permCerts[i].equals(classCerts[j])) {
        found=true;
        break;
      }
    }
    if (found == false) {
      return false;
    }
  }
  return true;
}

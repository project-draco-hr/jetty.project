{
  response.setContentType("text/html");
  ServletOutputStream out=response.getOutputStream();
  out.println("<html>");
  out.println("<h1>Hello World</h1>");
  out.println("</html>");
  out.flush();
  try {
    Thread.sleep(200);
  }
 catch (  InterruptedException e) {
    getServletContext().log("exception",e);
  }
}

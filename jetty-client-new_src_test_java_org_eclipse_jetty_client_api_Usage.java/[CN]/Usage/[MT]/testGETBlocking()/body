{
  HttpClient client=new HttpClient();
  Request request=client.newRequest("localhost",8080).scheme("https").method(HttpMethod.GET).path("/uri").version(HttpVersion.HTTP_1_1).param("a","b").header("X-Header","Y-value").agent("Jetty HTTP Client").cookie("cookie1","value1").decoder(null).content(null).idleTimeout(5000L);
  Future<Response> responseFuture=request.send();
  Response response=responseFuture.get();
  Assert.assertEquals(200,response.status());
}

{
  HTTPClient client=new HTTPClient();
  Response response=client.builder("localhost:8080").method("GET").uri("/").listener(new Request.Listener.Adapter(){
    @Override public void onEnd(    Request request){
    }
  }
).build().send(new Response.Listener.Adapter(){
    @Override public void onEnd(    Response response){
    }
  }
).get();
  response.getStatus();
}

{
  while (true) {
    try {
      if (runTest) {
        ByteBuffer buf=httpBuffers.getHeader();
        buffersRetrieved.getAndIncrement();
        buf.compact().put(new Byte("2")).flip();
        httpBuffers.returnBuffer(buf);
      }
 else {
        sleep(1);
      }
    }
 catch (    Exception e) {
      e.printStackTrace();
      break;
    }
  }
}

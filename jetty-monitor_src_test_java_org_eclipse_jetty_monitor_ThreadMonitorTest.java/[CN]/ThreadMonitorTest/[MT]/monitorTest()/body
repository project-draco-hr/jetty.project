{
  count=0;
  ThreadMonitor monitor=new ThreadMonitor(1,95,2){
    @Override protected void dump(    List<ThreadInfo> threads){
      ++count;
      super.dump(threads);
    }
  }
;
  monitor.start();
  Spinner spinner=new Spinner();
  Thread runner=new Thread(spinner);
  runner.start();
  Thread.sleep(DURATION);
  spinner.setDone();
  monitor.stop();
  assertTrue(count >= 2);
}

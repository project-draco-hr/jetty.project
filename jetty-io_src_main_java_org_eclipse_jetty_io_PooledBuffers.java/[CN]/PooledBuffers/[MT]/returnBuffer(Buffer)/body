{
  buffer.clear();
  if (buffer.isVolatile() || buffer.isImmutable())   return;
  if (_size.incrementAndGet() > _maxSize)   _size.decrementAndGet();
 else {
    if (isHeader(buffer))     _headers.add(buffer);
 else     if (isBuffer(buffer))     _buffers.add(buffer);
 else     _others.add(buffer);
  }
}

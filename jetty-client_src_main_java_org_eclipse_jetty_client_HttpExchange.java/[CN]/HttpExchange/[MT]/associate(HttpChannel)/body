{
  boolean result=false;
  boolean abort=false;
  try (Locker.Lock lock=_locker.lock()){
    if (requestState == State.PENDING && responseState == State.PENDING) {
      abort=_channel != null;
      if (!abort) {
        _channel=channel;
        result=true;
      }
    }
  }
   if (abort)   request.abort(new IllegalStateException(toString()));
  return result;
}

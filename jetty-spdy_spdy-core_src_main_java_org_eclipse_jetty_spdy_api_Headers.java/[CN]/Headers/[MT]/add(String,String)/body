{
  name=name.trim();
  Header header=headers.get(name.toLowerCase(Locale.ENGLISH));
  if (header == null) {
    header=new Header(name,value.trim());
    headers.put(name.toLowerCase(Locale.ENGLISH),header);
  }
 else {
    header=new Header(header.name(),header.value() + "," + value.trim());
    headers.put(name.toLowerCase(Locale.ENGLISH),header);
  }
}

{
  Fields.Field uriHeader=headers.get(HTTPSPDYHeader.URI.name(version));
  if (uriHeader != null)   if (version == SPDY.V2 && uriHeader.value().startsWith("http://"))   return true;
 else   if (version == SPDY.V3 && uriHeader.value().startsWith("/") && headers.get(HTTPSPDYHeader.HOST.name(version)) != null && headers.get(HTTPSPDYHeader.SCHEME.name(version)) != null)   return true;
  System.out.println(HTTPSPDYHeader.URI.name(version) + " not valid!");
  return false;
}

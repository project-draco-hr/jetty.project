{
  File[] files=dir.listFiles(new LibFilenameFilter());
  if (files == null || files.length == 0)   return null;
  File file=null;
  for (  File f : files) {
    if (f.getName().endsWith(Server.getVersion() + ".so")) {
      file=f;
      break;
    }
  }
  if (file == null)   file=files[0];
  if (LOG.isDebugEnabled())   LOG.debug("setuid library " + file.getName());
  return file;
}

{
  client.close();
  TLSRecord record=proxy.readFromClient();
  proxy.flushToServer(record);
  record=proxy.readFromClient();
  Assert.assertNull(String.valueOf(record),record);
  proxy.flushToServer(record);
  try {
    record=proxy.readFromServer();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  proxy.flushToClient(record);
}

{
  try {
    String value=field.getValue();
    if (value != null) {
      Map<String,List<String>> header=new HashMap<>(1);
      header.put(field.getHeader().asString(),Collections.singletonList(value));
      connection.getHttpClient().getCookieManager().put(uri,header);
    }
  }
 catch (  IOException x) {
    LOG.debug(x);
  }
}

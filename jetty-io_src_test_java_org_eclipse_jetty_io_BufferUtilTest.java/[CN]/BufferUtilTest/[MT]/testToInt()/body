{
  Buffer buf[]={new ByteArrayBuffer("0"),new ByteArrayBuffer(" 42 "),new ByteArrayBuffer("   43abc"),new ByteArrayBuffer("-44"),new ByteArrayBuffer(" - 45;"),new ByteArrayBuffer("-2147483648"),new ByteArrayBuffer("2147483647")};
  int val[]={0,42,43,-44,-45,-2147483648,2147483647};
  for (int i=0; i < buf.length; i++)   assertEquals("t" + i,val[i],BufferUtil.toInt(buf[i]));
}

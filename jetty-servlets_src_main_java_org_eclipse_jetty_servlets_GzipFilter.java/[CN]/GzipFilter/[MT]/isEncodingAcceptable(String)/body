{
  int state=STATE_DEFAULT;
  int qvalueIdx=-1;
  for (int i=0; i < encoding.length(); i++) {
    char c=encoding.charAt(i);
switch (state) {
case STATE_DEFAULT:
{
        if (';' == c)         state=STATE_SEPARATOR;
        break;
      }
case STATE_SEPARATOR:
{
      if ('q' == c || 'Q' == c)       state=STATE_Q;
      break;
    }
case STATE_Q:
{
    if ('=' == c)     state=STATE_QVALUE;
    break;
  }
case STATE_QVALUE:
{
  if (qvalueIdx < 0 && '0' == c || '1' == c)   qvalueIdx=i;
  break;
}
}
}
if (qvalueIdx < 0) return true;
if ("0".equals(encoding.substring(qvalueIdx).trim())) return false;
return true;
}

{
  if (ua == null)   return false;
  if (_excludedAgents != null) {
    if (_excludedAgents.contains(ua)) {
      return true;
    }
  }
 else   if (_excludedAgentPatterns != null) {
    for (    Pattern pattern : _excludedAgentPatterns) {
      if (pattern.matcher(ua).matches()) {
        return true;
      }
    }
  }
  return false;
}

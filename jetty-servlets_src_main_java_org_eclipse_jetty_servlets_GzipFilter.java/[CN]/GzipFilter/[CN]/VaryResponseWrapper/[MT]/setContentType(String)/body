{
  super.setContentType(ct);
  if (ct != null) {
    int colon=ct.indexOf(";");
    if (colon > 0)     ct=ct.substring(0,colon);
  }
  if (_mimeTypes != null && !_mimeTypes.contains(StringUtil.asciiToLowerCase(ct)))   super.setHeader("Vary",null);
 else   super.setHeader("Vary","Accept-Encoding");
}

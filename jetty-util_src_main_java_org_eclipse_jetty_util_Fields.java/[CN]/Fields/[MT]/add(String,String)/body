{
  name=name.trim();
  Field field=fields.get(name.toLowerCase(Locale.ENGLISH));
  if (field == null) {
    field=new Field(name,value);
    fields.put(name.toLowerCase(Locale.ENGLISH),field);
  }
 else {
    field=new Field(field.name(),field.values(),value);
    fields.put(name.toLowerCase(Locale.ENGLISH),field);
  }
}

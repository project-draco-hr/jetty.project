{
  File home=testdir.getFile("home");
  FS.ensureEmpty(home);
  TestEnv.copyTestDir("usecases/home",home);
  File common=testdir.getFile("common");
  FS.ensureEmpty(common);
  File corp=testdir.getFile("corp");
  FS.ensureEmpty(corp);
  TestEnv.makeFile(corp,"start.ini","jetty.port=9090","--include-jetty-dir=" + common.getAbsolutePath());
  TestEnv.makeFile(common,"start.ini","jetty.port=8080","--include-jetty-dir=" + corp.getAbsolutePath());
  File base=testdir.getFile("base");
  FS.ensureEmpty(base);
  TestEnv.makeFile(base,"start.ini","jetty.host=127.0.0.1","--include-jetty-dir=" + common.getAbsolutePath());
  try {
    runMain(base,home);
    Assert.fail("Should have thrown a UsageException");
  }
 catch (  UsageException e) {
    Assert.assertThat("UsageException",e.getMessage(),containsString("Duplicate"));
  }
}

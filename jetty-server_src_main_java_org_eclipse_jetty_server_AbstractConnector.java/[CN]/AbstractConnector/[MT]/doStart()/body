{
  if (_server == null)   throw new IllegalStateException("No server");
  open();
  super.doStart();
  if (_threadPool == null)   _threadPool=_server.getThreadPool();
  if (_threadPool != _server.getThreadPool() && (_threadPool instanceof LifeCycle))   ((LifeCycle)_threadPool).start();
synchronized (this) {
    _acceptorThread=new Thread[getAcceptors()];
    for (int i=0; i < _acceptorThread.length; i++)     _threadPool.dispatch(new Acceptor(i));
    if (_threadPool.isLowOnThreads())     LOG.warn("insufficient threads configured for {}",this);
  }
  LOG.info("Started {}",this);
}

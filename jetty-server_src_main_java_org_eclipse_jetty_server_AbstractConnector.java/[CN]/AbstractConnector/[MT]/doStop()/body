{
  try {
    close();
  }
 catch (  IOException e) {
    LOG.warn(e);
  }
  super.doStop();
  Thread[] acceptors;
synchronized (this) {
    acceptors=_acceptorThreads;
    _acceptorThreads=null;
  }
  if (acceptors != null) {
    for (    Thread thread : acceptors) {
      if (thread != null)       thread.interrupt();
    }
  }
}

{
  try {
    this.pojo=pojo;
    this.paramTypes=paramTypes;
    this.method=pojo.getClass().getMethod(methodName,paramTypes);
  }
 catch (  NoSuchMethodException|SecurityException e) {
    LOG.debug("Cannot use method {}({}): {}",methodName,paramTypes,e.getMessage(),e);
    this.method=null;
  }
}

{
  Set<Selection> selections=node.getSelections();
  if (selections == null) {
    return false;
  }
  if (selections.size() != howSet.size()) {
    return false;
  }
  for (  Selection selection : selections) {
    if (!this.howSet.contains(selection.getHow())) {
      return false;
    }
  }
  return true;
}

{
  try {
    if (m_bundleArchive_FIELD == null) {
      m_bundleArchive_FIELD=bundle.getClass().getDeclaredField("m_archive");
      m_bundleArchive_FIELD.setAccessible(true);
    }
    return m_bundleArchive_FIELD.get(bundle);
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
  return null;
}

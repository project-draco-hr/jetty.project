{
  this.rsv1InUse=false;
  this.rsv2InUse=false;
  this.rsv3InUse=false;
  this.isTextFrameValidated=true;
  for (  AbstractExtension ext : exts) {
    if (ext.isRsv1User()) {
      this.rsv1InUse=true;
    }
    if (ext.isRsv2User()) {
      this.rsv2InUse=true;
    }
    if (ext.isRsv3User()) {
      this.rsv3InUse=true;
    }
    if (ext.isTextDataDecoder()) {
      this.isTextFrameValidated=false;
    }
  }
}

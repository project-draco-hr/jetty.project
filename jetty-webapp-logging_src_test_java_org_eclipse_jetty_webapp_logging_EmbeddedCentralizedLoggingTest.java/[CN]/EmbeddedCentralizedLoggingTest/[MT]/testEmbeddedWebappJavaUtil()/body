{
  Server server=createWebAppServer("/javalogging","dummy-webapp-logging-java.war");
  server.start();
  SimpleRequest.get(server,"/javalogging/logging");
  server.stop();
  TestAppender.LogEvent expectedLogs[]={new LogEvent(Severity.DEBUG,LOGGING_SERVLET_ID,"LoggingServlet(java) initialized"),new LogEvent(Severity.INFO,LOGGING_SERVLET_ID,"LoggingServlet(java) GET requested")};
  assertContainsLogEvents(testAppender,expectedLogs);
}

{
  Server server=createWebAppServer("/log4j","dummy-webapp-logging-log4j.war");
  server.start();
  SimpleRequest.get(server,"/log4j/logging");
  server.stop();
  TestAppender.LogEvent expectedLogs[]={new LogEvent(Severity.DEBUG,LOGGING_SERVLET_ID,"LoggingServlet(log4j) initialized"),new LogEvent(Severity.INFO,LOGGING_SERVLET_ID,"LoggingServlet(log4j) GET requested")};
  assertContainsLogEvents(testAppender,expectedLogs);
}

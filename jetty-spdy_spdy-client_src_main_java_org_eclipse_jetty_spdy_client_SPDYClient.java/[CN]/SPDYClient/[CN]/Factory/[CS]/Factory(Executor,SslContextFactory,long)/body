{
  addBean(scheduler);
  this.idleTimeout=idleTimeout;
  if (executor == null)   executor=new QueuedThreadPool();
  this.executor=executor;
  addBean(executor);
  this.sslContextFactory=sslContextFactory;
  if (sslContextFactory != null)   addBean(sslContextFactory);
  selector=new ClientSelectorManager(executor,scheduler);
  addBean(selector);
}

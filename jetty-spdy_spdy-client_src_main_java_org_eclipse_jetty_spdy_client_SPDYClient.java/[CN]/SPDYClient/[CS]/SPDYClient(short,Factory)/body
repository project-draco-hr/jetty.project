{
  this.version=version;
  this.factory=factory;
  setInitialWindowSize(65536);
  setDispatchIO(true);
  ClientConnectionFactory connectionFactory=new SPDYClientConnectionFactory();
  if (factory.sslContextFactory != null)   connectionFactory=new SslClientConnectionFactory(factory.getSslContextFactory(),factory.getByteBufferPool(),factory.getExecutor(),new NPNClientConnectionFactory(this,connectionFactory));
  setClientConnectionFactory(connectionFactory);
}

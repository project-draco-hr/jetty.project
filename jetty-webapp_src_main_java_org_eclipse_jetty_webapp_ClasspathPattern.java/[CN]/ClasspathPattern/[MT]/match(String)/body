{
  boolean result=false;
  if (_entries != null) {
    int startIdx=0;
    name=name.replace('/','.');
    while (name.startsWith("."))     ++startIdx;
    if (startIdx > 0)     name=name.substring(startIdx);
    for (int idx=0; idx < _entries.length; idx++) {
      Entry entry=_entries[idx];
      if (entry != null) {
        if (entry.partial) {
          if (name.startsWith(entry.classpath)) {
            result=entry.result;
            break;
          }
        }
 else {
          if (name.equals(entry.classpath)) {
            result=entry.result;
            break;
          }
        }
      }
    }
  }
  return result;
}

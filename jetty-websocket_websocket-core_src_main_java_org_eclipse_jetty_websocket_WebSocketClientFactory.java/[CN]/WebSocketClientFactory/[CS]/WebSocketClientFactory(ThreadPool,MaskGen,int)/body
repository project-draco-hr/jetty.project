{
  if (threadPool == null)   threadPool=new QueuedThreadPool();
  _threadPool=threadPool;
  addBean(_threadPool);
  _buffers=new WebSocketBuffers(bufferSize);
  addBean(_buffers);
  _maskGen=maskGen;
  addBean(_maskGen);
  _selector=new WebSocketClientSelector();
  addBean(_selector);
  addBean(_sslContextFactory);
}

{
  for (  PathMatcher matcher : matchers) {
    if (matcher.matches(path)) {
      if (NOISY_LOG.isDebugEnabled()) {
        NOISY_LOG.debug("Matched TRUE on {}",path);
      }
      return true;
    }
  }
  if (NOISY_LOG.isDebugEnabled()) {
    NOISY_LOG.debug("Matched FALSE on {}",path);
  }
  return false;
}

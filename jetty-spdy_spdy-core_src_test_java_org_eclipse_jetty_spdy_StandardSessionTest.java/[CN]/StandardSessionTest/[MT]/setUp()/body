{
  threadPool=Executors.newCachedThreadPool();
  scheduler=new TimerScheduler();
  scheduler.start();
  session=new StandardSession(VERSION,bufferPool,scheduler,controller,endPoint,null,1,null,generator,new FlowControlStrategy.None());
  when(endPoint.getIdleTimeout()).thenReturn(30000L);
  headers=new Fields();
}

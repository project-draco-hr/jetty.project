{
  String uri=URIUtil.decodePath(request.getRequestURI());
  for (int i=0; i < uri.length(); ++i) {
    if (!isValidChar(uri.charAt(i))) {
      int code=Integer.parseInt(_code);
      if (code >= 400) {
        response.sendError(code,_reason);
      }
 else {
        response.setStatus(code);
      }
      return target;
    }
  }
  return null;
}

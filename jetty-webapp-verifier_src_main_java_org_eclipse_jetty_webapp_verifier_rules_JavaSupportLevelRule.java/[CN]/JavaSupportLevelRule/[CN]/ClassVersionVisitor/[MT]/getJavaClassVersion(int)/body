{
  int major=version & 0xFFFF;
  int minor=version >>> 16;
  if (major >= 50) {
    return 1.6;
  }
 else   if (major >= 49) {
    return 1.5;
  }
 else   if (major >= 48) {
    return 1.4;
  }
 else   if (major >= 47) {
    return 1.3;
  }
 else   if (major >= 46) {
    return 1.2;
  }
 else   if (major >= 45) {
    if (minor >= 3) {
      return 1.1;
    }
    return 1.0;
  }
  return 0.0;
}

{
  WebSocketTestClient client=new WebSocketTestClient();
  client.start();
  TestWebSocket[] ws=new TestWebSocket[100];
  for (int i=0; i < ws.length; i++)   ws[i]=new TestWebSocket(i);
  for (  TestWebSocket w : ws)   client.open(new InetSocketAddress("localhost",8080),w);
  Thread.sleep(5000);
  for (  TestWebSocket w : ws)   if (w._out != null)   w._out.sendMessage(WebSocket.SENTINEL_FRAME,"hello world from " + w._id);
  client._threadpool.join();
}

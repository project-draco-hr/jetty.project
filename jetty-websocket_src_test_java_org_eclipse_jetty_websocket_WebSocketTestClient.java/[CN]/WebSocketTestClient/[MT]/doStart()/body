{
  ((LifeCycle)_threadpool).start();
  _manager.start();
  _threadpool.dispatch(new Runnable(){
    public void run(){
      while (isRunning()) {
        try {
          _manager.doSelect(0);
        }
 catch (        Exception e) {
          Log.warn(e.toString());
          Log.debug(e);
          Thread.yield();
        }
      }
    }
  }
);
}

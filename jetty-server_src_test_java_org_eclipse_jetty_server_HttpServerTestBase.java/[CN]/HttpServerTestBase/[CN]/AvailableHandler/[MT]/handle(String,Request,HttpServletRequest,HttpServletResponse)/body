{
  baseRequest.setHandled(true);
  response.setStatus(200);
  response.setContentType("text/plain");
  InputStream in=request.getInputStream();
  ServletOutputStream out=response.getOutputStream();
  int avail=in.available();
  out.println(avail);
  String buf="";
  for (int i=0; i < avail; i++)   buf+=(char)in.read();
  avail=in.available();
  out.println(avail);
  try {
    _ex.exchange(null);
    _ex.exchange(null);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
  avail=in.available();
  if (avail == 0) {
    buf+=(char)in.read();
    avail=in.available();
    out.println(avail + 1);
  }
 else   if (avail == 1) {
    buf+=(char)in.read();
    avail=in.available();
    out.println(avail + 1);
  }
 else   out.println(avail);
  for (int i=0; i < avail; i++)   buf+=(char)in.read();
  avail=in.available();
  out.println(avail);
  out.println(buf);
  out.close();
}

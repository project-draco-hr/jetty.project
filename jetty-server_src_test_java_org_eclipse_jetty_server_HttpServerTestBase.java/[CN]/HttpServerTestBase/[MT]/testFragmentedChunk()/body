{
  configureServer(new EchoHandler());
  Socket client=newSocket(HOST,_connector.getLocalPort());
  try {
    OutputStream os=client.getOutputStream();
    os.write(("GET /R2 HTTP/1.1\015\012" + "Host: localhost\015\012" + "Transfer-Encoding: chunked\015\012"+ "Content-Type: text/plain\015\012"+ "Connection: close\015\012"+ "\015\012").getBytes());
    os.flush();
    Thread.sleep(PAUSE);
    os.write(("5\015\012").getBytes());
    os.flush();
    Thread.sleep(PAUSE);
    os.write(("ABCDE\015\012" + "0;\015\012\015\012").getBytes());
    os.flush();
    String response=readResponse(client);
    assertTrue(true);
  }
  finally {
    client.close();
  }
}

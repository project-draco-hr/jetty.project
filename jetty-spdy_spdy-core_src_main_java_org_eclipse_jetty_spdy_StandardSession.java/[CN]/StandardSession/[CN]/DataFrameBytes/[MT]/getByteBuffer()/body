{
  try {
    IStream stream=getStream();
    int windowSize=stream.getWindowSize();
    size=dataInfo.available();
    if (size > windowSize)     size=windowSize;
    buffer=generator.data(stream.getId(),size,dataInfo);
    return buffer;
  }
 catch (  Throwable x) {
    failed(x);
    return null;
  }
}

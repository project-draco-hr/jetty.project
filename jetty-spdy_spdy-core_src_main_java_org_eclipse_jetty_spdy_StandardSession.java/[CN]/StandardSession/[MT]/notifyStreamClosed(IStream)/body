{
  for (  Listener listener : listeners) {
    if (listener instanceof StreamListener) {
      try {
        ((StreamListener)listener).onStreamClosed(stream);
      }
 catch (      Exception x) {
        LOG.info("Exception while notifying listener " + listener,x);
      }
catch (      Error x) {
        LOG.info("Exception while notifying listener " + listener,x);
        throw x;
      }
    }
  }
}

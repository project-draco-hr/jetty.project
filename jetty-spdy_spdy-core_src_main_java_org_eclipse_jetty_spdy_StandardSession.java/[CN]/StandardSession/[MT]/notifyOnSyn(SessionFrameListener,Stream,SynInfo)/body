{
  try {
    if (listener == null)     return null;
    logger.debug("Invoking callback with {} on listener {}",synInfo,listener);
    return listener.onSyn(stream,synInfo);
  }
 catch (  Exception x) {
    logger.info("Exception while notifying listener " + listener,x);
    return null;
  }
catch (  Error x) {
    logger.info("Exception while notifying listener " + listener,x);
    throw x;
  }
}

{
  notifyIdle(idleListener,false);
  try {
    if (LOG.isDebugEnabled())     LOG.debug("Processing {}, {} data bytes",frame,data.remaining());
    if (goAwaySent.get()) {
      if (LOG.isDebugEnabled())       LOG.debug("Skipped processing of {}",frame);
      return;
    }
    int streamId=frame.getStreamId();
    IStream stream=streams.get(streamId);
    if (stream == null) {
      RstInfo rstInfo=new RstInfo(streamId,StreamStatus.INVALID_STREAM);
      if (LOG.isDebugEnabled())       LOG.debug("Unknown stream {}",rstInfo);
      rst(rstInfo,Callback.Adapter.INSTANCE);
    }
 else {
      processData(stream,frame,data);
    }
  }
  finally {
    notifyIdle(idleListener,true);
  }
}

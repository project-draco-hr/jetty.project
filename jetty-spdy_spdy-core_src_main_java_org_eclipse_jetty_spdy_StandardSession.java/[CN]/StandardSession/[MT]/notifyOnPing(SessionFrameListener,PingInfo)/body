{
  try {
    if (listener != null) {
      LOG.debug("Invoking callback with {} on listener {}",pingInfo,listener);
      listener.onPing(this,pingInfo);
    }
  }
 catch (  Exception x) {
    LOG.info("Exception while notifying listener " + listener,x);
  }
catch (  Error x) {
    LOG.info("Exception while notifying listener " + listener,x);
    throw x;
  }
}

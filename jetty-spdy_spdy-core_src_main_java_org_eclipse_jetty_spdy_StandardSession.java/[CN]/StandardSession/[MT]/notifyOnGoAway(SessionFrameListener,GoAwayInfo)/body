{
  try {
    if (listener != null) {
      logger.debug("Invoking callback with {} on listener {}",goAwayInfo,listener);
      listener.onGoAway(this,goAwayInfo);
    }
  }
 catch (  Exception x) {
    logger.info("Exception while notifying listener " + listener,x);
  }
catch (  Error x) {
    logger.info("Exception while notifying listener " + listener,x);
    throw x;
  }
}

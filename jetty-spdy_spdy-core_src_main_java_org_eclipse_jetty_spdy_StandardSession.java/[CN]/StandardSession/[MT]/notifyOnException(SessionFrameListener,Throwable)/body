{
  try {
    if (listener != null) {
      logger.debug("Invoking callback with {} on listener {}",x,listener);
      listener.onException(x);
    }
  }
 catch (  Exception xx) {
    logger.info("Exception while notifying listener " + listener,xx);
  }
catch (  Error xx) {
    logger.info("Exception while notifying listener " + listener,xx);
    throw xx;
  }
}

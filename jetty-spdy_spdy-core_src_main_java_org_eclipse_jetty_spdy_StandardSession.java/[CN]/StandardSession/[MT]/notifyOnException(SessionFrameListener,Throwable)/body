{
  try {
    if (listener != null) {
      LOG.debug("Invoking callback with {} on listener {}",x,listener);
      listener.onFailure(this,x);
    }
  }
 catch (  Exception xx) {
    LOG.info("Exception while notifying listener " + listener,xx);
  }
catch (  Error xx) {
    LOG.info("Exception while notifying listener " + listener,xx);
    throw xx;
  }
}

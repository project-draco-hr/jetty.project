{
  StringBuilder builder=new StringBuilder();
  builder.append(id).append(" ");
  builder.append(getMethod()).append(" ");
  builder.append(getURI()).append(CRLF);
  for (  Map.Entry<String,String> header : getHeaders().entrySet())   builder.append(header.getKey()).append(": ").append(header.getValue()).append(CRLF);
  builder.append(getBody().length).append(" body bytes").append(CRLF);
  return builder.toString();
}

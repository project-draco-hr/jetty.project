{
  if (_value == null || search == null)   return _value == search;
  if (_value.equals(search))   return true;
  String[] values=getValues();
  for (  String v : values) {
    if (v.equals(search))     return true;
    if (v.startsWith(search) && v.charAt(search.length()) == ';')     return true;
  }
  return false;
}

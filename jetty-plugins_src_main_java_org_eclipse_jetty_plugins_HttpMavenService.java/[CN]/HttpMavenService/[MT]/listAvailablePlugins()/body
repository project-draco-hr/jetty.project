{
  System.out.println("Using local repo: " + _searchLocalRepository + " remote repo: "+ _searchRemoteRepository);
  Set<String> availablePlugins=new HashSet<>();
  if (_searchRemoteRepository)   availablePlugins.addAll(getListOfRemotePlugins());
  if (_searchLocalRepository)   availablePlugins.addAll(getListOfLocalPlugins());
  return availablePlugins;
}

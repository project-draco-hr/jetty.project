{
  if (buf == null) {
    data=null;
    return this;
  }
  if (isControlFrame()) {
    if (buf.remaining() > WebSocketFrame.MAX_CONTROL_PAYLOAD) {
      throw new ProtocolException("Control Payloads can not exceed 125 bytes in length. (was " + buf.remaining() + " bytes)");
    }
  }
  data=buf.slice();
  payloadLength=data.limit();
  return this;
}

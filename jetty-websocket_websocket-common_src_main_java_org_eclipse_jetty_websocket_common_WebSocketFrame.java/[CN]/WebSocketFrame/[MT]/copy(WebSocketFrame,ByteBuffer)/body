{
  fin=copy.fin;
  rsv1=copy.rsv1;
  rsv2=copy.rsv2;
  rsv3=copy.rsv3;
  opcode=copy.opcode;
  type=copy.type;
  masked=copy.masked;
  mask=null;
  if (copy.mask != null) {
    mask=new byte[copy.mask.length];
    System.arraycopy(copy.mask,0,mask,0,mask.length);
  }
  continuationIndex=copy.continuationIndex;
  continuation=copy.continuation;
  setPayload(payload);
}

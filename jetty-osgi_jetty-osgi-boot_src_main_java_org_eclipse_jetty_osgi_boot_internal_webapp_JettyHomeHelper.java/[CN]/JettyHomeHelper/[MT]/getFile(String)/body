{
  if (file == null) {
    return null;
  }
  try {
    if (file.startsWith("file:/")) {
      if (!file.startsWith("file://")) {
        return new File(new URI(file));
      }
 else {
        return new File(new URL(file).toURI());
      }
    }
 else {
      return new File(file);
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
    return new File(file);
  }
}

{
  if (_compressedStream == null) {
    if (getResponse().isCommitted() || _noCompression)     return getResponse().getOutputStream();
    _compressedStream=newCompressedStream(_request,(HttpServletResponse)getResponse());
  }
 else   if (_writer != null)   throw new IllegalStateException("getWriter() called");
  return _compressedStream;
}

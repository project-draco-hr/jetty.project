{
  SSLParameters sslParams=sslEngine.getSSLParameters();
  sslParams.setEndpointIdentificationAlgorithm(_endpointIdentificationAlgorithm);
  sslParams.setUseCipherSuitesOrder(_useCipherSuitesOrder);
  if (!_certAliases.isEmpty() || !_certWilds.isEmpty())   sslParams.setSNIMatchers(Collections.singletonList((SNIMatcher)new AliasSNIMatcher()));
  sslEngine.setSSLParameters(sslParams);
  if (getWantClientAuth())   sslEngine.setWantClientAuth(getWantClientAuth());
  if (getNeedClientAuth())   sslEngine.setNeedClientAuth(getNeedClientAuth());
  sslEngine.setEnabledCipherSuites(selectCipherSuites(sslEngine.getEnabledCipherSuites(),sslEngine.getSupportedCipherSuites()));
  sslEngine.setEnabledProtocols(selectProtocols(sslEngine.getEnabledProtocols(),sslEngine.getSupportedProtocols()));
}

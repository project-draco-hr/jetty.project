{
  checkIsStarted();
  SSLServerSocketFactory factory=_factory._context.getServerSocketFactory();
  SSLServerSocket socket=(SSLServerSocket)(host == null ? factory.createServerSocket(port,backlog) : factory.createServerSocket(port,backlog,InetAddress.getByName(host)));
  if (getWantClientAuth())   socket.setWantClientAuth(getWantClientAuth());
  if (getNeedClientAuth())   socket.setNeedClientAuth(getNeedClientAuth());
  socket.setEnabledCipherSuites(_selectedCipherSuites);
  socket.setEnabledProtocols(_selectedProtocols);
  return socket;
}

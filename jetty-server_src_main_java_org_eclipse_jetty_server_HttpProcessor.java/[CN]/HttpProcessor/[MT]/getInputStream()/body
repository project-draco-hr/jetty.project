{
  if (_expect100Continue) {
    if (available() == 0) {
      if (_controller.isResponseCommitted())       throw new IllegalStateException("Committed before 100 Continues");
      _controller.send1xx(HttpStatus.CONTINUE_100);
    }
    _expect100Continue=false;
  }
  if (_in == null)   _in=new HttpInput(HttpProcessor.this);
  return _in;
}

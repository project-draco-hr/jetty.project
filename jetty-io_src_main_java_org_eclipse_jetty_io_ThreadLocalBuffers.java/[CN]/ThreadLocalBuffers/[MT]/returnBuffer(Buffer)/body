{
  buffer.clear();
  if (buffer.isVolatile() || buffer.isImmutable())   return;
  ThreadBuffers buffers=_buffers.get();
  if (buffers._header == null && isHeader(buffer))   buffers._header=buffer;
 else   if (buffers._buffer == null && isBuffer(buffer))   buffers._buffer=buffer;
 else   buffers._other=buffer;
}

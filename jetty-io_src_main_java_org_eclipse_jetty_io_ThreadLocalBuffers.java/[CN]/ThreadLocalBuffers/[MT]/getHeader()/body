{
  ThreadBuffers buffers=_buffers.get();
  if (buffers._header != null) {
    ByteBuffer b=buffers._header;
    buffers._header=null;
    return b;
  }
  if (buffers._other != null && isHeader(buffers._other)) {
    ByteBuffer b=buffers._other;
    buffers._other=null;
    return b;
  }
  return newHeader();
}

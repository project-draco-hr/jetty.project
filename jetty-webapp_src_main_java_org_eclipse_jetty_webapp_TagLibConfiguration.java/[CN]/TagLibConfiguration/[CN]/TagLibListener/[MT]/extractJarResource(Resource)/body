{
  if (r == null)   return null;
  try {
    String url=r.getURI().toURL().toString();
    int idx=url.lastIndexOf("!/");
    if (idx >= 0)     url=url.substring(0,idx);
    if (url.startsWith("jar:"))     url=url.substring(4);
    return Resource.newResource(url);
  }
 catch (  IOException e) {
    LOG.warn(e);
    return null;
  }
}

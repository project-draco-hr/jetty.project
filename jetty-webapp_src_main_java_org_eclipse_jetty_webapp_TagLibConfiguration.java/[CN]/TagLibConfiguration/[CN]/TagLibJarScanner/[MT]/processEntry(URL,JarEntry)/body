{
  try {
    String name=entry.getName();
    if (name.startsWith("META-INF/") && name.toLowerCase().endsWith(".tld")) {
      Resource tld=_context.newResource("jar:" + jarUrl + "!/"+ name);
      _tlds.add(tld);
      Log.debug("TLD found {}",tld);
    }
  }
 catch (  Exception e) {
    Log.warn("Problem processing jar entry " + entry,e);
  }
}

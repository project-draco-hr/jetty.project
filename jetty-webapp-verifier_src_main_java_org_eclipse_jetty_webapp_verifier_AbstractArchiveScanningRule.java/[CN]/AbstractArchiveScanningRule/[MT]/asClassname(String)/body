{
  StringBuffer name=new StringBuffer();
  for (  char c : path.toCharArray()) {
    if (c == '/') {
      name.append(".");
    }
 else {
      name.append(c);
    }
  }
  if (name.toString().endsWith(".class")) {
    name.delete(name.length() - 6,name.length() - 1);
  }
  return name.toString();
}

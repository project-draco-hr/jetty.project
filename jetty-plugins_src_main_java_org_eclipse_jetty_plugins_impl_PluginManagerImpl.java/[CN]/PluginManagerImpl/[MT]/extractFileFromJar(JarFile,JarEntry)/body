{
  for (  String exclude : excludes)   if (jarEntry.getName().startsWith(exclude))   return;
  System.out.println("Extracting: " + jarEntry.getName());
  File f=new File(_jettyHome + File.separator + jarEntry.getName());
  if (jarEntry.isDirectory()) {
    f.mkdir();
    return;
  }
  try (InputStream is=jarFile.getInputStream(jarEntry);FileOutputStream fos=new FileOutputStream(f)){
    while (is.available() > 0) {
      fos.write(is.read());
    }
  }
 catch (  IOException e) {
    throw new IllegalStateException("Could not extract plugin jar",e);
  }
}

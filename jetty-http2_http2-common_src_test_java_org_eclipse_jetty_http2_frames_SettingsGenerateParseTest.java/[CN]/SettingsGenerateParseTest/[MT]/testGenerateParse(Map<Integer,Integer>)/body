{
  SettingsGenerator generator=new SettingsGenerator(new HeaderGenerator());
  final List<SettingsFrame> frames=new ArrayList<>();
  Parser parser=new Parser(byteBufferPool,new Parser.Listener.Adapter(){
    @Override public boolean onSettings(    SettingsFrame frame){
      frames.add(frame);
      return false;
    }
  }
,4096,8192);
  for (int i=0; i < 2; ++i) {
    ByteBufferPool.Lease lease=new ByteBufferPool.Lease(byteBufferPool);
    generator.generateSettings(lease,settings,true,false);
    frames.clear();
    for (    ByteBuffer buffer : lease.getByteBuffers()) {
      while (buffer.hasRemaining()) {
        parser.parse(buffer);
      }
    }
  }
  return frames;
}

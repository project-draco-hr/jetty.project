{
  try {
    ByteChannel channel=null;
synchronized (this) {
      if (_connection == null && _exception == null && _channel != null) {
        channel=_channel;
        _channel=null;
      }
    }
    if (channel != null) {
      closeChannel(channel,WebSocketConnectionD10.CLOSE_NOCLOSE,"cancelled");
      return true;
    }
    return false;
  }
  finally {
    _done.countDown();
  }
}

{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Violation other=(Violation)obj;
  if (detail == null) {
    if (other.detail != null) {
      return false;
    }
  }
 else   if (!detail.equals(other.detail)) {
    return false;
  }
  if (path == null) {
    if (other.path != null) {
      return false;
    }
  }
 else   if (!path.equals(other.path)) {
    return false;
  }
  if (severity == null) {
    if (other.severity != null) {
      return false;
    }
  }
 else   if (!severity.equals(other.severity)) {
    return false;
  }
  return true;
}

{
  this.file=file;
  init();
  try (FileReader reader=new FileReader(file)){
    try (BufferedReader buf=new BufferedReader(reader)){
      String line;
      while ((line=buf.readLine()) != null) {
        if (line.length() == 0) {
          continue;
        }
        if (line.charAt(0) == '#') {
          continue;
        }
        process(line.trim());
      }
    }
   }
 }

{
  boolean execute=true;
  while (execute) {
    try {
      if (LOG.isDebugEnabled())       LOG.debug("Executing {}",this);
      super.run();
    }
  finally {
      if (LOG.isDebugEnabled())       LOG.debug("Completing {}",this);
synchronized (this) {
        dispatched=redispatch;
        redispatch=false;
        execute=dispatched;
      }
    }
  }
}

{
  super.doStart();
  if (_propertyUserStore == null) {
    if (Log.isDebugEnabled())     Log.debug("doStart: Starting new PropertyUserStore. PropertiesFile: " + _config + " refreshInterval: "+ _refreshInterval);
    _propertyUserStore=new PropertyUserStore();
    _propertyUserStore.setRefreshInterval(_refreshInterval);
    _propertyUserStore.setConfig(_config);
    _propertyUserStore.registerUserListener(this);
    _propertyUserStore.start();
  }
}

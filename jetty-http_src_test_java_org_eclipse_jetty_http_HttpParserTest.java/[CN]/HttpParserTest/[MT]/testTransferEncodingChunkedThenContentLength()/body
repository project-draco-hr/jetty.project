{
  ByteBuffer buffer=BufferUtil.toBuffer("POST /chunk HTTP/1.1\015\012" + "Host: localhost\015\012" + "Transfer-Encoding: chunked\015\012"+ "Content-Length: 1\015\012"+ "\015\012"+ "1\015\012"+ "X\015\012"+ "0\015\012"+ "\015\012");
  HttpParser.RequestHandler handler=new Handler();
  HttpParser parser=new HttpParser(handler);
  parseAll(parser,buffer);
  assertEquals("POST",_methodOrVersion);
  assertEquals("/chunk",_uriOrStatus);
  assertEquals("HTTP/1.1",_versionOrReason);
  assertEquals("X",_content);
  assertTrue(_headerCompleted);
  assertTrue(_messageCompleted);
}

{
  ByteBuffer buffer=BufferUtil.toBuffer("HTTP/1.1 200 OK\015\012" + "Content-Length: 0\015\012" + "Connection: close\015\012"+ "\015\012"+ "\015\012"+ "HTTP/1.1 400 OK\015\012");
  Handler handler=new Handler();
  HttpParser parser=new HttpParser((HttpParser.ResponseHandler)handler);
  parser.parseNext(buffer);
  assertEquals("HTTP/1.1",f0);
  assertEquals("200",f1);
  assertEquals("OK",f2);
  assertEquals(null,_content);
  assertTrue(headerCompleted);
  assertTrue(messageCompleted);
}

{
  ByteBuffer buffer=BufferUtil.toBuffer("POST /fo\u0690 HTTP/1.0\015\012" + "\015\012",StringUtil.__UTF8_CHARSET);
  Handler handler=new Handler();
  HttpParser parser=new HttpParser((HttpParser.RequestHandler)handler);
  parser.parse(buffer);

{
  StringEndPoint io=new StringEndPoint();
  io.setInput("POST /222  \015\012");
  ByteArrayBuffer buffer=new ByteArrayBuffer(4096);
  SimpleBuffers buffers=new SimpleBuffers(buffer,null);
  f2=null;

{
  _fields.clear();
  _headers=-1;
  _hdr=new String[10];
  _val=new String[10];
  _methodOrVersion=method;
  _uriOrStatus=BufferUtil.toUTF8String(uri);
  _versionOrReason=version == null ? null : version.asString();
  fields=new HttpFields();
  _messageCompleted=false;
  _headerCompleted=false;
  _early=false;
  return false;
}

{
  _fields.clear();
  request=false;
  _methodOrVersion=version.asString();
  _uriOrStatus=Integer.toString(status);
  _versionOrReason=reason == null ? null : reason.toString();
  fields=new HttpFields();
  _hdr=new String[9];
  _val=new String[9];
  _messageCompleted=false;
  _headerCompleted=false;
  return false;
}

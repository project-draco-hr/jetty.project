{
  if (sourceFile.isDirectory()) {
    if (!destFile.exists()) {
      destFile.mkdir();
    }
    String files[]=sourceFile.list();
    for (int i=0; i < files.length; i++) {
      copyDirectory(new File(sourceFile,files[i]),new File(destFile,files[i]));
    }
  }
 else {
    if (!sourceFile.exists()) {
    }
 else {
      copyFile(sourceFile,destFile);
    }
  }
}

{
  if (_methodName == null || _className == null)   return;
  if (_targetClass == null)   _targetClass=Loader.loadClass(null,_className);
  Class arg=TypeUtil.fromName(_paramCanonicalName);
  if (arg == null)   arg=Loader.loadClass(null,_paramCanonicalName);
  _target=_targetClass.getDeclaredMethod(_methodName,new Class[]{arg});
}

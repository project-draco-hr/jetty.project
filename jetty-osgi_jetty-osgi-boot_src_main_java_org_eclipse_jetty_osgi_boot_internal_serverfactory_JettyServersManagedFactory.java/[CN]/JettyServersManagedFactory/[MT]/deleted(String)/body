{
  ServerInstanceWrapper server=(ServerInstanceWrapper)_serversIndexedByPID.remove(pid);
  String name=_serversNameIndexedByPID.remove(pid);
  if (name != null) {
    _serversPIDIndexedByName.remove(name);
  }
 else {
  }
  if (server != null) {
    try {
      server.stop();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}

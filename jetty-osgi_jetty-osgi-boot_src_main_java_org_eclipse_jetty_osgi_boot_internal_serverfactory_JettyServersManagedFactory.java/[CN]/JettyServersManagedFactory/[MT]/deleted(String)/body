{
  Server server=(Server)_serversIndexedByPID.remove(pid);
  String name=_serversNameIndexedByPID.remove(pid);
  if (name != null) {
    _serversPIDIndexedByName.remove(name);
  }
 else {
  }
  if (server != null) {
    try {
      ServerInstanceWrapperHelper.stop(server,pid);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}

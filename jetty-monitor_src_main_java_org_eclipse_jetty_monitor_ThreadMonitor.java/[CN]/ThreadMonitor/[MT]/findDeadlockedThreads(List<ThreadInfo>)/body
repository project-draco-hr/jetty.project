{
  if (threadInfo != null) {
    try {
      long[] threads=findDeadlockedThreads();
      if (threads != null && threads.length > 0) {
        ThreadInfo currInfo;
        for (int idx=0; idx < threads.length; idx++) {
          currInfo=getThreadInfo(threads[idx],Integer.MAX_VALUE);
          if (currInfo != null) {
            threadInfo.add(currInfo);
          }
        }
      }
    }
 catch (    Exception ex) {
      Log.debug(ex);
    }
  }
  return threadInfo;
}

{
  if (basedir == null) {
    String cwd=System.getProperty("basedir");
    if (cwd == null) {
      cwd=System.getProperty("user.dir");
      basedir=new File(cwd);
      System.setProperty("basedir",basedir.getAbsolutePath());
    }
 else {
      basedir=new File(cwd);
    }
    baseURI=basedir.toURI();
  }
  return basedir;
}

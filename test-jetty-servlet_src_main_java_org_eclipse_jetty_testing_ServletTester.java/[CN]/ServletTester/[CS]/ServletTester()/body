{
  try {
    _server.addBean(new ErrorHandler());
    _server.setSendServerVersion(false);
    _server.addConnector(_connector);
    _server.setHandler(_context);
  }
 catch (  Error e) {
    throw e;
  }
catch (  RuntimeException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException(e);
  }
}

{
  int actualCount=0;
  for (  MuxedFrame frame : frames) {
    if (frame.getChannelId() == channelId) {
      if (frame.getOpCode() == opcode) {
        actualCount++;
      }
    }
  }
  Assert.assertThat("Expected Count of " + OpCode.name(opcode) + " frames on Channel ID "+ channelId,actualCount,is(expectedCount));
}

{
  StringBuilder b=new StringBuilder();
  b.append("WriteBytesProvider[");
  if (failure != null) {
    b.append("failure=").append(failure.getClass().getName());
    b.append(":").append(failure.getMessage()).append(',');
  }
 else {
    b.append("queue.size=").append(queue.size());
  }
  b.append(']');
  return b.toString();
}

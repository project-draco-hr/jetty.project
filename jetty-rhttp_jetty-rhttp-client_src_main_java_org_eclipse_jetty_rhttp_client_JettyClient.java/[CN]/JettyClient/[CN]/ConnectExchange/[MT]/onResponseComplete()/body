{
  int responseStatus=getResponseStatus();
  if (responseStatus == 200) {
    try {
      connectComplete(content.toByteArray());
    }
 catch (    IOException x) {
      onException(x);
    }
  }
 else   if (responseStatus == 401) {
    notifyConnectRequired();
  }
 else {
    notifyConnectException();
  }
}

{
  if (servlet == null)   return false;
  Class c=servlet.getClass();
  boolean result=false;
  while (c != null && !result) {
    result=isJspServlet(c.getName());
    c=c.getSuperclass();
  }
  System.err.println(servlet.getClass().getName() + " is " + (result ? "" : "not")+ " jsp servlet");
  return result;
}

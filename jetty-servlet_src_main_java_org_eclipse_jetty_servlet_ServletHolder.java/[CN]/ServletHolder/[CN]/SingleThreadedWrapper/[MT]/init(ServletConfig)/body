{
synchronized (this) {
    if (_stack.size() == 0) {
      try {
        Servlet s=(Servlet)newInstance();
        s=getServletHandler().customizeServlet(s);
        s.init(config);
        _stack.push(s);
      }
 catch (      ServletException e) {
        throw e;
      }
catch (      Exception e) {
        throw new ServletException(e);
      }
    }
  }
}

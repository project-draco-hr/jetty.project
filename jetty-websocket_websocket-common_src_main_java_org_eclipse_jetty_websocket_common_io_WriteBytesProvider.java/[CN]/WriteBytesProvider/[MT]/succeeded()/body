{
  generator.getBufferPool().release(buffer);
  if ((active != null) && (active.frame.remaining() <= 0)) {
    FrameEntry entry=active;
    active=null;
    entry.notifySucceeded();
  }
  while (!past.isEmpty()) {
    FrameEntry entry=past.pop();
    entry.notifySucceeded();
  }
  flushCallback.succeeded();
}

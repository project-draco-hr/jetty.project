{
synchronized (this) {
    boolean notified=false;
    if (active != null) {
      active.notifyFailure(t);
      notified=true;
    }
    failure=t;
    for (    FrameEntry fe : queue) {
      fe.notifyFailure(t);
      notified=true;
    }
    queue.clear();
    if (notified) {
      flushCallback.failed(t);
    }
  }
}

{
  if ((flags & 0x8) != 0)   _utf8.append(buffer.array(),buffer.getIndex(),buffer.length());
 else   if (_utf8.length() == 0)   _data.add(opcode,buffer.toString("utf-8"));
 else {
    _utf8.append(buffer.array(),buffer.getIndex(),buffer.length());
    _data.add(opcode,_utf8.toString());
    _utf8.reset();
  }
}

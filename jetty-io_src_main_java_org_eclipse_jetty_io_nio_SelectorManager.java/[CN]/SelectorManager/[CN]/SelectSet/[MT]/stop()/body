{
  final CountDownLatch closed=new CountDownLatch(1);
  Closer close=new Closer(){
    public void close(){
      try {
        super.close();
      }
  finally {
        closed.countDown();
      }
    }
  }
;
  addChange(close);
  if (!closed.await(1,TimeUnit.SECONDS))   close.close();
synchronized (this) {
    _timeout.cancelAll();
    try {
      if (_selector != null)       _selector.close();
    }
 catch (    IOException e) {
      Log.ignore(e);
    }
    _selector=null;
  }
}

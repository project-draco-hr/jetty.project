{
  for (int i=0; i < _configurations.length; i++)   _configurations[i].configureDefaults();
  Resource web_inf=getWebInf();
  if (web_inf != null) {
    Resource work=web_inf.addPath("work");
    if (work.exists() && work.isDirectory() && work.getFile() != null && work.getFile().canWrite() && getAttribute(ServletContext.TEMPDIR) == null)     setAttribute(ServletContext.TEMPDIR,work.getFile());
  }
  for (int i=0; i < _configurations.length; i++)   _configurations[i].configureWebApp();
  super.startContext();
}

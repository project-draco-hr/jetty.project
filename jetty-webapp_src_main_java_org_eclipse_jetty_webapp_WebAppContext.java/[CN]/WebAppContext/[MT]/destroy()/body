{
  MultiException mx=new MultiException();
  if (_configurations != null) {
    for (int i=_configurations.length; i-- > 0; ) {
      try {
        _configurations[i].destroy(this);
      }
 catch (      Exception e) {
        mx.add(e);
      }
    }
  }
  _configurations=null;
  super.destroy();
  mx.ifExceptionThrowRuntime();
}

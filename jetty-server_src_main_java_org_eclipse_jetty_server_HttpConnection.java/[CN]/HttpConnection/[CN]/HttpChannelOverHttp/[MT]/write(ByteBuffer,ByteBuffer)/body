{
  FutureCallback<Void> fcb=new FutureCallback<>();
  if (BufferUtil.hasContent(b0)) {
    if (BufferUtil.hasContent(b1)) {
      getEndPoint().write(null,fcb,b0,b1);
    }
 else {
      getEndPoint().write(null,fcb,b0);
    }
  }
 else {
    if (BufferUtil.hasContent(b1)) {
      getEndPoint().write(null,fcb,b1);
    }
 else {
      fcb.completed(null);
    }
  }
  return fcb;
}

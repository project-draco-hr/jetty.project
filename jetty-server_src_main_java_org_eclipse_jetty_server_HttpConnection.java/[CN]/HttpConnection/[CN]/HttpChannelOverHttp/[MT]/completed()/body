{
  LOG.debug("{} completed");
  if (!_parser.isComplete()) {
    _httpInput.consumeAll();
  }
  HttpConnection.this.reset();
  if (getCurrentConnection() == null) {
    if (_parser.isStart()) {
      if (_requestBuffer == null)       scheduleOnReadable();
 else {
        LOG.debug("{} pipelined",this);
        execute(new Runnable(){
          @Override public void run(){
            onReadable();
          }
        }
);
      }
    }
 else     if (!getEndPoint().isOutputShutdown() && _parser.isState(HttpParser.State.CLOSED)) {
      LOG.warn("Endpoint output not shutdown when seeking EOF");
      getEndPoint().close();
    }
  }
}

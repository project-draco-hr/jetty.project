{
  if (_parser.inContentState() && _generator.isPersistent() && !_channel.isExpecting100Continue())   _channel.getRequest().getHttpInput().consumeAll();
  if (_channel.getResponse().getStatus() == HttpStatus.SWITCHING_PROTOCOLS_101) {
    Connection connection=(Connection)_channel.getRequest().getAttribute(UPGRADE_CONNECTION_ATTRIBUTE);
    if (connection != null) {
      LOG.debug("Upgrade from {} to {}",this,connection);
      onClose();
      getEndPoint().setConnection(connection);
      connection.onOpen();
      reset();
      return;
    }
  }
  reset();
  if (getCurrentConnection() != this) {
    if (_parser.isStart()) {
      if (_requestBuffer == null) {
        fillInterested();
      }
 else       if (getConnector().isStarted()) {
        LOG.debug("{} pipelined",this);
        try {
          getExecutor().execute(this);
        }
 catch (        RejectedExecutionException e) {
          if (getConnector().isStarted())           LOG.warn(e);
 else           LOG.ignore(e);
          getEndPoint().close();
        }
      }
 else {
        getEndPoint().close();
      }
    }
  }
}

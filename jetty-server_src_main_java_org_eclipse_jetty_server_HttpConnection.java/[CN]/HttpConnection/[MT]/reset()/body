{
  if (_channel.isExpecting100Continue())   _parser.close();
  _channel.reset();
  if (_generator.isPersistent() && !_parser.isClosed())   _parser.reset();
 else   _parser.close();
  _generator.reset();
  releaseRequestBuffer();
  if (_chunk != null) {
    _bufferPool.release(_chunk);
    _chunk=null;
  }
}

{
  _parser.reset();
  _generator.reset();
  _channel.reset();
  if (_requestBuffer != null)   _bufferPool.release(_requestBuffer);
  _requestBuffer=null;
  if (_responseHeader != null)   _bufferPool.release(_responseHeader);
  _responseHeader=null;
  if (_responseBuffer != null)   _bufferPool.release(_responseBuffer);
  _responseBuffer=null;
  if (_chunk != null)   _bufferPool.release(_chunk);
  _chunk=null;
  _info=null;
}

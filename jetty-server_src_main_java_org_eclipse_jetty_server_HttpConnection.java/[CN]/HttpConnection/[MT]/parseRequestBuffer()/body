{
  if (LOG.isDebugEnabled())   LOG.debug("{} parse {} {}",this,BufferUtil.toDetailString(_requestBuffer));
  boolean had_input=BufferUtil.hasContent(_requestBuffer);
  boolean handle=_parser.parseNext(_requestBuffer == null ? BufferUtil.EMPTY_BUFFER : _requestBuffer);
  if (LOG.isDebugEnabled())   LOG.debug("{} parsed {} {}",this,handle,_parser);
  if (had_input && BufferUtil.isEmpty(_requestBuffer)) {
    if (_parser.inContentState())     _input.addPoisonPillContent(_recycle);
 else     releaseRequestBuffer();
  }
  return handle;
}

{
  if (info == null) {
    if (!lastContent && BufferUtil.isEmpty(content)) {
      callback.succeeded();
      return;
    }
  }
 else {
    if (_channel.isExpecting100Continue())     _generator.setPersistent(false);
  }
  if (_sendCallback.reset(info,content,lastContent,callback))   _sendCallback.iterate();
}

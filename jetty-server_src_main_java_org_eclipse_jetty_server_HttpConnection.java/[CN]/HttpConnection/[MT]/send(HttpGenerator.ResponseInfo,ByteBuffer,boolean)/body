{
  try {
    if (info == null)     new ContentCallback(content,lastContent,_writeBlocker).iterate();
 else {
      if (_channel.isExpecting100Continue())       _generator.setPersistent(false);
      new CommitCallback(info,content,lastContent,_writeBlocker).iterate();
    }
    _writeBlocker.block();
  }
 catch (  ClosedChannelException e) {
    throw new EofException(e);
  }
catch (  IOException e) {
    throw e;
  }
}

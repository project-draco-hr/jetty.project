{
  if (data.contains("disconnect")) {
    try {
      session.close();
    }
 catch (    IOException ignore) {
    }
    return;
  }
  ListIterator<ChatWebSocket> iter=members.listIterator();
  while (iter.hasNext()) {
    ChatWebSocket member=iter.next();
    if (!member.session.isOpen()) {
      iter.remove();
      continue;
    }
    member.remote.sendStringByFuture(data);
  }
}

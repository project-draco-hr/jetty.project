{
  server=new Server();
  connector=new SelectChannelConnector(server,new HttpConnectionFactory(){
    @Override public Connection newConnection(    Connector connector,    EndPoint endPoint){
      return new HttpConnection(new HttpChannelConfig(),connector,endPoint){
        @Override public void onFillable(){
          handles.incrementAndGet();
          super.onFillable();
        }
      }
;
    }
  }
);
  server.addConnector(connector);
  connector.setPort(0);
  server.setHandler(handler);
  server.start();
}

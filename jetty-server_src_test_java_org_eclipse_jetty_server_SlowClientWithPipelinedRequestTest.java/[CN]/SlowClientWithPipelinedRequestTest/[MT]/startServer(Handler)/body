{
  server=new Server();
  connector=new SelectChannelConnector(server){
    @Override protected AsyncConnection newConnection(    AsyncEndPoint endpoint){
      return new HttpConnection(getHttpConfig(),this,endpoint){
        @Override public synchronized void onFillable(){
          handles.incrementAndGet();
          super.onFillable();
        }
      }
;
    }
  }
;
  server.addConnector(connector);
  connector.setPort(0);
  server.setHandler(handler);
  server.start();
}

{
  PrintWriter out=new PrintWriter(writer);
  out.println("# --------------------------------------- ");
  out.println("# Module: " + module.getName());
  out.println("--module=" + module.getName());
  out.println();
  for (  String line : module.getIniTemplate()) {
    out.println(line);
  }
  out.println();
  out.flush();
}

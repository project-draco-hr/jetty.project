{
  if (frame instanceof WebSocketFrame) {
    if (masker == null) {
      ProtocolException ex=new ProtocolException("Must set a Masker");
      LOG.warn(ex);
      if (callback != null) {
        callback.writeFailed(ex);
      }
      return;
    }
    masker.setMask((WebSocketFrame)frame);
  }
  super.outgoingFrame(frame,callback);
}

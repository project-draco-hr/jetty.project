{
  try {
    if (!goAwaySent.get()) {
      RstStreamFrame frame=new RstStreamFrame(version,rstInfo.getStreamId(),rstInfo.getStreamStatus().getCode(version));
      control(null,frame);
    }
  }
 catch (  StreamException x) {
    logger.info("Could not send reset on stream " + rstInfo.getStreamId(),x);
  }
}

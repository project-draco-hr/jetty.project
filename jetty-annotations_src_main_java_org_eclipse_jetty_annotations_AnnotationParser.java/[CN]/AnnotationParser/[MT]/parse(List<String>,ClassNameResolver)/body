{
  for (  String s : classNames) {
    if ((resolver == null) || (!resolver.isExcluded(s) && (!isParsed(s) || resolver.shouldOverride(s)))) {
      s=s.replace('.','/') + ".class";
      URL resource=Loader.getResource(this.getClass(),s,false);
      if (resource != null)       scanClass(resource.openStream());
    }
  }
}

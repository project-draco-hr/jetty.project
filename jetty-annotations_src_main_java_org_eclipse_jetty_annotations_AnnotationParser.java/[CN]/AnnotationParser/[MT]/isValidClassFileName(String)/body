{
  if (!path.toLowerCase(Locale.ENGLISH).endsWith(".class"))   return false;
  int c0=0;
  int ldir=path.lastIndexOf('/',path.length() - 6);
  c0=(ldir > -1 ? ldir + 1 : c0);
  if (!Character.isJavaIdentifierStart(path.charAt(c0)))   return false;
  if (path.startsWith(".") || path.contains("/."))   return false;
  return true;
}

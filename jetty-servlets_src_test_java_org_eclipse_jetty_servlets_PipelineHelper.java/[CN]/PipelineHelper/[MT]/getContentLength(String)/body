{
  Pattern pat=Pattern.compile("Content-Length: ([0-9]*)",Pattern.CASE_INSENSITIVE);
  Matcher mat=pat.matcher(respHeader);
  if (mat.find()) {
    try {
      return Integer.parseInt(mat.group(1));
    }
 catch (    NumberFormatException e) {
      return -1;
    }
  }
 else {
    return -1;
  }
}

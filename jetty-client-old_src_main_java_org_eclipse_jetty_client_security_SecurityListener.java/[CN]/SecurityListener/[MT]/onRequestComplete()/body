{
  _requestComplete=true;
  if (_needIntercept) {
    if (_requestComplete && _responseComplete) {
      if (LOG.isDebugEnabled())       LOG.debug("onRequestComplete, Both complete: Resending from onResponseComplete " + _exchange);
      _responseComplete=false;
      _requestComplete=false;
      setDelegatingRequests(true);
      setDelegatingResponses(true);
      _destination.resend(_exchange);
    }
 else {
      if (LOG.isDebugEnabled())       LOG.debug("onRequestComplete, Response not yet complete onRequestComplete, calling super for " + _exchange);
      super.onRequestComplete();
    }
  }
 else {
    if (LOG.isDebugEnabled())     LOG.debug("onRequestComplete, delegating to super with Request complete=" + _requestComplete + ", response complete="+ _responseComplete+ " "+ _exchange);
    super.onRequestComplete();
  }
}

{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Selection other=(Selection)obj;
  if (explicit != other.explicit) {
    return false;
  }
  if (how == null) {
    if (other.how != null) {
      return false;
    }
  }
 else   if (!how.equals(other.how)) {
    return false;
  }
  return true;
}

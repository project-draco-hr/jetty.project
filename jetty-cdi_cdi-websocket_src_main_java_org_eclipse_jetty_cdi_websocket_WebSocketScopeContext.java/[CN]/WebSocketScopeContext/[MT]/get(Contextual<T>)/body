{
  if (LOG.isDebugEnabled()) {
    LOG.debug("get({})",contextual);
  }
  SimpleBeanStore store=state.get();
  if (store == null) {
    return null;
  }
  List<ScopedInstance<?>> beans=store.getBeans(contextual);
  if ((beans != null) && (!beans.isEmpty())) {
    return (T)beans.get(0).instance;
  }
  return null;
}

{
  if (executor == null)   setExecutor(new QueuedThreadPool());
  if (scheduler == null)   setScheduler(new ScheduledExecutorScheduler());
  if (bufferPool == null)   setByteBufferPool(new MappedByteBufferPool());
  if (connectionFactory == null)   setClientConnectionFactory(new HTTP2ClientConnectionFactory());
  if (sessions == null) {
    sessions=new ConcurrentLinkedQueue<>();
    addBean(sessions);
  }
  if (selector == null) {
    selector=newSelectorManager();
    addBean(selector);
  }
  selector.setConnectTimeout(getConnectTimeout());
  super.doStart();
}

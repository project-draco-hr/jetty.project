{
  _redirected=((status == HttpStatus.MOVED_PERMANENTLY_301 || status == HttpStatus.MOVED_TEMPORARILY_302) && _attempts < _destination.getHttpClient().maxRedirects());
  if (_redirected) {
    setDelegatingRequests(false);
    setDelegatingResponses(false);
  }
  super.onResponseStatus(version,status,reason);
}

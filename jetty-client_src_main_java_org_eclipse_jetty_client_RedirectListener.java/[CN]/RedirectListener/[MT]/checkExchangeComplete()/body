{
  if (_redirected && _requestComplete && _responseComplete) {
    if (_location != null) {
      if (_location.indexOf("://") > 0)       _exchange.setURL(_location);
 else       _exchange.setURI(_location);
      _destination.resend(_exchange);
      return false;
    }
 else {
      setDelegationResult(false);
    }
  }
  return true;
}

{
  int len=0;
  if (buffers.length == 1)   len=_channel.write(buffers[0]);
 else   if (buffers.length > 1 && _channel instanceof GatheringByteChannel)   len=(int)((GatheringByteChannel)_channel).write(buffers,0,buffers.length);
 else {
    for (    ByteBuffer b : buffers) {
      if (b.hasRemaining()) {
        int l=_channel.write(b);
        if (l > 0)         len+=l;
 else         break;
      }
    }
  }
  return len;
}

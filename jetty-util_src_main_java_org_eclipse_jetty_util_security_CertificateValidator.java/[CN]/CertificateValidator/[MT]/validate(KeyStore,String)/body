{
  String result=null;
  if (keyAlias != null) {
    try {
      validate(keyStore,keyStore.getCertificate(keyAlias));
    }
 catch (    KeyStoreException ex) {
      Log.debug(ex);
      throw new CertificateException("Unable to validate certificate for alias [" + keyAlias + "]: "+ ex.getMessage());
    }
    result=keyAlias;
  }
  return result;
}

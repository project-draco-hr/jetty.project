{
  _uri=null;
  _scheme=uri.getScheme();
  _host=uri.getHost();
  _port=uri.getPort();
  _path=uri.getRawPath();
  _decodedPath=uri.getPath();
  int p=_path.lastIndexOf(';');
  if (p >= 0)   _param=_path.substring(p + 1);
  _query=uri.getRawQuery();
  _fragment=uri.getFragment();
  _decodedPath=null;
}

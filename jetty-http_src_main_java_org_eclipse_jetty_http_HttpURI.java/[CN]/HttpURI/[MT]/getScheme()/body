{
  if (_scheme == _authority)   return null;
  int l=_authority - _scheme;
  if (l == 5 && _raw[_scheme] == 'h' && _raw[_scheme + 1] == 't' && _raw[_scheme + 2] == 't' && _raw[_scheme + 3] == 'p')   return HttpScheme.HTTP.toString();
  if (l == 6 && _raw[_scheme] == 'h' && _raw[_scheme + 1] == 't' && _raw[_scheme + 2] == 't' && _raw[_scheme + 3] == 'p' && _raw[_scheme + 4] == 's')   return HttpScheme.HTTPS.toString();
  return toUtf8String(_scheme,_authority - _scheme - 1);
}

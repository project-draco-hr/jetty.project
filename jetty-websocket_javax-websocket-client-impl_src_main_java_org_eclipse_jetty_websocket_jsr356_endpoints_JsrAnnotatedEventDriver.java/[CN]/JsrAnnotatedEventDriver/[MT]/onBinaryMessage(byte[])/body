{
  if (data == null) {
    return;
  }
  ByteBuffer buf=ByteBuffer.wrap(data);
  if (LOG.isDebugEnabled()) {
    LOG.debug("onBinaryMessage({})",BufferUtil.toDetailString(buf));
  }
  try {
    events.callBinary(jsrsession.getAsyncRemote(),websocket,buf,true);
  }
 catch (  DecodeException e) {
    onFatalError(e);
  }
}

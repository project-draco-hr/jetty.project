{
  Log.debug("ClosedOut {} {}",this,message);
  final boolean close;
  final boolean closed;
synchronized (this) {
    close=_closedIn || _closedOut;
    _closedOut=true;
    closed=_close == 0;
    if (closed)     _close=code;
  }
  try {
    if (closed)     _webSocket.onClose(code,message);
  }
  finally {
    try {
      if (close)       _endp.close();
 else {
        if (code <= 0)         code=WebSocketConnectionD10.CLOSE_NORMAL;
        byte[] bytes=("xx" + (message == null ? "" : message)).getBytes(StringUtil.__ISO_8859_1);
        bytes[0]=(byte)(code / 0x100);
        bytes[1]=(byte)(code % 0x100);
        _outbound.addFrame((byte)0x8,WebSocketConnectionD10.OP_CLOSE,bytes,0,bytes.length);
      }
      _outbound.flush();
    }
 catch (    IOException e) {
      Log.ignore(e);
    }
  }
}

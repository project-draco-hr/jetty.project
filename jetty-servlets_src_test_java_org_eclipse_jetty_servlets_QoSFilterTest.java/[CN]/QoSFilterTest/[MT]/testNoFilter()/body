{
  for (int i=0; i < NUM_CONNECTIONS; ++i) {
    new Thread(new Worker(i)).start();
  }
  _doneRequests.await(10,TimeUnit.SECONDS);
  if (TestServlet.__maxSleepers <= MAX_QOS)   LOG.warn("TEST WAS NOT PARALLEL ENOUGH!");
 else   Assert.assertThat(TestServlet.__maxSleepers,Matchers.lessThanOrEqualTo(NUM_CONNECTIONS));
}

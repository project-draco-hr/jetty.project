{
  Iterator<AppEntry> it=_apps.iterator();
  while (it.hasNext()) {
    AppEntry entry=it.next();
    if (entry.app.equals(app)) {
      if (!AppLifeCycle.UNDEPLOYED.equals(entry.lifecyleNode.getName()))       requestAppGoal(entry.app,AppLifeCycle.UNDEPLOYED);
      it.remove();
      Log.info("Deployable removed: " + entry.app);
    }
  }
}

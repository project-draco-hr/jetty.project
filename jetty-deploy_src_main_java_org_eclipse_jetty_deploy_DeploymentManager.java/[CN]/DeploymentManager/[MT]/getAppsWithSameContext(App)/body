{
  List<App> ret=new ArrayList<App>();
  if (app == null) {
    return ret;
  }
  String contextId=app.getContextId();
  if (contextId == null) {
    return ret;
  }
  for (  AppEntry entry : _apps) {
    if (entry.app.equals(app)) {
      continue;
    }
    if (contextId.equals(entry.app.getContextId())) {
      ret.add(entry.app);
    }
  }
  return ret;
}

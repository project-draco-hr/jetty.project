{
  if (_expect100Continue) {
    if (!isContentAvailable()) {
      if (isResponseCommitted())       throw new IllegalStateException("Committed before 100 Continues");
      send1xx(HttpStatus.CONTINUE_100);
    }
    _expect100Continue=false;
  }
  if (_in == null)   _in=new HttpInput(ServerConnection.this);
  return _in;
}

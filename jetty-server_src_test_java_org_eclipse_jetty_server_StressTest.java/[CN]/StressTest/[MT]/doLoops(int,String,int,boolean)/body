{
  try {
    for (int i=0; i < loops; i++) {
      _loops[thread]=i;
      doPaths(thread,name + "-" + i,persistent);
      Thread.sleep(1 + _random.nextInt(10) * _random.nextInt(10));
      Thread.sleep(10);
    }
    _loops[thread]=loops;
  }
 catch (  Exception e) {
    System.err.println(e);
    _loops[thread]=-_loops[thread];
    throw e;
  }
}

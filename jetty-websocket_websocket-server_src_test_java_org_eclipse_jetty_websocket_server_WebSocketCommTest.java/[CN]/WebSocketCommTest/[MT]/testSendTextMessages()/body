{
  MessageSender sender=new MessageSender();
  try {
    sender.awaitConnect();
    for (int i=0; i < 5; i++) {
      System.out.printf("Sending msg-%d%n",i);
      sender.sendMessage("msg-%d",i);
    }
    TimeUnit.MILLISECONDS.sleep(500);
  }
  finally {
    System.out.println("Closing client socket");
    sender.close();
  }
}

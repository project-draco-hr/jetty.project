{
  Node fromNode=null;
  Node toNode=null;
  try {
    fromNode=getNodeByName(from);
  }
 catch (  NodeNotFoundException e) {
    fromNode=new Node(from);
    addNode(fromNode);
  }
  try {
    toNode=getNodeByName(to);
  }
 catch (  NodeNotFoundException e) {
    toNode=new Node(to);
    addNode(toNode);
  }
  addEdge(fromNode,toNode);
}

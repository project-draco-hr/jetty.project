{
  NodePath shortest=null;
  int shortestlen=Integer.MAX_VALUE;
  for (  NodePath path : paths) {
    if (shortest == null) {
      shortest=path;
      continue;
    }
    int len=path.length();
    if (len < shortestlen) {
      shortest=path;
      shortestlen=len;
    }
  }
  return shortest;
}

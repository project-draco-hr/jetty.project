{
  if (__boundsChecking) {
    if (isImmutable())     throw new IllegalStateException(__IMMUTABLE);
    if (putIndex > capacity())     throw new IllegalArgumentException("putIndex>capacity: " + putIndex + ">"+ capacity());
    if (getIndex() > putIndex)     throw new IllegalArgumentException("getIndex>putIndex: " + getIndex() + ">"+ putIndex);
  }
  _put=putIndex;
  _hash=0;
}

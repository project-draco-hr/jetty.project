{
  int len=part.length();
  for (int i=0; i < len; i++) {
    char c=part.charAt(i);
    for (    char g : GLOB_CHARS) {
      if (c == g) {
        return true;
      }
    }
    if (syntaxed) {
      for (      char g : SYNTAXED_GLOB_CHARS) {
        if (c == g) {
          return true;
        }
      }
    }
  }
  return false;
}

{
  Handler[] handlers=getHandlers();
  if (handlers != null) {
    int last=handlers.length - 1;
    for (int h=0; h <= last; h++) {
      if (handlers[h] == null)       continue;
      out.append(indent);
      out.append(" +-");
      if (handlers[h] instanceof AbstractHandler)       ((AbstractHandler)handlers[h]).dump(out,indent + ((h == last) ? "   " : " | "));
 else {
        out.append(String.valueOf(handlers[h]));
        out.append("\n");
      }
    }
  }
}

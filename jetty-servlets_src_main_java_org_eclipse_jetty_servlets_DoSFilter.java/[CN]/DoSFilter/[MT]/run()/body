{
  try {
    while (_running) {
      long now=_requestTimeoutQ.setNow();
      _requestTimeoutQ.tick();
      _trackerTimeoutQ.setNow(now);
      _trackerTimeoutQ.tick();
      try {
        Thread.sleep(100);
      }
 catch (      InterruptedException e) {
        LOG.ignore(e);
      }
    }
  }
  finally {
    LOG.debug("DoSFilter timer exited");
  }
}

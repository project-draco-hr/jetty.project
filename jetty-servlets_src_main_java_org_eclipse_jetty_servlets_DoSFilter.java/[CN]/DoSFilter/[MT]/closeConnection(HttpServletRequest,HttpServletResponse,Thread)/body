{
  if (!response.isCommitted()) {
    response.setHeader("Connection","close");
  }
  try {
    try {
      response.getWriter().close();
    }
 catch (    IllegalStateException e) {
      response.getOutputStream().close();
    }
  }
 catch (  IOException e) {
    Log.warn(e);
  }
  thread.interrupt();
}

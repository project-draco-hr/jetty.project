{
  HttpParser.LOG.info("badMessage: 3 bad messages expected ...");
  String response;
  response=connector.getResponses("GET http://localhost:EXPECTED_NUMBER_FORMAT_EXCEPTION/ HTTP/1.1\n" + "Host: localhost\n" + "Connection: close\015\012"+ "\015\012");
  checkContains(response,0,"HTTP/1.1 400");
  response=connector.getResponses("GET /bad/encoding%1 HTTP/1.1\n" + "Host: localhost\n" + "Connection: close\n"+ "\015\012");
  checkContains(response,0,"HTTP/1.1 400");
  response=connector.getResponses("GET % HTTP/1.1\n" + "Host: localhost\n" + "Connection: close\n"+ "\015\012");
  checkContains(response,0,"HTTP/1.1 200");
  response=connector.getResponses("GET /bad/utf8%c1 HTTP/1.1\n" + "Host: localhost\n" + "Connection: close\n"+ "\015\012");
  checkContains(response,0,"HTTP/1.1 200");
}

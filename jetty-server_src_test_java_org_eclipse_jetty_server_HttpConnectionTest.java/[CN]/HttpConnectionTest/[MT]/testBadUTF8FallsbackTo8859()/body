{
  Log.getLogger(HttpParser.class).info("badMessage: bad encoding expected ...");
  String response;
  response=connector.getResponses("GET /foo/bar%c0%00 HTTP/1.1\n" + "Host: localhost\n" + "Connection: close\n"+ "\015\012");
  checkContains(response,0,"HTTP/1.1 200");
  response=connector.getResponses("GET /bad/utf8%c1 HTTP/1.1\n" + "Host: localhost\n" + "Connection: close\n"+ "\015\012");
  checkContains(response,0,"HTTP/1.1 200");
}

{
  this.result=result;
  if (result.isSucceeded()) {
    if (LOG.isDebugEnabled())     LOG.debug("Queuing end of content {}{}",EOF,"");
    queue.offer(EOF);
  }
 else {
    if (LOG.isDebugEnabled())     LOG.debug("Queuing failure {} {}",FAILURE,failure);
    queue.offer(FAILURE);
    this.failure=result.getFailure();
    responseLatch.countDown();
  }
  resultLatch.countDown();
  signal();
}

{
  int length=65536;
  ByteBuffer bb=ByteBuffer.allocate(length);
  for (int i=0; i < length; ++i) {
    bb.put("*".getBytes());
  }
  bb.flip();
  WebSocketFrame binaryFrame=FrameBuilder.binary(BufferUtil.toArray(bb)).asFrame();
  Generator generator=new Generator(policy);
  ByteBuffer actual=generator.generate(binaryFrame);
  ByteBuffer expected=ByteBuffer.allocate(length + 11);
  expected.put(new byte[]{(byte)0x82});
  byte b=0x00;
  b|=0x7F;
  expected.put(b);
  expected.put(new byte[]{0x00,0x00,0x00,0x00,0x00,0x01,0x00,0x00});
  for (int i=0; i < length; ++i) {
    expected.put("*".getBytes());
  }
  actual.flip();
  expected.flip();
  ByteBufferAssert.assertEquals("buffers do not match",expected,actual);
}

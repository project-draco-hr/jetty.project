{
  try {
    AsyncEndPoint endPoint=getEndPoint();
    try {
      logger.debug("Shutting down output {}",endPoint);
      endPoint.shutdownOutput();
      if (!onlyOutput) {
        logger.debug("Closing {}",endPoint);
        endPoint.close();
      }
    }
 catch (    IOException x) {
      endPoint.close();
    }
  }
 catch (  IOException x) {
    logger.trace("",x);
  }
}

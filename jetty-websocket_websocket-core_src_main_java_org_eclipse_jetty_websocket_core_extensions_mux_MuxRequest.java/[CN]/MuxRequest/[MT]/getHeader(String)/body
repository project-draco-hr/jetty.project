{
  List<String> values=headers.get(name);
  if ((values == null) || (values.isEmpty())) {
    return null;
  }
  if (values.size() == 1) {
    return values.get(0);
  }
  StringBuilder ret=new StringBuilder();
  boolean delim=false;
  for (  String value : values) {
    if (delim) {
      ret.append(", ");
    }
    QuotedStringTokenizer.quoteIfNeeded(ret,value,HEADER_VALUE_DELIM);
    delim=true;
  }
  return ret.toString();
}

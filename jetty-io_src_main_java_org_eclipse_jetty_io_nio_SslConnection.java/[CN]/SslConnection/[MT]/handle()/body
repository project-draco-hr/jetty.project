{
  try {
    allocateBuffers();
    boolean progress=true;
    while (progress) {
      progress=false;
      if (_engine.getHandshakeStatus() != HandshakeStatus.NOT_HANDSHAKING)       progress|=process(null,null);
 else {
        AsyncConnection next=(AsyncConnection)_connection.handle();
        if (next != _connection && next == null) {
          _connection=next;
          progress=true;
        }
      }
      LOG.debug("{} handle {} progress=",_session,this,progress);
    }
  }
  finally {
    releaseBuffers();
  }
  return this;
}

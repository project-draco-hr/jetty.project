{
synchronized (this) {
    if (--_allocations == 0) {
      if (_buffers != null && _inbound.remaining() == 0 && _outbound.remaining() == 0 && _unwrapBuf.remaining() == 0) {
        _inbound=null;
        _outbound=null;
        _unwrapBuf=null;
        _buffers._in.clear().limit(0);
        _buffers._out.clear().limit(0);
        _buffers._unwrap.clear().limit(0);
        __buffers.set(_buffers);
        _buffers=null;
      }
    }
  }
}

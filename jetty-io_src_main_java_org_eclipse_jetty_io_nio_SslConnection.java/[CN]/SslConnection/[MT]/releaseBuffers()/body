{
synchronized (this) {
    if (--_allocations == 0) {
      if (_buffers != null && _inbound.length() == 0 && _outbound.length() == 0) {
        _inbound=null;
        _outbound=null;
        __buffers.set(_buffers);
        _buffers=null;
      }
      if (_unwrapBuf != null && _unwrapBuf.length() == 0)       _unwrapBuf=null;
    }
  }
}

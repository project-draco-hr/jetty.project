{
synchronized (this) {
    if (--_allocations == 0) {
      if (_inbound != null && _inbound.length() == 0) {
        __inBuffer.set(_inbound);
        _inbound=null;
      }
      if (_outbound != null && _outbound.length() == 0) {
        __outBuffer.set(_outbound);
        _outbound=null;
      }
      if (_unwrapBuf != null && _unwrapBuf.length() == 0)       _unwrapBuf=null;
    }
  }
}

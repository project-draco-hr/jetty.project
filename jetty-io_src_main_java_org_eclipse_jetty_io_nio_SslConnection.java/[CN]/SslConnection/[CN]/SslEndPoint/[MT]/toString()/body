{
  int i;
  int o;
  int u;
synchronized (SslConnection.this) {
    i=_inbound == null ? -1 : _inbound.length();
    o=_outbound == null ? -1 : _outbound.length();
    u=_unwrapBuf == null ? -1 : _unwrapBuf.length();
  }
  return String.format("SSL:%s %s i/u/o=%d/%d/%d ishut=%b oshut=%b",_endp,_engine.getHandshakeStatus(),i,u,o,_ishut,_oshut);
}

{
  Fields responseHeaders=new Fields();
  for (  HttpField header : response.getHeaders())   responseHeaders.add(header.getName(),header.getValue());
  short version=clientStream.getSession().getVersion();
  if (response.getStatus() > 0)   responseHeaders.add(HTTPSPDYHeader.STATUS.name(version),String.valueOf(response.getStatus()));
  responseHeaders.add(HTTPSPDYHeader.VERSION.name(version),HttpVersion.HTTP_1_1.asString());
  addResponseProxyHeaders(clientStream,responseHeaders);
  return responseHeaders;
}

{
switch (version()) {
case SPDY.V2:
{
      Matcher matcher=Pattern.compile("https?://[^:]+:\\d+(/.*)").matcher(pushedURI);
      Assert.assertTrue(matcher.matches());
      pushedResources.add(matcher.group(1));
      break;
    }
case SPDY.V3:
{
    pushedResources.add(pushedURI);
    break;
  }
default :
{
  throw new IllegalStateException();
}
}
}

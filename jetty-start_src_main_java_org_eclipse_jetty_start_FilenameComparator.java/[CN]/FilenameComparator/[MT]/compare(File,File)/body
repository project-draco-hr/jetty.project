{
  if (o1.isFile()) {
    if (o2.isFile()) {
      CollationKey key1=toKey(o1);
      CollationKey key2=toKey(o2);
      return key1.compareTo(key2);
    }
 else {
      return -1;
    }
  }
 else {
    if (o2.isDirectory()) {
      CollationKey key1=toKey(o1);
      CollationKey key2=toKey(o2);
      return key1.compareTo(key2);
    }
 else {
      return 1;
    }
  }
}

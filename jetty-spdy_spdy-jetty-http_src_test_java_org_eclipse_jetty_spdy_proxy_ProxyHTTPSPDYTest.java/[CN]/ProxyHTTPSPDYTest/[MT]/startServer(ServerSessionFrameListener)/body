{
  server=new Server();
  SPDYServerConnector serverConnector=new SPDYServerConnector(server,listener);
  serverConnector.setDefaultConnectionFactory(new ServerSPDYConnectionFactory(version,serverConnector.getByteBufferPool(),serverConnector.getExecutor(),serverConnector.getScheduler(),listener));
  serverConnector.setPort(0);
  server.addConnector(serverConnector);
  server.start();
  return new InetSocketAddress("localhost",serverConnector.getLocalPort());
}
